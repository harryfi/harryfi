    <script>
        $(document).ready(function () {
            // Play Video
            $('#modalVideo').on('shown.bs.modal', function (e) {
                $("iframe#ytplayer").attr("src", $("iframe#ytplayer").attr("src").replace("autoplay=0", "autoplay=1"));
            });

            // Stop Video
            $('#modalVideo').on('hidden.bs.modal', function (e) {
                $('iframe#ytplayer')[0].contentWindow.postMessage('{"event":"command","func":"' + 'stopVideo' + '","args":""}', '*');
            });

            $('#close-video').click(function () {
                $('#modalVideo').modal('hide');
            });

            //var waypoint = new Waypoint({
            //    element: document.getElementById('counter'),
            //    handler: function (direction) {
            //        setData(300, 881, 760, 79);
            //    },
            //    offset: "100%"
            //})

        });

        function setData(member, order, sent, revenue) {
            document.getElementById("member").textContent = formatNumber(member);
            document.getElementById("order").textContent = formatNumber(order);
            document.getElementById("sent").textContent = formatNumber(sent);
            document.getElementById("revenue").textContent = formatNumber(revenue);
        }
        function formatNumber(num) {
            if (num >= 1000000000) {
                num /= 1000000000;
                num = Math.floor(num);
                return String(num) + "B +";
            }
            else if (num >= 1000000) {
                num /= 1000000;
                num = Math.floor(num);
                return String(num) + "M +";
            }
            else if (num >= 1000) {
                num /= 1000;
                num = Math.floor(num);
                return String(num) + "K +";
            }
            else if (num >= 100) {
                num /= 100;
                num = Math.floor(num);
                return String(num) + "00+";
            }

        }
    </script>
}
