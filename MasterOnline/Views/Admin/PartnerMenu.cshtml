        function searchTablePartner(){
            var $link = '@Html.Raw(Url.Action("RefreshTablePartner", "Admin", new { search = "replaceSearch" }))';
            $link = $link.replace("replaceSearch", $('#search_partner').val());

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#search_partner_click')[0].disabled = true;
                    $('#loading_partner_tab').show();
                },
                success: function (response) {
                    $('#search_partner_click')[0].disabled = false;
                    $('#table-partner-partial').html(response);
                    $('#loading_partner_tab').hide();
                },
                error: function (xhr, status, error) {
                    $('#search_partner_click')[0].disabled = false;
                    console.log(error);
                }
            });
        }

