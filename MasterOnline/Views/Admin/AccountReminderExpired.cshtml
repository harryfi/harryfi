        function refreshTableReminderWillExpired($pageAwal) {
            var $setPage = "";
            if ($pageAwal != undefined) {
                $setPage = $pageAwal;
            } else {
                $setPage = $('#txt_last_page_reminder_expired').val();
            }

            var $link = '@Html.Raw(Url.Action("RefreshTableReminderWillExpired", "Admin", new { take = "replaceTake", page = "replaceLastPage", drTgl = "replacedrtgl", search = "replaceSearch", filter = "replaceFilter" }))'; //, drTgl = "replacedrtgl", sdTgl = "replacesdtgl"
            $link = $link.replace("replaceTake", encodeURIComponent($('#TakeRecordReminderExpired').val()));
            $link = $link.replace("replaceSearch", encodeURIComponent($('#search_willexpired').val()));
            $link = $link.replace("replaceLastPage", encodeURIComponent($setPage));
