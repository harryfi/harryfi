                $Sdtgl = $('#SdTanggal').val();
            }
            $link = $link.replace("replacedrtgl", encodeURIComponent($Drtgl));
            $link = $link.replace("replacesdtgl", encodeURIComponent($Sdtgl));
            var filtervalue = get_range_tgl_subs();
            $link = $link.replace("replaceFilter", encodeURIComponent(filtervalue));
            //console.log($link);
            //debugger;
            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#search_willexpired')[0].disabled = true;
                    $('#loading_willexpired').show();
                },
                success: function (response) {
                    $('#search_willexpired')[0].disabled = false;
                    $('#table-account-will-expired').html(response);
                    $('#loading_willexpired').hide();
                },
                error: function (xhr, status, error) {
                    $('#search_willexpired')[0].disabled = false;
                    $('#loading_willexpired').hide();
                    console.log(error);
                }
            });
        }

