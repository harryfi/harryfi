                            }
                            if ($('#Payment_DrTGL').val() != '') {
                                $('#drTGL').val($('#Payment_DrTGL').val());
                            }
                            if ($('#Payment_SdTGL').val() != '') {
                                $('#sdTGL').val($('#Payment_SdTGL').val());
                            }
                            $('#KODESUBS').change(function () {
                                $("#Payment_TipeSubs").val($('#KODESUBS').val())
                            });
                            $("#KODESUBS").change(
                                function () {
