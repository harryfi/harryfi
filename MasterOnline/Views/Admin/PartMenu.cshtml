@using System.Globalization
@model MasterOnline.ViewModels.PartnerViewModel
@{
    ViewBag.Title = "Partner";
    Layout = "~/Views/Shared/_LayoutAdminCS.cshtml";

    var editMode = 0;

    if (ViewData["Editing"] != null)
    {
        editMode = (int)ViewData["Editing"];
    }
}

@section styles
{
    <link href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.dataTables.min.css" rel="stylesheet" />

    <style>
        .user-link {
            text-decoration: underline;
        }

        #akun-section {
            margin-top: 60px;
            background: white;
            padding: 20px;
        }

        /*add by nurul 18/2/2019*/
        .logo_perusahaan_section {
            padding: 0 10px 10px 10px;
        }

            .logo_perusahaan_section > .box_upload {
                width: 200px;
                height: 200px;
                border: dashed 1.5px #bbb;
                border-radius: 10px;
                background-color: #f7f7f7;
                background-image: url('');
                text-align: center;
                display: table;
                cursor: pointer;
            }

        .box_upload > .box_upload_placeholder {
            color: #ccc;
            font-size: 20px;
            display: table-cell;
            vertical-align: middle;
        }

        .btn_hapus_logo {
            position: absolute;
            bottom: 25px;
            left: 20px;
        }
        /*add by nurul 18/2/2019*/
    </style>
}

<div class="row" id="akun-section">
    <div class="col-lg-12 col-md-12">
        @*<div class="row user_table_section">*@
        <div class="row partner_table_section">
            <div class="col-lg-4 col-sm-6">
                <div class="input-group">
                    <input id="search_user" type="text" class="form-control" placeholder="Pencarian">
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-primary">
                            <i class="fa fa-search"></i>
                        </button>
                    </span>
                </div>
            </div>
            <div class="col-lg-8 col-sm-6">
                <div class="pull-right">
                    <button type="button" class=" btn btn-default" data-style="expand-right">
                        <span>Refresh</span>
                    </button>
                </div>
            </div>
        </div>
        @*<div class="row user_table_section">*@
        <div class="row partner_table_section">
            <div class="col-sm-12">
                @if (ViewData["SuccessMessage"] != null)
                {
                    <div class="alert alert-success">
                        @ViewData["SuccessMessage"].ToString()
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                }
                @if (ViewData.ModelState.Any(x => x.Value.Errors.Any()))
                {
                    foreach (var modelError in ViewData.ModelState.SelectMany(x => x.Value.Errors))
                    {
                        <div class="alert alert-danger">
                            @modelError.ErrorMessage
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                    }
                }
                <table id="datatable" class="table table-striped table-bordered dataTable" role="grid" aria-describedby="datatable_info">
                    <thead>
                        <tr role="row">
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="2" style="width: 50px; ">Komisi Sales</th>

                            <th style="border-bottom:unset"></th>
                            <th style="border-bottom:unset"></th>

                        </tr>
                        <tr>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 90px;border-top:unset">Nama Lengkap</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 50px;border-top:unset">Email</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 90px;border-top:unset">Tgl. Daftar</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 85px;border-top:unset">No. HP</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 85px;border-top:unset">Tipe Partner</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 85px;border-top:unset">Kode Referral</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 70px;border-top:unset">Status Persetujuan</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 70px;border-top:unset">Status Approval</th>
                            @*<th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 60px;border-top:unset">Ubah Approval</th>*@
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" width="50%" style="border-right:none; max-width:90px">Silver</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" width="50%" style="border-right:none; max-width:90px">Gold</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 70px;border-top:unset">Komisi Support</th>
                            <th class="sorting text-center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" style="width: 60px;border-top:unset">Edit Detail</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var partner in Model?.ListPartner)
                            {
                                var Koderef = "-";
                                if (partner.KodeRefPilihan != null || partner.KodeRefPilihan != "")
                                {
                                    Koderef = partner.KodeRefPilihan;
                                }
                                <tr>
                                    <td class="text-center">@partner.Username</td>
                                    <td class="text-center">@partner.Email</td>
                                    <td class="text-center">@(partner.TGL_DAFTAR?.ToString("dd/MM/yyyy"))</td>
                                    <td class="text-center">@partner.NoHp</td>
                                    <td class="text-center">@partner.NamaTipe</td>
                                    @*add by nurul 14/2/2019*@
                                    <td class="text-center">@Koderef</td>
                                    @*end add by nurul 14/2/2019*@
                                    <td class="text-center">
                                        @if (partner.StatusSetuju)
                                        {
                                            @("Setuju")
                                        }
                                        else
                                        {
                                            @("Tidak Setuju")
                                        }
                                    </td>
                                    <td class="text-center">
                                        @if (partner.Status)
                                        {
                                            @("Aktif")
                                        }
                                        else
                                        {
                                            @("Tidak Aktif")
                                        }
                                    </td>
                                    @*<td class="edit-hapus-col">
                                            @{
                                                var status = 0;

                                                if (partner.Status)
                                                {
                                                    status = 1;
                                                }
                                            }

                                            <button class="btn btn-primary" data-toggle="modal" data-target="#konfUbahStatus" onclick="passId(@partner.PartnerId, @status, '@partner.Username')">
                                                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                                            </button>
                                        </td>*@
                                    @*add by nurul 14/2/2019*@
                                    <td class="text-center">
                                        @partner.komisi_subscribe
                                    </td>
                                    <td class="text-center">
                                        @partner.komisi_subscribe_gold
                                    </td>
                                    <td class="text-center">
                                        @partner.komisi_support
                                    </td>
                                    <td class="edit-hapus-col">
                                        <button class="btn btn-primary" onclick="editKomisi(@partner.PartnerId)">
                                            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                                        </button>
                                    </td>
                                    @*end add by nurul 14/2/2019*@
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
        @*add by nurul 15/2/2019*@
        <div class="row partner_editor_section" style="display: none;">
            <div class="col-lg-12">
                <div class="page-editor">
                    <h2 class="editor-title">Komisi Partner</h2>
                    <span class="title-accent"></span>
                    <button type="button" class="pull-right page-close">
                        <span class="close-btn thick"></span>
                    </button>
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="x_panel">
                                    <div class="x_title">
                                        <h2 style="font-size: 16px">Detail Komisi Partner</h2>
                                        <ul class="nav navbar-right panel_toolbox">
                                            <li>
                                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                            </li>
                                        </ul>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="x_content">
                                        @if (ViewData.ModelState.Any(x => x.Value.Errors.Any()))
                                        {
                                            foreach (var modelError in ViewData.ModelState.SelectMany(x => x.Value.Errors))
                                            {
                                                <div class="alert alert-danger">
                                                    <span class="message-error">@modelError.ErrorMessage</span>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            }
                                        }
                                        <br>
                                        @using (Html.BeginForm("SaveKomisiCS", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {
                                            @Html.AntiForgeryToken()
                                            @Html.HiddenFor(m => m.partner.PartnerId)
                                            var setStatus = "";
                                            var status1 = Model.partner.Status;
                                            if (status1)
                                            {
                                                setStatus = "Aktif";
                                            }
                                            else
                                            {
                                                setStatus = "Tidak Aktif";
                                            }
                                            var setSetuju = "";
                                            var status2 = Model.partner.StatusSetuju;
                                            if (status2)
                                            {
                                                setSetuju = "Setuju";
                                            }
                                            else
                                            {
                                                setSetuju = "Tidak Setuju";
                                            }
                                            <div id="hidden_partner">
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.PartnerId, "PartnerID", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.PartnerId, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.Email, "Email", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.Email, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.Username, "Nama Lengkap", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.Username, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.TGL_DAFTAR, "Tanggal Daftar", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.TGL_DAFTAR, new { @class = "form-control", @ReadOnly = true, Value = Model?.partner?.TGL_DAFTAR?.ToString("dd/MM/yyyy") })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.PhotoKtpUrl, "Photo Ktp", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12 logo_perusahaan_section">
                                                        @*@Html.TextBoxFor(m => m.partner.PhotoKtpUrl, new { @class = "form-control", @ReadOnly = true })*@
                                                        @*<div class="box_upload" id="box_upload_logo">
                                                                <span class="box_upload_placeholder"></span>
                                                                <input type='file' id='PhotoKtpUrl' name="PhotoKtpUrl" accept=".jpg" />
                                                            </div>*@
                                                        @if (!String.IsNullOrWhiteSpace(Model.partner.PhotoKtpUrl))
                                                        {
                                                            <img src="@Url.Content(Model.partner.PhotoKtpUrl)" width="65%" />
                                                        }
                                                        else
                                                        {
                                                            <img src="~/Content/Images/no-img-market.jpg" />
                                                        }
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.NoHp, "No Hp", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.NoHp, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.NamaTipe, "Nama Tipe", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.NamaTipe, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.StatusSetuju, "Status Setuju", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => setSetuju, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.KodeRefPilihan, "Kode Referral Pilihan", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @Html.TextBoxFor(m => m.partner.KodeRefPilihan, new { @class = "form-control", @ReadOnly = true })
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    @Html.LabelFor(m => m.partner.Status, "Status", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                                        @*@Html.TextBoxFor(m => setStatus, new { @class = "form-control", @ReadOnly = true })*@
                                                        @Html.DropDownListFor(m => m.partner.Status, new List<SelectListItem>()
                                                    {
                                                        new SelectListItem()
                                                        {
                                                            Text = @"Tidak Aktif",
                                                            Value = "false",
                                                        },
                                                        new SelectListItem()
                                                        {
                                                            Text = @"Aktif",
                                                            Value = "true"
                                                        }
                                                    }, new { @class = "form-control" })
                                                    </div>
                                                </div>


                                                <div id="photo_base">
                                                    <div class="form-group">
                                                        @Html.LabelFor(m => m.partner.PhotoKtpBase64, "Photo Base", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                                            @Html.TextBoxFor(m => m.partner.PhotoKtpBase64, new { @class = "form-control", @ReadOnly = true })
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        @Html.LabelFor(m => m.partner.TipePartner, "Tipe Partner", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                                            @Html.TextBoxFor(m => m.partner.TipePartner, new { @class = "form-control", @ReadOnly = true })
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="form-group">
                                                @Html.LabelFor(m => m.partner.komisi_subscribe, "Komisi Subscribe Silver", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    @Html.TextBoxFor(m => m.partner.komisi_subscribe, new { @class = "form-control limited_number_textbox", min = 0, type = "number" })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                @Html.LabelFor(m => m.partner.komisi_subscribe_gold, "Komisi Subscribe Gold", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    @Html.TextBoxFor(m => m.partner.komisi_subscribe_gold, new { @class = "form-control limited_number_textbox", min = 0, type = "number" })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                @Html.LabelFor(m => m.partner.komisi_support, "Komisi Support", new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    @Html.TextBoxFor(m => m.partner.komisi_support, new { @class = "form-control limited_number_textbox", min = 0, type = "number" })
                                                </div>
                                            </div>
                                            <hr />
                                            @*<button type="submit" id="simpan_btn" class="btn btn-primary pull-right">Simpan</button>*@
                                            <button type="button" id="simpan_btn" class="btn btn-primary pull-right" onclick="simpanKomisi(this , '@Model.partner.PartnerId');">Simpan</button>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*end add by nurul 15/2/2019*@
    </div>
</div>

<div class="modal fade" id="konfUbahStatus" tabindex="-1" role="dialog" aria-labelledby="konfUbahStatusLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="konfUbahStatusLabel">Konfirmasi</h4>
            </div>
            <div class="modal-body">
                <div class="row text-center">
                    <h4>Apakah Anda yakin ingin <span id="will-change-to"></span><br /> status partner <b id="partner-name"></b>?</h4>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Tidak</button>
                <button type="button" class="btn btn-success" onclick="ubahStatusPartner()">Ya</button>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="//cdn.datatables.net/plug-ins/1.10.11/sorting/date-eu.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.3/js/dataTables.responsive.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        var $partnerId = 0;
        var $status = 0;

        $(document).on('ready',
            function () {
                //var table = $('#datatable').DataTable();
                var table = $('#datatable').DataTable({
                    "order": [[2, "desc"], [0, "asc"]], //or asc
                    "columnDefs": [{ "targets": 2, "type": "date-eu" }],
                    destroy: true,
                });
                $('#search_user').keyup(function () {
                    table.search($(this).val()).draw();
                });

                //add by nurul 15/2/2019
                $('button>.close-btn').on('click', function () {
                    $('.partner_table_section').toggle('display');
                    $('.partner_editor_section').toggle('display');
                    //$('#hidden_partner').hide()
                    $('#photo_base').hide()
                });

                if ($('.message-error').text() !== '' || @editMode === 1) {
                    $('.partner_table_section').toggle('display');
                    $('.partner_editor_section').toggle('display');
                    //$('#hidden_partner').hide()
                    $('#photo_base').hide()
                }

                $('.close-btn').click(function() {
                    location.href = '@Url.Action("PartMenu")';
                });

                $('#partner_komisi_subscribe').change(function () {
                    $(this).val(convertToRupiah($(this).val()));
                });

                $('#partner_komisi_subscribe_gold').change(function () {
                    $(this).val(convertToRupiah($(this).val()));
                });

                $('#partner_komisi_support').change(function () {
                    $(this).val(convertToRupiah($(this).val()));
                });

                //$('#simpan_btn').click(function () {
                //    var subs = $('#partner_komisi_subscribe').val();
                //    var supp = $('#partner_komisi_support').val();
                //    var gold = $('#partner_komisi_subscribe_gold').val();
                //    if (subs == "") {
                //        subs = "0";
                //    }
                //    if (supp == "") {
                //        supp = "0";
                //    }
                //    if (gold == "") {
                //        gold = "0";
                //    }
                //    $('#partner_komisi_support').val(convertToAngka(supp));
                //    $('#partner_komisi_subscribe').val(convertToAngka(subs));
                //    $('#partner_komisi_subscribe_gold').val(convertToAngka(gold));
                //});

                //var $linkLogo = '/Content/Logo_Perusahaan/LogoUsaha-' + $('#DataUsaha_USERNAME').val() + '-' + $('#DataUsaha_NAMA_PT').val() + '.jpg';
                var $linkLogo = '@Model.partner.PhotoKtpUrl';

                $.ajax({
                    url: $linkLogo,
                    type: 'GET',
                    success: function () {
                        $('#box_upload_logo').css({
                            'background-image': 'url("' + $linkLogo + '")',
                            'background-size': 'cover'
                        });
                        $('#box_upload_logo span').hide();
                    },
                    error: function () {
                    }
                });

                $('#partner_status').change(function () {
                    ubahStatusPartner();
                });
                //end add by nurul 15/2/2019
            });

        function passId(partnerId, status, partnerName) {
            this.$partnerId = partnerId;
            this.$status = status;

            $('#partner-name').text(partnerName);

            if (this.$status === 1) {
                $('#will-change-to').text("menonaktifkan");
            } else {
                $('#will-change-to').text("mengaktifkan");
            }
        }

        function ubahStatusPartner() {
            var link = '@Url.Action("ChangeStatusPart", "Admin", new { partnerId = "replaceId" })';
            link = link.replace("replaceId", this.$partnerId);

            location.href = link;
        }

        //add by nurul 15/2/2019

        function editKomisi($Id) {
            var link = '@Url.Action("EditKomisiCS", "Admin", new { partnerId = "replaceId" })';
            link = link.replace("replaceId", $Id);

            location.href = link;

        }

        function convertToRupiah(angka) {
            var rupiah = '';
            var angkarev = angka.toString().split('').reverse().join('');
            for (var i = 0; i < angkarev.length; i++) if (i % 3 === 0) rupiah += angkarev.substr(i, 3) + '.';
            return rupiah.split('', rupiah.length - 1).reverse().join('');
        }

        function convertToAngka(rupiah) {
            return parseInt(rupiah.replace(/,.*|[^0-9]/g, ''), 10);
        }

        function simpanKomisi(btnClicked, $ID) {
                var subs = $('#partner_komisi_subscribe').val();
                var supp = $('#partner_komisi_support').val();
                var gold = $('#partner_komisi_subscribe_gold').val();
                if (subs == "") {
                        subs = "0";
                }
                if (supp == "") {
                        supp = "0";
                }
                if (gold == "") {
                        gold = "0";
                }
                $('#partner_komisi_support').val(convertToAngka(supp));
                $('#partner_komisi_subscribe').val(convertToAngka(subs));
                $('#partner_komisi_subscribe_gold').val(convertToAngka(gold));


            var $form = $(btnClicked).parents('form');

            var link = '@Url.Action("ChangeStatusPart", "Admin", new { partnerid = "replaceId" })';
            link = link.replace("replaceId", $ID);

            $.ajax({
                type: "GET",
                url: link,
                success: function (res) {
                $.ajax({
                type: "POST",
                url: $form.attr('action'),
                    data: $form.serialize(),
                        success: function (response) {
                            alert('Data berhasil tersimpan');

