    }
    var listPesananMingguan = @Html.Raw(Json.Encode(listPesananMingguan));
    var listPesananBulanan = @Html.Raw(Json.Encode(listPesananBulanan));
    var listPesananTahunan = @Html.Raw(Json.Encode(listPesananTahunan));

    var listFakturMingguan = @Html.Raw(Json.Encode(listFakturMingguan));
    var listFakturBulanan = @Html.Raw(Json.Encode(listFakturBulanan));
    var listFakturTahunan = @Html.Raw(Json.Encode(listFakturTahunan));

    var listReturMingguan = @Html.Raw(Json.Encode(listReturMingguan));
    var listReturBulanan = @Html.Raw(Json.Encode(listReturBulanan));
    var listReturTahunan = @Html.Raw(Json.Encode(listReturTahunan));
</script>
