@using System.Globalization
@using MasterOnline.Models
@model MasterOnline.ViewModels.DashboardViewModel
<script type="text/javascript">
    @{
        var listPesananMingguan = Model.ListdashboardPesananMingguan.ToList();
        var listPesananBulanan = Model.ListdashboardPesananBulanan.ToList();
        var listPesananTahunan = Model.ListdashboardPesananTahunan.ToList();

        var listFakturMingguan = Model.ListdashboardFakturMingguan.ToList();
