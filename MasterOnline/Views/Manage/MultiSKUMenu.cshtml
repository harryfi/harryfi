@using MasterOnline.ViewModels
@model multiSKUViewModel
@{
    ViewBag.Title = "Multi SKU";
    Layout = "~/Views/Shared/_LayoutManage.cshtml";
    var context = new MoDbContext("");
}

@section styles
{
    <link href="~/Content/build/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link href="~/Content/selectize.css" rel="stylesheet" />
    <link href="~/Content/build/css/selectivity-jquery.min.css" rel="stylesheet" />

    <style>
        .input-group {
            margin-bottom: 0;
        }

        #piutang-section > div {
            background-color: #fff;
            padding: 20px;
        }

        #Piutang_AL {
            resize: vertical;
            height: 100px;
            max-height: 100px;
        }

        #nm-brg-acuan {
            color: orange;
            font-weight: bold;
        }
        #nm-brg-sku {
            color: orange;
            font-weight: bold;
        }
    </style>
}

<div class="row" id="MultiSKU-section">
    <div class="col-lg-12 col-md-12">
        <div class="row MultiSKU_table_section">
            <div class="col-lg-4 col-sm-6">
                <div class="input-group">
                    <input id="search_MultiSKU" type="text" class="form-control" placeholder="Pencarian">
                    <span class="input-group-btn">
                        <button type="button" id="search_MultiSKU_click" class="btn btn-primary">
                            <i class="fa fa-search"></i>
                        </button>
                    </span>
                </div>
            </div>
            <div class="col-lg-8 col-sm-6">
                <div class="pull-right">
                    <button class="btn btn-primary btn_tambah_data" id="tambah_MultiSKU">Tambah Baru</button>
                </div>
            </div>
        </div>
        <div class="row MultiSKU_table_section" style="margin-top: 5px;">
            <div class="col-sm-12">
                <div id="table-MultiSKU-partial">
                    @*@Html.Partial("TableMultiSKUPartial")*@
                </div>
            </div>
        </div>
        <div class="row MultiSKU_editor_section" style="display: none;">
            <div class="col-lg-12">
                <div class="page-editor">
                    <h2 class="editor-title">Multi SKU</h2>
                    <span class="title-accent"></span>
                    <button id="close-editor" type="button" class="pull-right page-close">
                        <span class="close-btn thick"></span>
                    </button>
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="x_panel">
                                    <div class="x_title">
                                        <h2 style="font-size: 16px">Detail Multi SKU</h2>
                                        <ul class="nav navbar-right panel_toolbox">
                                            <li>
                                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                            </li>
                                        </ul>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="x_content">
                                        @if (ViewData.ModelState.Any(x => x.Value.Errors.Any()))
                                        {
                                            foreach (var modelError in ViewData.ModelState.SelectMany(x => x.Value.Errors))
                                            {
                                                <div class="alert alert-danger">
                                                    <span class="message-error">@modelError.ErrorMessage</span>
                                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                    </button>
                                                </div>
                                            }
                                        }
                                        <br>
                                        @using (Html.BeginForm("SaveMultiSKU", "Manage", FormMethod.Post, new { enctype = "multipart/form-data", id = "form-sku" }))
                                        {
                                            <div id="form-partial">
                                                @*@Html.Partial("FormMultiSKUPartial")*@
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

