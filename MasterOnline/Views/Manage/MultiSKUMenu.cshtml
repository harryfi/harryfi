                document.getElementById("multiSKU_BRG_ACUAN").focus();
                ret = false;
            }
            if ($("#nama-acuan").val() == "" || $("#nama-acuan").val() == null) {
                addError($("#nama-acuan").parent().parent(), "Nama barang acuan tidak boleh kosong.");
                document.getElementById("nama-acuan").focus();
                ret = false;
            }
            return ret;
        }

