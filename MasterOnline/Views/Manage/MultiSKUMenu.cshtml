            function refreshTableMultiSKU() {
                var $link = '@Html.Raw(Url.Action("RefreshTableMultiSKU", "Manage", new { page = "replaceLastPage", search = "replaceSearch" }))';
                $link = $link.replace("replaceSearch", encodeURIComponent($('#search_MultiSKU').val()));
                $link = $link.replace("replaceLastPage", encodeURIComponent($('#txt_last_page').val()));

                $.ajax({
                    type: "GET",
                    url: $link,
                    beforeSend: function () {
                        $('#table-MultiSKU-partial').hide();
                        $('#loading_MultiSKU_tab').show();
                    },
                    success: function (response) {
                        $('#table-MultiSKU-partial').html(response);
                        $('#loading_MultiSKU_tab').hide();
                        $('#table-MultiSKU-partial').show();
                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                    }
                });
        }
        function searchTableMultiSKU(){
            var $link = '@Html.Raw(Url.Action("RefreshTableMultiSKU", "Manage", new { search = "replaceSearch" }))';
            $link = $link.replace("replaceSearch", encodeURIComponent($('#search_MultiSKU').val()));

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#search_MultiSKU_click')[0].disabled = true;
                    $('#loading_MultiSKU_tab').show();
                },
                success: function (response) {
                    $('#search_MultiSKU_click')[0].disabled = false;
                    $('#table-MultiSKU-partial').html(response);
                    $('#loading_MultiSKU_tab').hide();
                },
                error: function (xhr, status, error) {
                    $('#search_MultiSKU_click')[0].disabled = false;
                    console.log(error);
                }
            });
        }

