        function deleteDetailSKUKlik($id,$acuan) {
            if ($id != null && $id != "" && $id != undefined && $acuan != null && $acuan != "" && $acuan != undefined) {
                $('#nm-brg-sku').text($id);
                $('#konfHapusBarang').modal('show');
                $('#btn-delete-sku').off('click').on('click', function () {
                    deleteBarang($id, $acuan)
                });
            }
        }

        function deleteBarang($idBrg, $idAcuan) {
            if ($idBrg != null && $idBrg != "" && $idBrg != undefined && $idAcuan != null && $idAcuan != "" && $idAcuan != undefined) {
                @*var link = '@Url.Action("DeleteDetailMultiSKU", "Manage", new { kdBrg = "replaceBrgId", Acuan = "replaceBrgAcuan" })';
                //link = link.replace("replaceBrgId", encodeURIComponent(this.$brg));
                link = link.replace("replaceBrgId", encodeURIComponent($idBrg));
                link = link.replace("replaceBrgAcuan", encodeURIComponent($idAcuan));*@

                $postdata = {
                    kdBrg: $idBrg,
                    Acuan: $idAcuan
                };


                $.ajax({
                    type: "POST",
                    url: '@Url.Action("DeleteDetailMultiSKU", "Manage")',
                    contentType: "application/json; charset=UTF-8",
                    cache: false,
                    data: JSON.stringify($postdata),
                    beforeSend: function () {
                        $('#loading_spinner').show();
                    },
                    cache: false,
                    success: function (response) {
                        if (response.Errors == null) {
                            $('#form-partial').html(response);
                            $('.MultiSKU_table_section').hide();
                            $('.MultiSKU_editor_section').show();
                        } else {
                            if (response.Errors[0] != null) {
                                alert(response.Errors[0]);
                            } else {
                                console.log(response.Errors);
                                alert('Ada data yang tidak valid!');
                            }
                        }
                        $('#konfHapusBarang').modal('hide');
                        $('#loading_spinner').hide();
                    },
                    error: function (xhr) {
                        $('#loading_spinner').hide();
                        console.log(xhr);
                    }
                });
            }
        }
    </script>
