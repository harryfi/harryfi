        function snapMidtrans(kode, bln, addon) {
            var link = '@Url.Action("PaymentMidtrans", "Midtrans")';
            link += '?code=' + encodeURIComponent(kode) + '&bulan=' + encodeURIComponent(bln) + '&addon=' + encodeURIComponent(addon) + '&accId=0' + '&accCount=' + encodeURIComponent($('#txtJmlUser')[0].innerText);

            $.ajax({
                type: "GET",
                url: link,
                success: function (response) {
                    if (response != null) {
                        //window.location = response.urlView;
                        if (response.error == null) {
                            snap.pay(response.token, { skipOrderSummary: true });
                            $('#modalTransfer').modal('hide');
                        } else {
                            alert(response.error);
                            $('#modalTransfer').modal('hide');
                        }
                    }
                    //window.open(response, "_self");
                    //window.location.href(response);

                },
                error: function (xhr, status, error) {
                    console.log(error);
                    $('#modalTransfer').modal('hide');
                }
            });
        }
