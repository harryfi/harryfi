                        //end unremark by calvin 8 nov 2018, proteksi qty
                        refreshNettoBarang($idBarang);
                    })
                            .show();
                                //add by nurul 19/11/2018
                            },
                            error: function (xhr, status, error) {
                                console.log(error);
                            }
                        });
