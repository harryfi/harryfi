                    type: "GET",
                    url: link,
                    contentType: 'application/json',
                    cache: false,
                    success: function (res) {
                //end add
                $totalAwalBarang = parseInt(convertToAngka($($tr + ' .netto-barang').text()));
                $btnEdit.parent().removeClass('btn-primary').addClass('btn-success');
                $btnEdit.removeClass('glyphicon-pencil').addClass('glyphicon-floppy-disk');
                $qtyInitialSpan.hide();
                $qtyInputBox.val($qtyAwal)
                    //.attr('max', $qtyAwal)
                    .attr('max', res)
                    .change(function() {
                        //unremark by calvin 8 nov 2018, proteksi qty
                        //if (parseInt($(this).val()) > parseInt($qtyAwal)) {
                        if (parseInt($(this).val()) > parseInt(res)) {
                            alert('Kuantitas retur tidak boleh melebihi kuantitas awal invoice (' + res + ')!');
