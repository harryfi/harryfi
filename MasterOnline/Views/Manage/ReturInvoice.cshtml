            $link = $link.replace("replaceSearch", $('#search_invoice').val());

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#search_invoice_click')[0].disabled = true;
                    $('#loading_invoice_tab').show();
                },
                success: function (response) {
                    $('#search_invoice_click')[0].disabled = false;
                    $('#table-invoice-partial').html(response);
                    $('#loading_invoice_tab').hide();
                },
                error: function (xhr, status, error) {
                    $('#search_invoice_click')[0].disabled = false;
                    console.log(error);
                }
            });
        }

