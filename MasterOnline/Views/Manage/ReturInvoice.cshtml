        function simpanBrg($idBarang,$qtyInputBox,$btnEdit,$qtyInitialSpan,qtyInitialBeforeValidation) {
            if (!validateForm()) return false;

            var $tr = 'tr[data-barang-id="' + $idBarang + '"]';

            if ($('#Invoice_TGL').val() != '' &&
                $('#Invoice_SUPP').val() != ''
                //$('#Invoice_TERM').val() != '' &&
                //$('#Invoice_NAMA').val() != ''
            ) {
                var tgl = $('#Invoice_TGL').val();
                $('#InvoiceDetail_TGL_INPUT').val(tgl);
                console.log($($tr + ' .kode-barang').text());
                $('#InvoiceDetail_NO').val($idBarang);
                $('#InvoiceDetail_BK').val('2');
                $('#InvoiceDetail_BRG').val($($tr + ' .kode-barang').text());
                $('#InvoiceDetail_H_SATUAN').val(convertToAngka($($tr + ' .harga-barang').text()));
                $('#InvoiceDetail_QTY').val($($tr + ' .qty-barang').text());
                $('#InvoiceDetail_DISCOUNT').val($($tr + ' .disc1-barang').text());
                $('#InvoiceDetail_DISCOUNT_2').val($($tr + ' .disc2-barang').text());
                $('#InvoiceDetail_NDISC1_1').val(convertToAngka($($tr + ' .ndisc1-barang').text()));
                $('#InvoiceDetail_NDISC1_2').val(convertToAngka($($tr + ' .ndisc2-barang').text()));
                $('#InvoiceDetail_HARGA').val(convertToAngka($($tr + ' .netto-barang').text()));
                $("#Invoice_BRUTO").val(convertToAngka($("#Invoice_BRUTO").val()));
                $("#Invoice_NDISC1").val(convertToAngka($("#Invoice_NDISC1").val()));
                $("#Invoice_NPPN").val(convertToAngka($("#Invoice_NPPN").val()));
                $('#Invoice_BIAYA_LAIN').val(convertToAngka($("#Invoice_BIAYA_LAIN").val()))
                $("#Invoice_NETTO").val($bakalNetto);

                $nettoSebelumnya += $bakalNetto;

                $.ajax({
                    type: "POST",
                    url: '@Url.Action("SaveReturInvoice", "Manage")',
                    data: $('#form-invoice').serialize(),
                    beforeSend: function () {
                        $('#loading_spinner').show();
                    },
                    success: function (response) {
                        if (response.Errors == null) {
                            //add by calvin 8 nov 2018
                            $btnEdit.parent().removeClass('btn-success').addClass('btn-primary');
                            $btnEdit.removeClass('glyphicon-floppy-disk').addClass('glyphicon-pencil');
                            $qtyInitialSpan.show();
                            $qtyInputBox.hide();
                            //end add by calvin 8 nov 2018

                            $modeEdit = 1; // 0 new, 1 edit

                            $('#form-partial').html(response);
                            refreshInputan();
                            getTanggal();
                            getSupplier();
                            getInvoice();
                            //change by nurul 2/11/2018 -- $('#btn-simpan-perubahan-row').show();
                            $('#btn-simpan-perubahan-row').hide();
                            $('#btn-autoload-row').hide();
                            $('tr[data-invoice-id="0"]').hide();
                            $('#loading_spinner').hide();
                        } else {
                            //add by calvin 8 nov 2018, kembalikan value initial ke semula ( karena gagal save )
                            $qtyInitialSpan.text(qtyInitialBeforeValidation);
                            //end add by calvin 8 nov 2018
                            console.log(response.Errors);
                            alert(response.Errors);
                            $('#loading_spinner').hide();
                        }
                    },
                    error: function (xhr) {
                        console.log(xhr);
                    }
                });
            } else {
                alert('Silakan isi semua field terlebih dahulu!');
            }
        }
    </script>

    <script>
        //Disable Enter Key Submit Form
        function stopRKey(evt) {
            var evt = (evt) ? evt : ((event) ? event : null);
            var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);
            if ((evt.keyCode === 13) && (node.type === "text")) { return false; }
        }
        document.onkeypress = stopRKey;
    </script>
