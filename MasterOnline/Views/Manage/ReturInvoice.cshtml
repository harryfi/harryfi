            } else {
                if ($qtyInputBox.val() != '' && parseInt($qtyInputBox.val()) > 0) {
                    //remark by calvin 8 nov 2018, pindah ke setelah simpanBrg, karena ada validasi lewat manageController
                    //$btnEdit.parent().removeClass('btn-success').addClass('btn-primary');
                    //$btnEdit.removeClass('glyphicon-floppy-disk').addClass('glyphicon-pencil');
                    var qtyInitialBeforeValidation = $qtyInitialSpan.val();
                    $qtyInitialSpan.text($qtyInputBox.val());//.show();
                    //$qtyInputBox.hide();
                    //end remark by calvin 8 nov 2018
                    //add by nurul 28/12/2018
                    refreshNettoBarang($idBarang);
                    //end add by nurul 
                    simpanBrg($idBarang,$qtyInputBox,$btnEdit,$qtyInitialSpan,qtyInitialBeforeValidation);
