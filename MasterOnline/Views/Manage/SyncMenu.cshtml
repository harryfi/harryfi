@section scripts
{
    <script>
        function warningDataPerusahaan() {
            alert('Isi data perusahaan terlebih dahulu!');
            return;
        }

        $(document).ready(function () {
            $('#close-editor-1').off('click').on('click', function () {
                //$('#close-editor-1')[0].disabled = true;
                $('.sync_editor_section').hide();
                $('.sync_step').show();
            });
        });

        function ViewLog() {
            var link = '@Url.Action("ListLogSinkronisasi", "Manage")';
            var listCust = [];

            $('.logCustomers').each(function (i, item) {
                if ($(item).is(":checked")) {
                    //if ($(item).attr("data-log") != undefined) {
                        listCust.push($(item).attr("data-log"));
                    //}
                }
            });
            link += '?cust=' + encodeURIComponent(listCust);
            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function() {
                    $('#loading_upload_log').show();
                    //$('#form-partial').html('');
                },
                success: function (response) {
                    $('#uploadbrgviewdiv').html(response);
                    $('#loading_upload_log').hide();
                },
                error: function(xhr, status, error) {
                    console.log(error);
                    $('#loading_upload_log').hide();
                }
            });
        }

        function GetDataMenu(step) {
            var link = '@Url.Action("GetDataMenu", "Manage")';
            //setMode(step);
            $('.label_record').hide();
            //$('.ckCustomers').each(function (i, item) {
            //    $(item).prop('checked', true);
            //});
            rows_selected = [];
            listErrorMessage = '';

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function() {
                    $('#loading_spinner').show();
                    $('#form-partial').html('');
                },
                success: function (response) {
                    $('#form-partial').html(response);
                    $('.sync_editor_section').show();
                    $('.sync_step').hide();
                    setMode(step);
                    $('#loading_spinner').hide();
                },
                error: function(xhr, status, error) {
                    console.log(error);
                    $('#loading_spinner').hide();
                    setMode(step);
                }
            });
        }

        function setMode(step) {
            $('#proses_btn').hide();
            $('#download_btn').hide();
            $('#upload_btn').hide();
            $('#log_btn').hide();
            $('#div_with_list').hide();
            $('#div_with_upload').hide();
            $('#tbl_progbar').remove();
            $('.div-spinner').hide();
            if (step == 1) {
                $('#proses_btn').show();
                $('#div_with_list').show();
                document.getElementById('lblForm').innerText = "Ambil Data Barang dari Marketplace";
            }
            else if (step == 2) {
                $('#download_btn').show();
                $('#div_with_list').show();
                document.getElementById('lblForm').innerText = "Download Data Barang ke Excel";
            }
            else if (step == 4) {
                $('#log_btn').show();
                $('#div_with_upload').show();
                document.getElementById('lblForm').innerText = "Upload Excel dan Sinkronisasi Data Barang ke Master Online";
            }
        }

        var rows_selected = [];
        window.selected_cust = [];
        var exGetData = 0;

        function GetAllDataBrg(listCust) {
            if (listCust == undefined) {
                $('.ckCustomers').each(function (i, item) {
                    if ($(item).is(":checked")) {
                        rows_selected.push($(item).attr("data-cust"));
                    }
                });

                if (rows_selected.length == 0) {
                    alert('Anda belum memilih Toko yang akan diambil data barangnya.');
                } else {
                    GetDataBrgWithRecord(rows_selected[0], 0, 0, 1, 0);
                }
            } else {
                GetDataBrgWithRecord(listCust[0], 0, 0, 1, 0);
            }

        }

        function checkfile(s) {
            var validExts = new Array(".xlsx", ".xls");
            var fileExt = s.value;
            if (fileExt != null && fileExt != "") {
                fileExt = fileExt.substring(fileExt.lastIndexOf('.'));
                if (validExts.indexOf(fileExt) < 0) {
                    alert("Invalid file selected, valid files are of " +
                        validExts.toString() + " types.");
                    document.getElementById("file_upload_excel").value = "";

                    return false;
                }
                updateList();
            }
        }

        function updateList (){
            var input = document.getElementById('file_upload_excel');
            var output = document.getElementById('fileList');

            output.innerHTML = '<ul>';
            for (var i = 0; i < input.files.length; ++i) {
                output.innerHTML += '<li>' + input.files.item(i).name + '</li>';
            }
            output.innerHTML += '</ul>';
        }

        var msg2 = '';
        var namaFileLog = '';
        var lastPage = 0;
        var loop = 1;
        //get data barang with record
        function GetDataBrgWithRecord(cust, page, recordCount, statBL, totalData) {
                 var link = '@Url.Action("ImportDataMP", "Manage")';
            link += '?cust=' + encodeURIComponent(cust) + '&page=' + encodeURIComponent(page) + '&recordCount=' + encodeURIComponent(recordCount) + '&statBL=' + encodeURIComponent(statBL) + '&totalData=' + encodeURIComponent(totalData);
                $.ajax({
                    type: "GET",
                    url: link,
                    beforeSend: function () {
                        $('#lblPleaseWait').show();
                        //document.getElementById('lblRecord_' + cust).innerText = 'Jumlah data berhasil tersimpan ke MO : 0';
                        //document.getElementById('lblTotalData_' + cust).innerText = 'Jumlah data yang didapat dari marketplace : 0';
                        $('#lblRecord_' + cust).show();
                        $('#lblTotalData_' + cust).show();
                        $('#spinner_' + cust).show();
                    },
                    success: function (response) {
                        lastPage = page;
                        if (response.Errors == null || response.Errors.length == 0) {
                            if (response.RecordCount != null) {
                                document.getElementById('lblRecord_' + cust).innerText = 'Jumlah data berhasil tersimpan ke MO : ' + response.RecordCount;
                                document.getElementById('lblTotalData_' + cust).innerText = 'Jumlah data yang didapat dari marketplace : ' + response.totalData;
                            }
                            if (response.exception == 1)
                                exGetData = 1;
                            if (response.Recursive) {
                                if (response.BLProductActive == 1) {
                                    GetDataBrgWithRecord(cust, response.Page, response.RecordCount, 1, response.totalData);
                                } else {
                                    GetDataBrgWithRecord(cust, response.Page, response.RecordCount, 0, response.totalData);
                                }
                            } else {
                                //document.getElementById('txtInfo').innerText = '';
                                //alert('Jumlah data berhasil tersimpan :' + response.RecordCount);
                                //refreshTableBarangUpload();
                                //$('#info_record').hide();
                                //document.getElementById('txtInfo').innerText = 'Jumlah data berhasil tersimpan ke Master Online : 0';
                                if (exGetData == 1) {
                                    msg2 += '\nAda data barang yang tidak masuk ke MO dari akun : ' + document.getElementById('lblNama_' + cust).innerText + '.';
                                    exGetData = 0;
                                }

                                $('#spinner_' + cust).hide();
                                rows_selected.shift();
                                window.selected_cust.push(cust);
                                if (rows_selected.length > 0) {
                                    GetAllDataBrg(rows_selected);
                                } else {
                                    $('#lblPleaseWait').hide();
                                    if (msg2 != '') {
                                        msg2 += '\nSilahkan lakukan pengambilan data lagi. \nJika anda masih mendapat pesan error ini setelah mencoba lagi, silahkan hubungi tim support MO.';
                                    }
                                    alert('Proses mengambil data barang dari marketplace telah selesai.' + msg2);
                                    msg2 = '';
                                }
                            }
                        } else {
                            //alert(response.Errors);
                            //$('#loading_barang_tab').hide();
                            //$('#datatable').show();
                            //$('#info_record').hide();
                            document.getElementById('lblListError').innerText = response.Errors;
                            $('#modalError').modal('show');
                            $('#spinner_' + cust).hide();
                            rows_selected.shift();
                            window.selected_cust.push(cust);
                            if (rows_selected.length > 0) {
                                GetAllDataBrg(rows_selected);
                            } else {
                                $('#lblPleaseWait').hide();
                                alert('Proses mengambil data barang dari marketplace telah selesai.');
                            }
                        }
                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                        if (xhr.status == 524) {
                            if (lastPage == page) {
                                loop = loop + 1;
                            } else {
                                loop = 1;
                            }
                            if (loop <= 3) {
                                //GetDataBrgWithRecord(page, recordCount, statBL, totalData);
                                GetDataBrgWithRecord(cust, page, recordCount, statBL, totalData);
                            } else {
                                alert('Terdapat Error saat mengambil data barang marketplace ini.\nSilahkan hubungi tim support MO');
                                //refreshTableBarangUpload();
                                //$('#lblPleasewait').hide();
                                //$('#info_record').hide();
                                $('#spinner_' + cust).hide();
                                rows_selected.shift();
                                window.selected_cust.push(cust);
                                if (rows_selected.length > 0) {
                                    GetAllDataBrg(rows_selected);
                                } else {
                                    $('#lblPleaseWait').hide();
                                    alert('Proses mengambil data barang dari marketplace telah selesai.');
                                }
                            }
                        } else {
                            $('#spinner_' + cust).hide();
                            rows_selected.shift();
                            window.selected_cust.push(cust);
                            if (rows_selected.length > 0) {
                                GetAllDataBrg(rows_selected);
                            } else {
                                $('#lblPleaseWait').hide();
                                alert('Proses mengambil data barang dari marketplace telah selesai.');
                            }
                        }                        
                        //var err = JSON.parse(xhr.responseText);
                        //alert(err.Message);
                        //console.log(err.error);

                    }
                });
                //$('#konfTarikData').modal('hide');
                //$('#konfSyncAll').modal('hide');
        }
        //end get data barang with record
        function ApproveDl() {
            $('.ckCustomers').each(function (i, item) {
                if ($(item).is(":checked")) {
                    rows_selected.push($(item).attr("data-cust"));
                }
            });

            if (rows_selected.length == 0) {
                alert('Anda belum memilih Toko yang akan diambil data barangnya.');
            } else {
                var msg = "Apakah anda ingin mendownload file berikut ini :";
                for (i = 0; i < rows_selected.length; i++) {
                    msg += "\n" + '@username' + "_" + $('#lblNama_' + rows_selected[i])[0].innerText + ".xlsx";
                }
                if (confirm(msg)) {
                    exportExcel(rows_selected[0]);
                }
            }
        }
        function DownloadAllExcel(listCust) {
            if (listCust == undefined) {
                $('.ckCustomers').each(function (i, item) {
                    if ($(item).is(":checked")) {
                        rows_selected.push($(item).attr("data-cust"));
                    }
                });

                if (rows_selected.length == 0) {
                    alert('Anda belum memilih Toko yang akan diambil data barangnya.');
                } else {
                    exportExcel(rows_selected[0]);
                }
            } else {
                exportExcel(listCust[0]);
            }
        }

        function exportExcel(cust) {
            var link = '@Html.Raw(Url.Action("SQLtoExcel", "TransferExcel"))';
            link += "?cust=" + encodeURIComponent(cust);

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                async: false,
                beforeSend: function () {
                    //$('#loading_spinner_btn').show();
                    $('#lblRecord_' + cust).show();
                    $('#spinner_' + cust).show();
                },
                success: function (response) {
                    $('#loading_spinner_btn').hide();
                    if (response.Errors.length == 0) {
                        var bytes = new Uint8Array(response.byteExcel);
                        saveByteArray(response.namaFile, bytes);
                        document.getElementById('lblRecord_' + cust).innerText = 'sukses download';
                    } else {
                        //alert(response.Errors[0]);
                        document.getElementById('lblRecord_' + cust).innerText = 'gagal download : ' + response.Errors[0];
                    }
                    $('#spinner_' + cust).hide();
                    rows_selected.shift();
                    if (rows_selected.length > 0) {
                        DownloadAllExcel(rows_selected);
                    } else {
                        //alert('Proses mengambil data barang dari marketplace telah selesai.');
                        //alert('Proses download data barang telah selesai.');
                    }
                },
                error: function (xhr) {
                    //$('#loading_spinner_btn').hide();
                    console.log(xhr);
                    $('#spinner_' + cust).hide();
                    rows_selected.shift();
                    if (rows_selected.length > 0) {
                        DownloadAllExcel(rows_selected);
                    } else {
                        //alert('Proses download data barang telah selesai.');
                    }
                }
            });
        }

        function saveByteArray(reportName, byte) {
            var blob = new Blob([byte], { type: "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" });
            var link = document.createElement('a');
            link.href = window.URL.createObjectURL(blob);
            var fileName = reportName;
            link.download = fileName;
            //document.body.appendChild(link);
            link.click();
            //document.body.removeChild(link);
        };

