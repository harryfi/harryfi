        function UploadAllBrgWithProgressBar(currentPage, maxPage, dataPerPage, failedToSync) {

            var link = '@Url.Action("UploadItemByCust", "Manage")';
            link += "?cust=" + encodeURIComponent(rows_selected[0]) + /*"&page=" + currentPage +*/ "&dataPerPage=" + encodeURIComponent(dataPerPage) + "&skipDataError=" + encodeURIComponent(failedToSync) + '&namaFileLog=' + encodeURIComponent(namaFileLog);

            $.ajax({
                type: "GET",
                url: link,
                //beforeSend: function () {
                //    $('#loading_barang_tab').show();
                //},
                success: function (response) {
                    lastPage = currentPage;
                    if (response.Errors != null) {
                        if (response.Errors.length > 0) {
                            response.Errors.forEach(ErrorMessageDialog);
                            //alert(listErrorMessage);
                            //document.getElementById('txtMessage').innerText = listErrorMessage;
                        }
                    }
                    $('#pBar_' + rows_selected[0]).css('width', parseFloat(((currentPage + 1) / (maxPage + 1) * 100)).toFixed(2) + '%').attr('aria-valuenow', (currentPage + 1) / (maxPage + 1) * 100);
                    $('#pBar_' + rows_selected[0]).html(parseFloat(((currentPage + 1) / (maxPage + 1) * 100)).toFixed(2) + '% Complete');
                    if (response.contRecursive == "1") {
                        if ((currentPage + 1) > maxPage) {
                            lastPage = 0;
                            loop = 1;
                            //alert('Sinkronisasi barang selesai');
                            //$('#btnSelesai').prop('disabled', false)
                            rows_selected.shift();
                            if (rows_selected.length > 0) {
                                //DownloadAllExcel(rows_selected);
                                startSyncAll();
                            } else {
                                alert('Proses sinkronisasi data barang telah selesai.');
                                //alert(listErrorMessage);
                                if (listErrorMessage != "") {
                                    document.getElementById('lblListError').innerText = listErrorMessage;
                                    $('#modalError').modal('show');
                                }
                                
                            }
                        } else {
                            UploadAllBrgWithProgressBar((currentPage + 1), maxPage, dataPerPage, response.failedRecord);
                        }
                    } else {
                        //$('#btnSelesai').prop('disabled', false);
                        rows_selected.shift();
                        if (rows_selected.length > 0) {
                            startSyncAll();
                        } else {
                            alert('Proses mengambil data barang dari marketplace telah selesai.');
                            //alert(listErrorMessage);
                            if (listErrorMessage != "") {
                                document.getElementById('lblListError').innerText = listErrorMessage;
                                $('#modalError').modal('show');
                            }
                        }
                    }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                    //$('#table-upload-barang-partial').show();
                    //$('#btnSelesai').prop('disabled', false);
                    if (xhr.status == 524) {
                        if (lastPage == currentPage) {
                            loop = loop + 1;
                        } else {
                            loop = 1;
                        }
                        if (loop <= 3) {
                            UploadAllBrgWithProgressBar(currentPage, maxPage, dataPerPage, failedToSync);
                        } else {
                            alert('Terdapat Error saat sinkroisasi data barang dari excel ini.\nSilahkan hubungi tim support MO');
                            //refreshTableBarangUpload();
                            //$('#lblPleasewait').hide();
                            //$('#info_record').hide();
                            rows_selected.shift();
                            if (rows_selected.length > 0) {
                                startSyncAll();
                            }
                        }
                    } else {
                        rows_selected.shift();
                        if (rows_selected.length > 0) {
                            startSyncAll();
                        } else {
                            alert('Proses mengambil data barang dari marketplace telah selesai.');
                            //alert(listErrorMessage);
                            if (listErrorMessage != "") {
                                document.getElementById('lblListError').innerText = listErrorMessage;
                                $('#modalError').modal('show');
                            }
                        }
                    }
                }
            });

        }

        var listErrorMessage = "";
        function ErrorMessageDialog(item, index) {
            var error = item.split(';');
            if (error.length > 1) {
                if (error.length > 2) {
                    listErrorMessage += '\nKode Barang :' + error[0] + '\nError message :' + error[2] + '\n';
                } else {
                    listErrorMessage += '\nKode Barang :' + error[0] + '\nError message :' + error[1] + '\n';
