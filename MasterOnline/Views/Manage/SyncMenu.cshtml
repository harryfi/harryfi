        function uploadfileexcelbatch(fileNum, pageNum) {
            //if ($('#file_upload_excel')[0].files.length < 1) {
            if ($('#file_upload_excel')[0].files.length <= fileNum) {
                $('#progressBarUpload').hide();
                $('#pBar_uploadFile').css('width', '0%').attr('aria-valuenow', 0);
                $('#pBar_uploadFile').html('0% Complete');
                $('#loading_spinner').hide();
                $('#sync_btn').click();
                //$('#file_upload_excel').click();
            }
            else
            {
                var formdata = new FormData(); //FormData object
                var fileInput = document.getElementById('file_upload_excel');
                //Iterating through each files selected in fileInput
                for (i = 0; i < fileInput.files.length; i++) {
                    //Appending each file to FormData object
                    if (i == fileNum) {
                        formdata.append(fileInput.files[i].name, fileInput.files[i]);
                        $('#lblPBUpload').text('Uploading File : ' + fileInput.files[i].name)
                    }
                }
                var url1 = '@Url.Action("UploadXcelwithPage", "TransferExcel", new { page = "replacePage"})';
                url1 = url1.replace("replacePage", encodeURIComponent(pageNum));
                $.ajax({
                    type: "POST",
                    url: url1,
                    data: formdata,
                    processData: false,  // tell jQuery not to process the data
                    contentType: false,  // tell jQuery not to set contentType
                    beforeSend: function () {
                        //$('#loading_spinner').show();
                    },
                    success: function (response) {
                        lastPage = pageNum;
                        if (response.lastRow != null) {
                            if (parseInt(response.lastRow[0]) > 0) {
                                var maxPage2 = parseInt(parseInt(response.lastRow[0]) / 300);
                                $('#pBar_uploadFile').css('width', parseFloat(((pageNum + 1) / (maxPage2 + 1) * 100)).toFixed(2) + '%').attr('aria-valuenow', (pageNum + 1) / (maxPage2 + 1) * 100);
                                $('#pBar_uploadFile').html(parseFloat(((pageNum + 1) / (maxPage2 + 1) * 100)).toFixed(2) + '% Complete');
                            }
                        }
                        if (response.nextFile) {
                            loop = 1;
                            rows_selected.push(response.cust[0]);
                            listCustomerUploaded.push(response.namaCust[0]);
                            uploadfileexcelbatch(fileNum + 1, 0);
                        } else {
                            if (response.Errors.length == 0) {
                                //$('#loading_spinner').hide();
                                ////alert('Data anda berhasil di Update.\nSilahkan lanjutkan sinkronisasi, dapat secara manual satu per satu atau otomatis semua barang.');
                                ////$('#upload_btn').show();
                                //rows_selected = response.cust;
                                //listCustomerUploaded = response.namaCust;
                            } else {
                                var err = "";
                                for (i = 0; i < response.Errors.length; i++) {
                                    err += response.Errors[i] + "\n";
                                }
                                //alert(err);
                                document.getElementById('lblListError').innerText += err;
                                //$('#modalError').modal('show');
                                //$('#loading_spinner').hide();
                                ////$('#upload_btn').show();
                                //rows_selected = response.cust;
                                //listCustomerUploaded = response.namaCust;
                            }
                            //$('#sync_btn').click();
                            uploadfileexcelbatch(fileNum, pageNum + 1);
                        }
                        
                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                        //$('#loading_spinner').hide();
                        if (xhr.status == 524) {
                            if (lastPage == pageNum) {
                                loop = loop + 1;
                            } else {
                                loop = 1;
                            }
                            if (loop <= 3) {
                                uploadfileexcelbatch(fileNum, pageNum);
                            } else {
                                alert('Terdapat Error saat mengupload data barang excel ini.\nSilahkan hubungi tim support MO');
                                //refreshTableBarangUpload();
                                //$('#lblPleasewait').hide();
                                //$('#info_record').hide();
                                //$('#loading_spinner').hide();
                            }
                        }
                    }
                });
            }
        }

