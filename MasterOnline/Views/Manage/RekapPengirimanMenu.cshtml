        function getDataPesanan() {
            if ($('#Pengiriman_NAMA_EKSPEDISI').val() != '') {
                var link = '@Url.Action("GetDataPesananSerahTerima", "Manage", new { code = "replaceCode" })';
                link = link.replace("replaceCode", $('#Pengiriman_NAMA_EKSPEDISI').val());
                $.ajax({
                    type: "GET",
                    //url: '@Url.Action("GetDataPesananSerahTerima", "Manage")',
                    url: link,
                    contentType: 'application/json',
                    cache: false,
                    success: function (data) {
                        var listKodePesanan = [];

                        $.each(data,
                            function (i, item) {

                                listKodePesanan[i] = {
                                    id: item.NoPesanan,
                                    text: item.NoPesanan

                                };
                            });

                        $('#PESANAN').selectivity({
                            allowClear: true,
                            items: listKodePesanan,
                            placeholder: 'Harap pilih'
                        }).change(function () {
                            var val = $(this).selectivity('value');
                            var $trId = $(this).closest('tr').attr('data-kirim-id');
                            var $trElement = 'tr[data-kirim-id="' + $trId + '"]';

                            if (val != null) {
                                $.each(data,
                                    function (i, item) {
                                        if (item.NoPesanan == val) {
                                            var $link = '@Url.Action("GetDataPesananSerahTerimaDetail", "Manage", new { code = "replaceCode" })';
                                            $link = $link.replace("replaceCode", val);
                                            $.ajax({
                                                type: "GET",
                                                url: $link,
                                                success: function (response) {
