        function getTanggal() {
            var $flagPerubahan = 0;


            if ($modeEdit == 1) { // mode edit
                $('#Pengiriman_TGL_KIRIM').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();

                    }
                    $(this).datepicker('hide');

                    $flagPerubahan++;
                    });

                $('#timepicker1').timepicker({
                    minuteStep: 1,
                    showSeconds: false,
                    showMeridian: false,
                    defaultTime: 'current',
                    showWidget: true,
                    showInputs: true,
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();

                    }
                    $(this).timepicker('hide');

                    $flagPerubahan++;
                });
            }
            else {
                $('#Pengiriman_TGL_KIRIM').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).datepicker('setDate', '0').change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();
                    }
                    $(this).datepicker('hide');

                    $flagPerubahan++;
                    $modeEdit = 1;
                    });
                $('#timepicker1').timepicker({
                    minuteStep: 1,
                    showSeconds: false,
                    showMeridian: false,
                    defaultTime: 'current',
                    showWidget: true,
                    showInputs: true,
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();
                    }
                    $(this).timepicker('hide');

                    $flagPerubahan++;
                    $modeEdit = 1;
                });
            }
        }

