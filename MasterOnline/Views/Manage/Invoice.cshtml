        function validasiTambahan() {
            var $isValid = true;
            var $Gd = $('#GUDANG');
            var $this = $('tr[data-invoice-id="0"] .qty-barang');
            //add by nurul 19/9/2019
            var $total = $('tr[data-invoice-id="0"] .netto-barang');
            //end add by nurul 19/9/2019
            if ($Gd.val() == "-" || $Gd.val() == "") {
                if (!$Gd.closest('div').find('.text-danger').length) {
                    if ($Gd.closest('div').hasClass('input-group')) {
                        $('tr[data-invoice-id="0"] .gd-barang').parent().parent().append('<span class="text-danger">Gudang tidak boleh kosong!</span>');
                    } else {
                        $('tr[data-invoice-id="0"] .gd-barang').closest('div').append('<span class="text-danger">Gudang tidak boleh kosong!</span>');
                    }
                    $isValid = false;
                }
            } else if ($this.text().trim() == "0" || $this.text().trim() == "") {
                if (!$this.closest('div').find('.text-danger').length) {
                    if ($this.closest('div').hasClass('input-group')) {
                        $this.parent().parent().append('<span class="text-danger">Quantity tidak boleh 0!</span>');
                    } else {
                        $this.closest('div').append('<span class="text-danger">Quantity tidak boleh 0!</span>');
                    }
                    $isValid = false;
                }
            }
            //add by nurul 19/9/2019
            else if (convertToAngka($total.text()) == 0 || convertToAngka($total.text()) == "") {
                if (!$total.closest('div').find('.text-danger').length) {
                    if ($total.closest('div').hasClass('input-group')) {
                        $total.parent().parent().append('<span class="text-danger">total tidak boleh 0!</span>');
                    } else {
                        $total.closest('div').append('<span class="text-danger">total tidak boleh 0!</span>');
                    }
                    $isValid = false;
                }
            }
            //end add by nurul 19/9/2019
            else {
                if ($Gd.closest('div').find('.text-danger').length) {
                    $Gd.closest('div').find('.text-danger').remove();
                } else if ($this.closest('div').find('.text-danger').length) {
                    $this.closest('div').find('.text-danger').remove();
                }
            }

            return $isValid;

        }
