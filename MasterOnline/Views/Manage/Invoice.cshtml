        function deleteInvoice(vid, vnobuk, vcountAll, vpercentvprogress, vstatusLoopvSuccess) {
            var splitpro = vpercentvprogress.split(';');
            var vpercent = splitpro[0];
            var vprogress = splitpro[1];

            var splitStatus = vstatusLoopvSuccess.split(';');
            var vstatusLoop = splitStatus[0];
            var vstatusSuccess = splitStatus[1];

            $('#konfHapusInvoice_btn_ya').attr('disabled', 'disabled');
            $('#lblPBDelete').text('Processing Delete');

            var $savePage = $('#txt_last_page').val();

            var $urlProgress = "";
                    $urlProgress = '@Html.Raw(Url.Action("DeleteInvoice", "Manage"))';

            if (vid == null || vid == "") {
                vid = $idStokTemp;
            }
            if (vnobuk == null || vnobuk == "") {
                vnobuk = $NobukTemp;
            }

            $postdata = {
                stokId: vid,
                nobuk: vnobuk,
                countAll: vcountAll,
                percentDanprogress: String(vpercent) + ";" + String(vprogress),
                statusLoopSuccess: String(vstatusLoop) + ";" + String(vstatusSuccess)
            };

            $.ajax({
                type: "POST",
                url: $urlProgress,
                contentType: "application/json; charset=UTF-8",
                cache: false,
                data: JSON.stringify($postdata),
                beforeSend: function () {
                    $('#konfHapusInvoice_btn_tidak').hide();
                },
                success: function (response) {
                    if (response.countAll != null && response.statusLoop == true && response.statusSuccess == false && response.TidakLanjutProses == false) {
                        $('#pBar_Delete').css('width', response.percent + '%').attr('aria-valuenow', response.percent);
                        $('#pBar_Delete').html(response.percent + '% Complete');

                        if (response.progress <= response.countAll) {
                            deleteInvoice(response.id, response.nobuk, response.countAll, String(response.percent) + ";" + String(response.progress + 1), String(response.statusLoop) + ";" + String(response.statusSuccess));
                        }

                    } else if (response.statusSuccess == true) {
                        $('#pBar_Delete').css('width', '100%').attr('aria-valuenow', 100);
                        $('#pBar_Delete').html('100% Complete');
                        $('#konfHapusInvoice_btn_ya').removeAttr('disabled');
                        $('#konfHapusInvoice_btn_tidak').show();
                        alert('Proses delete invoice sudah selesai.');
