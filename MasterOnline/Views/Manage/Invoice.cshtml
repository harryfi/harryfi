                    console.log(error);
                    alert(error);
                    //$('#loading_spinner').hide();
                    $('#progressBarDelete').hide();
                    $('#konfHapusInvoice_btn_ya').removeAttr('disabled');
                    $('#konfHapusInvoice_btn_tidak').show();
                    $("#konfHapusInvoice").modal('hide');
                }
            });
        }
