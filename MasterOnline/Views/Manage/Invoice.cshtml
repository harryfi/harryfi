                    keydown: function (e) {
                        var keyCode = e.keyCode || e.which;

                        if ($(this).hasClass('qty-input')) {
                            numOnlyOnKeyDown(e);
                            if (keyCode === 190 || keyCode === 188) e.preventDefault();

                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                    $(this).val('0');
                                }
                                $('#table_tambah_invoice > tbody > tr > td.disc1-barang').click();
                                e.preventDefault();
                            }
                        } else if ($(this).hasClass('disc-input') && $(this).parent().hasClass('disc1-barang')) {
                            //numOnlyOnKeyDown(e);
                            numOnlyAllowDecimalOnKeyDown(e);
                            //if (keyCode === 190 || keyCode === 188) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                    $(this).val('0');
                                    $('#table_tambah_invoice > tbody > tr > td.ndisc1-barang').click().click();
                                }
                                else if ($(this).val() == 0) {
                                    $('#table_tambah_invoice > tbody > tr > td.ndisc1-barang').click().click();
                                } else {
                                    $('#table_tambah_invoice > tbody > tr > td.disc2-barang').click();
                                }

                                e.preventDefault();
                            }
                        } else if ($(this).hasClass('disc-input') && $(this).parent().hasClass('disc2-barang')) {
                            //numOnlyOnKeyDown(e);
                            numOnlyAllowDecimalOnKeyDown(e);
                            //if (keyCode === 190 || keyCode === 188) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                    $(this).val('0');
                                    $('#table_tambah_invoice > tbody > tr > td.ndisc2-barang').click().click();
                                }
                                else if ($(this).val() == 0) {
                                    $('#table_tambah_invoice > tbody > tr > td.ndisc2-barang').click().click();
                                } else {
                                    $(this).blur();
                                }

                                e.preventDefault();
                            }
                        } else if ($(this).hasClass('ndisc-input') && $(this).parent().hasClass('ndisc1-barang')) {
                            numOnlyAllowDecimalOnKeyDown(e);
                            //if (keyCode === 190) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                    $(this).val('0');
                                    $('#table_tambah_invoice > tbody > tr > td.disc2-barang').click();
                                }
                                else if ($(this).val() == '0,00') {
                                    $('#table_tambah_invoice > tbody > tr > td.disc2-barang').click();
                                } else {
                                    $('#table_tambah_invoice > tbody > tr > td.ndisc2-barang').click();
                                }

                                e.preventDefault();
                            }
                        } else {
                            //numOnlyOnKeyDown(e);
                            numOnlyAllowDecimalOnKeyDown(e);
                            //if (keyCode === 190) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                    $(this).val('0,00');
                                }
                                $(this).blur();
                            }
                        }
                    },
                    change: function () {
                        //remark by nurul 27/2/2019
                        //if ($(this).hasClass('qty-input')) {
                        //    if ($(this).val() > 999) {
                        //        $(this).val(999);
                        //    }
                        //}
                        //end remark by nurul 27/2/2019

                        if ($(this).hasClass('disc-input')) {
                            if ($(this).val() > 99.99) {
                                $(this).val(99.99);
                            }
                        }
                        if ($(this).hasClass('disc-input') || $(this).hasClass('ndisc-input')) {
                            var num = parseFloat($(this).val());
                            var cleanNum = num.toFixed(2);
                            $(this).val(cleanNum);
                        }
                        if ($(this).hasClass('disc-input') || $(this).hasClass('ndisc-input')) {
                            if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                $(this).val('0,00');
                            }
                        }
                        if ($(this).hasClass('qty-input')) {
                            if ($(this).val() == "" || $(this).val() == undefined || $(this).val() == "NaN") {
                                $(this).val('0');
                            }
                        }
                    }
                }).appendTo($this.empty()).focus();
            });
        }

        function getGudang() {
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetGudang", "Manage")',
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listGudang = [];

                    $.each(data,
                        function (i, item) {
                            listGudang[i] = {
                                id: item.Kode_Gudang,
                                text: item.Nama_Gudang
                            };
                        });

                    var gudangSelect = $('#GUDANG').selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: listGudang,
                        onChange: function(value) {
                            $('#InvoiceDetail_GD').val(value);
                            //console.log(value);

                            if (value != null) {
                                $('#gudang-kode').text(value);

                                // Otomatis masuk ke field Qty setelah pilih gudang
                                $('#table_tambah_invoice > tbody > tr[data-invoice-id="0"] > td.qty-barang').click();
                            } else {
                                $('#gudang-kode').text('-');
                            }
                            //remark by nurul 7/8/2019
                            //$('#btn-simpan-perubahan-row').show();
                            //end remark by nurul 7/8/2019
                        }
                    });

                    gudangSelect[0].selectize.setValue($('#InvoiceDetail_GD').val());
                    //remark by nurul 7/8/2019
                    //$('#btn-simpan-perubahan-row').hide();
                    //end remark by nurul 7/8/2019
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }

        function getSupplier() {
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetSupplier", "Manage")',
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var suppList = [];

                    $.each(data,
                        function (i, item) {
                            suppList[i] = {
                                id: item.SUPP,
                                text: item.NAMA,
                                term: item.TERM
                            };
                        });

                    var suppSelect = $('#SUPP').selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: suppList,
                        onChange: function(value) {
                            if (value == null) {
                                $("#Invoice_SUPP").val(null);
                            } else {
                                $("#Invoice_SUPP").val(value);
                                $.each(suppList,
                                    function(i, item) {
                                        if (item.id == value) {
                                            $('#Invoice_TERM').val(item.term).change();
                                        }
                                    });
                            }

                            getTanggal();
                            $('#btn-simpan-perubahan-row').show();
                        }
                    });

                    //change by calvin 21 juni 2018, set silent jadi true, agar event onchange tidak jalan saat set Faktur_CUST saat buka form faktur
                    suppSelect[0].selectize.setValue($('#Invoice_SUPP').val(), true);
                    //end change by calvin 21 juni 2018
                    $('#btn-simpan-perubahan-row').hide();
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }

