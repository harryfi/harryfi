        function getTanggal() {
            if ($modeEdit == 1) { // mode edit
                $('#TGL').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).change(function () {
                    if ($(this).val() == '') {
                        $('#Invoice_TGJT').val('Pilih Tanggal dahulu');
                        return;
                    }

                    var $tgl = moment($(this).val(), "DD/MM/YYYY");
                    $('#Invoice_TGL').val($(this).val());
                    //add by nurul 31/10/2018
                    $(this).datepicker('hide');
                    //end add

                    if ($('#Invoice_TERM').val() == '') {
                        $('#Invoice_TGJT').val('Masukkan TOP dahulu');
                    } else {
                        var $tglTop = $tgl.add($('#Invoice_TERM').val(), 'days');
                        $('#Invoice_TGJT').val($tglTop.format('DD/MM/YYYY'));
                    }

                    $('#btn-simpan-perubahan-row').show();
                });
            } else {
                $('#TGL').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).datepicker('setDate', '0').change(function () {
                    if ($(this).val() == '') {
                        $('#Invoice_TGJT').val('Pilih Tanggal dahulu');
                        return;
                    }

                    var $tgl = moment($(this).val(), "DD/MM/YYYY");
                    $('#Invoice_TGL').val($(this).val());
                    //add by nurul 31/10/2018
                    $(this).datepicker('hide');
