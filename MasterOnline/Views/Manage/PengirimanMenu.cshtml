@using System.Globalization
@using MasterOnline.ViewModels
@model PengirimanViewModel
@{
    ViewBag.Title = "Pengiriman";
    Layout = "~/Views/Shared/_LayoutManage.cshtml";

    ErasoftContext erasoftContext = null;
    var context = new MoDbContext();

    var dataSession = Session["SessionInfo"] as AccountUserViewModel;

    if (dataSession?.Account != null)
    {
        erasoftContext = new ErasoftContext(dataSession.Account.DatabasePathErasoft);
    }
    else
    {
        var accFromUser = context.Account.Single(a => a.AccountId == dataSession.User.AccountId);
        erasoftContext = new ErasoftContext(accFromUser.DatabasePathErasoft);

    }
}
@section styles
{
    <link href="~/Content/build/css/selectivity-jquery.min.css" rel="stylesheet" />
    <link href="~/Content/build/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/build/css/bootstrap-timepicker.min.css" />
    <link type="text/css" href="~/Content/build/css/bootstrap-timepicker.css" />
    <link href="~/Content/selectize.css" rel="stylesheet" />
    <style>
        .input-group {
            margin-bottom: 0;
        }

        .btn-success[disabled]:hover {
            background: #6ad6bf;
            border-color: #6ad6bf;
        }

        table#table_tambah_kirim thead tr {
            background-color: #c7f1f5;
        }

        .qty-input {
            width: 50px;
        }

        .harga-input {
            width: 100px;
        }

        .disc-input {
            width: 50px;
        }

        .ndisc-input {
            width: 100px;
        }

        #GUDANG,
        #GUDANG .selectivity-single-select-input {
            width: 100px !important;
        }

        #kirim-section > div {
            background-color: #fff;
            padding: 20px;
        }

        #cetakfaktur_label {
            color: orange;
            font-weight: bold;
        }
    </style>
}
<div class="row" id="kirim-section">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="row kirim_table_section">
            <div class="col-lg-4 col-sm-6">
                <div class="input-group">
                    <input id="search_kirim" type="text" class="form-control" placeholder="Pencarian">
                    <span class="input-group-btn">
                        <button type="button" id="search_kirim_click" class="btn btn-primary">
                            <i class="fa fa-search"></i>
                        </button>
                    </span>
                </div>
            </div>
            <div class="col-lg-8 col-sm-6">
                <div class="pull-right">
                    <button class="btn btn-primary btn_tambah_data" id="tambah_kirim">Tambah Baru</button>
                </div>
            </div>
        </div>
        <div class="row kirim_table_section" style="margin-top: 5px;">
            <div class="col-sm-12" id="table-kirim-partial">
                @*@Html.Partial("TableStokPartial")*@
            </div>
        </div>
        <div class="row kirim_editor_section" style="display: none;">
            <div class="col-lg-12">
                <div class="page-editor">
                    <h2 class="editor-title">Pengiriman</h2>
                    <span class="title-accent"></span>
                    <button id="close-editor" type="button" class="pull-right page-close">
                        <span class="close-btn thick"></span>
                    </button>
                    @if (ViewData.ModelState.Any(x => x.Value.Errors.Any()))
                    {
                        foreach (var modelError in ViewData.ModelState.SelectMany(x => x.Value.Errors))
                        {
                            <div class="alert alert-danger">
                                <span class="message-error">@modelError.ErrorMessage</span>
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                        }
                    }
                    @using (Html.BeginForm("SaveKirim", "Manage", FormMethod.Post, new { enctype = "multipart/form-data", id = "form-kirim" }))
                    {
                        <div id="form-partial">
                            @*@Html.Partial("BarangStokPartial")*@
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="konfHapusKirim" tabindex="-1" role="dialog" aria-labelledby="konfirmasiHapusLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="konfirmasiHapusLabel">Konfirmasi</h4>
            </div>
            <div class="modal-body">
                <div class="row text-center">
                    <h4>Apakah Anda yakin ingin menghapus pengiriman ini?</h4>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Tidak</button>
                <button type="button" class="btn btn-success" onclick="deleteKirim();">Ya</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="konfHapusDetail" tabindex="-1" role="dialog" aria-labelledby="konfirmasiHapusLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="konfirmasiHapusLabel">Konfirmasi</h4>
            </div>
            <div class="modal-body">
                <div class="row text-center">
                    <h4>Apakah Anda yakin ingin menghapus pesanan ini?</h4>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Tidak</button>
                <button type="button" class="btn btn-success" onclick="deleteDetail();">Ya</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalTutupEditor" tabindex="-1" role="dialog" aria-labelledby="labelModalTutupEditor">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="labelModalTutupEditor">Konfirmasi</h4>
            </div>
            <div class="modal-body">
                <div class="row text-center">
                    <h4>Ada perubahan data yang belum Anda simpan. <br />Yakin ingin menutup form ini?</h4>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Tidak</button>
                <button type="button" class="btn btn-success" onclick="closeEditor()">Ya</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalPrintOption" tabindex="-1" role="dialog" aria-labelledby="printOptionLabel">
    <div class="modal-dialog" id="modal-print-option" role="document">
        @*<div class="modal-content cetakKirim" style="margin:auto;height:469px;">*@
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="printOptionLabel">Cetak Pengiriman</h4>
            </div>
            <div class="row text-center">
                <h4>Apakah Anda yakin ingin mencetak pengiriman dengan No. Bukti <span id="cetakfaktur_label"></span>?</h4>
            </div>
            <div class="modal-header">
                <div class="col-md-2">
                </div>
                <div class="col-md-8" style="padding-top:10px">
                    <button id="btn-print-kirim" type="button" class="btn btn-success btn-block">Cetak</button>
                </div>
                <div class="col-md-2">
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script src="~/Content/build/js/selectivity-jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/build/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="~/Content/vendors/moment/min/moment.min.js" type="text/javascript"></script>
    <script src="~/Content/selectize.js" type="text/javascript"></script>
    <script src='@Url.Content("~/Scripts/jquery.validate.js")' type='text/javascript'></script>
    <script src='@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")' type='text/javascript'></script>
    <script type="text/javascript" src="~/Content/build/js/bootstrap-timepicker.js"></script>
    <script type="text/javascript" src="~/Content/build/js/bootstrap-timepicker.min.js"></script>
    <script type="text/javascript">
        var $flagPerubahan = 0;
        var $modeEdit = 0; // 0 new, 1 edit
        var $page = window.text;

        $(document).ready(function () {
            refreshTableKirim();
            $page = 0;
            var tableSiapProses = $('#datatable_siapproses').DataTable({
                columnDefs: [{
                    orderable: false,
                    className: 'select-checkbox',
                    targets: 0
                }],
                select: {
                    style: 'os',
                    selector: 'td:first-child'
                },
            });

            $('#search_siapproses').keyup(function () {
                tableSiapProses.search($(this).val()).draw();
            });


            $('#tambah_kirim').on('click', function () {
                refresh();
            });

            $('#close-editor').off('click').on('click', function () {
                if ($('#btn-simpan-perubahan-row').is(':visible')) {
                    $('#modalTutupEditor').modal('show');
                } else {
                    $modeEdit = 0;
                    $('input, select, .btn-danger').each(function () {
                        $(this).removeAttr('disabled');
                    });
                    $('.kirim_table_section').show();
                    $('.kirim_editor_section').hide();
                    $('#form-partial').html('');
                    refreshTableKirim();
                }



            });



            $('#search_kirim').keypress(function (e) {
                var key = e.which;
                if (key == 13)// the enter key code
                {
                    searchTableKirim();
                    return false;
                }
            });
            $('#search_kirim_click').click(function () {
                searchTableKirim();
            });
        });

        function closeEditor() {
            $('#modalTutupEditor').modal('hide');
            $modeEdit = 0;
            $('input, select, .btn-danger').each(function () {
                $(this).removeAttr('disabled');
            });
            $('.kirim_table_section').show();
            $('.kirim_editor_section').hide();
            $('#form-partial').html('');
            refreshTableKirim();
        }

        function Initialize() {

            if ($('.message-error').text() !== '') {
                $('.kirim_table_section').hide();
                $('.kirim_editor_section').show();
            }

            $('#table_tambah_kirim > tbody > tr > td.editable-kode').prop('contentEditable', true);
            $flagPerubahan = 0;

            //$('#Pengiriman_JAM_KIRIM').datetimepicker();
            $('#timepicker1').timepicker({
                minuteStep: 1,
                showSeconds: false,
                showMeridian: false,
                defaultTime: 'current',
                showWidget: true,
                showInputs: true,
            });

            if ($modeEdit == 1) { // mode edit
                $('#Pengiriman_TGL_KIRIM').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();

                    }
                    $(this).datepicker('hide');

                    $flagPerubahan++;
                    });
                $('#timepicker1').timepicker({
                    minuteStep: 1,
                    showSeconds: false,
                    showMeridian: false,
                    defaultTime: 'current',
                    showWidget: true,
                    showInputs: true,
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();

                    }
                    $(this).timepicker('hide');

                    $flagPerubahan++;
                });
            }
            else {
                $('#Pengiriman_TGL_KIRIM').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).datepicker('setDate', '0').change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();
                    }
                    $(this).datepicker('hide');

                    $flagPerubahan++;
                    });
                $('#timepicker1').timepicker({
                    minuteStep: 1,
                    showSeconds: false,
                    showMeridian: false,
                    defaultTime: 'current',
                    showWidget: true,
                    showInputs: true,
                }).change(function () {
                    if ($flagPerubahan == 0) {
                        $('#btn-simpan-perubahan-row').show();

                    }
                    $(this).timepicker('hide');

                    $flagPerubahan++;
                });
            }

            $("input.num-only").on('keydown',
                function (e) {
                    // Allow: backspace, delete, tab, escape, enter and .
                    if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                        // Allow: Ctrl+A, Command+A
                        (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                        // Allow: home, end, left, right, down, up
                        (e.keyCode >= 35 && e.keyCode <= 40)) {
                        // let it happen, don't do anything
                        return;
                    }
                    // Ensure that it is a number and stop the keypress
                    if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                        e.preventDefault();
                    }
                });

            $('#btn-simpan-perubahan').click(function () {
                var $this = $(this);
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("UpdateKirim", "Manage")',
                    data: JSON.stringify({
                        NoBuktiKirim: $('#Pengiriman_NO_BUKTI').val(),
                        TglKirim: $('#Pengiriman_TGL_KIRIM').val(),
                        Kurir: $('#Pengiriman_KURIR').val(),
                        NamaKurir: $('#Pengiriman_NAMA_KURIR').val(),
                        jamKirim: $('#Pengiriman_TGL_KIRIM').val() + ' ' + $('#timepicker1').val()
                    }),
                    contentType: 'application/json; charset=UTF-8',
                    cache: false,
                    success: function () {
                        $('#close-editor')[0].disabled = false;
                        alert('Data pengiriman berhasil di-update');
                        $('#btn-simpan-perubahan-row').hide();
                    },
                    error: function (xhr) {
                        console.log(xhr);
                    }
                });
            });

        }
        
        function getDataPesanan() {
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetDataPesanan", "Manage")',
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listKodePesanan = [];

                    $.each(data,
                        function (i, item) {

                            listKodePesanan[i] = {
                                id: item.PESANAN,
                                text: item.PESANAN
                            };
                        });

                    $('#PESANAN').selectivity({
                        allowClear: true,
                        items: listKodePesanan,
                        placeholder: 'Harap pilih'
                    }).change(function () {
                        var val = $(this).selectivity('value');
                        var $trId = $(this).closest('tr').attr('data-kirim-id');
                        var $trElement = 'tr[data-kirim-id="' + $trId + '"]';

                        if (val != null) {
                            $.each(data,
                                function (i, item) {
                                    if (item.PESANAN == val) {
                                        $($trElement + ' .mp-detail ').html(item.NAMA_MARKET + '<input data-val="true" data-val-length="The field NAMA Marketplace must be a string with a maximum length of 10." data-val-length-max="10" id="PengirimanDetail_NAMA_MARKET" name="PengirimanDetail.NAMA_MARKET" type="hidden" value="">');
                                        $($trElement + ' .buyer-detail').html(item.NAMA_PEMBELI + '<input data-val="true" data-val-length="The field NAMA Pembeli must be a string with a maximum length of 10." data-val-length-max="10" id="PengirimanDetail_NAMA_PEMBELI" name="PengirimanDetail.NAMA_PEMBELI" type="hidden" value="">');
                                        $($trElement + ' .al-detail').html(item.ALAMAT + '<input data-val="true" data-val-length="The field Alamat KIRIM must be a string with a maximum length of 250." data-val-length-max="250" id="PengirimanDetail_ALAMAT_KIRIM" name="PengirimanDetail.ALAMAT_KIRIM" type="hidden" value="">');

                                        $('#PengirimanDetail_MARKETPLACE').val(item.MARKETPLACE);
                                        $('#PengirimanDetail_PEMBELI').val(item.PEMBELI);
                                        $('#PengirimanDetail_KOTA').val(item.KOTA);
                                        $('#PengirimanDetail_PROPINSI').val(item.PROPINSI);
                                        $('#PengirimanDetail_KODE_POS').val(item.KODE_POS);

                                        return;
                                    }
                                });

                            $($trElement + ' .btn-primary').removeAttr('disabled');
                            offTab();
                        } else {
                            $($trElement + ' .mp-detail').text('-');
                            $($trElement + ' .buyer-detail').text('-');
                            $($trElement + ' .al-detail').text('-');

                            $($trElement + ' .btn-primary').attr('disabled', 'disabled');
                            onTab();
                        }

                    });
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }

        function offTab() {
            $(document).keydown(function (objEvent) {
                if (objEvent.keyCode == 9) {
                    objEvent.preventDefault();
                }
            });
        }

        function onTab() {
            $(document).keydown(function (objEvent) {
                if (objEvent.keyCode == 9) { }
            });
        }
        
        var $idKirim = "";

        function undeleteable() {
            alert('Transaksi Sudah Posting! \nTidak dapat dihapus!');
        }

        function editKirim(idKirim) {
            var link = '@Url.Action("EditKirim", "Manage", new { kirimId = "replaceId" })';
            link = link.replace("replaceId", encodeURIComponent(idKirim));

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#loading_spinner').show();
                },
                success: function (response) {
                    $modeEdit = 1;

                    $('.kirim_table_section').hide();
                    $('.kirim_editor_section').show();
                    $('#close-editor')[0].disabled = false;

                    $('#form-partial').html(response);
                    getDataPesanan();
                    getExpedisi();
                    $('#loading_spinner').hide();
                },
                error: function (xhr, status, error) {
                    console.log(error);
                    $('#loading_spinner').hide();
                }
            });
        }

        function refreshTableKirim() {
            var $link = '@Html.Raw(Url.Action("RefreshTableKirim", "Manage", new { page = "replaceLastPage", search = "replaceSearch" }))';
            $link = $link.replace("replaceSearch", encodeURIComponent($('#search_kirim').val()));
            $link = $link.replace("replaceLastPage", encodeURIComponent($('#txt_last_page').val()));

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#table-kirim-partial').hide();
                    $('#loading_kirim_tab').show();
                },
                success: function (response) {
                    $('#table-kirim-partial').html(response);
                    $('#loading_kirim_tab').hide();
                    $('#table-kirim-partial').show();

                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }
        function searchTableKirim(){
            var $link = '@Html.Raw(Url.Action("RefreshTableKirim", "Manage", new { search = "replaceSearch" }))';
            $link = $link.replace("replaceSearch", encodeURIComponent($('#search_kirim').val()));

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#search_kirim_click')[0].disabled = true;
                    $('#loading_kirim_tab').show();
                },
                success: function (response) {
                    $('#search_kirim_click')[0].disabled = false;
                    $('#table-kirim-partial').html(response);
                    $('#loading_kirim_tab').hide();
                },
                error: function (xhr, status, error) {
                    $('#search_kirim_click')[0].disabled = false;
                    console.log(error);
                }
            });
        }

        function refresh() {
            var link = '@Url.Action("RefreshKirimForm", "Manage")';

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#loading_spinner').show();
                },
                success: function (response) {
                    $('.kirim_table_section').hide();
                    $('.kirim_editor_section').show();
                    $('#form-partial').html(response);
                    getDataPesanan();
                    getExpedisi();
                    $('#loading_spinner').hide();
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }

        function pass(idKirim) {
            this.$idKirim = idKirim;
        }

        function deleteKirim() {
            var link = '@Url.Action("DeleteKirim", "Manage", new { kirimId = "replaceId" })';
            link = link.replace("replaceId", encodeURIComponent(this.$idKirim));

            $.ajax({
                type: "GET",
                url: link,
                success: function (response) {
                    if (response.Errors == null) {
                        refreshTableKirim();
                        $('#konfHapusKirim').modal('hide');
                    } else {
                        alert(response.Errors[0]);
                    }
                    $('#konfHapusKirim').modal('hide');
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }

        var $brg = '';

        function passBarangInDb($brg) {
            this.$brg = $brg;
        }

        function deleteDetail() {
            var link = '@Url.Action("DeleteDetailKirim", "Manage", new { noUrut = "replaceBrgId" })';
            link = link.replace("replaceBrgId", encodeURIComponent(this.$brg));

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (response) {
                    if (response.Errors == null) {
                        $('#form-partial').html(response);
                        getDataPesanan();
                        getExpedisi();
                    } else {
                        alert(response.Errors[0]);
                    }
                    $('#konfHapusDetail').modal('hide');
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }
        
        function simpandetail($id) {
            if (!validateForm()) return false;
            if ($id == 'detail-0') {
                if ($('#Pengiriman_TGL_KIRIM').val() != null &&
                    $('#Pengiriman_KURIR').val() != '' &&
                    $('#Pengiriman_NAMA_KURIR').val() != '' &&
                    $('#timepicker1').val() != ''
                ) {
                    $('#PengirimanDetail_PESANAN').val($('#PESANAN').selectivity('val'));
                    $('#PengirimanDetail_NAMA_MARKET').val($('tr[data-kirim-id="0"] .mp-detail').text());
                    $('#PengirimanDetail_NAMA_PEMBELI').val($('tr[data-kirim-id="0"] .buyer-detail').text());
                    $('#PengirimanDetail_ALAMAT_KIRIM').val($('tr[data-kirim-id="0"] .al-detail').text());

                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("SaveKirim", "Manage")',
                        data: $('#form-kirim').serialize(),
                        beforeSend: function () {
                            $('#loading_spinner').show();
                        },
                        success: function (response) {
                            if (response.Errors == null) {
                                $('#close-editor')[0].disabled = false;
                                $modeEdit = 1;
                                $('#form-partial').html(response);
                                getDataPesanan();
                                getExpedisi();
                                getTanggal();
                                $('#loading_spinner').hide();
                            } else {
                                alert('Ada data yang tidak valid!');
                                console.log(response.Errors);
                                $('#loading_spinner').hide();
                            }
                        },
                        error: function (xhr) {
                            console.log(xhr);
                        }
                    });
                } else {
                    alert('Silakan isi semua field terlebih dahulu!');
                }
            }
        }

        function getTanggal() {
            var $flagPerubahan = 0;


            if ($modeEdit == 1) { // mode edit
                $('#Pengiriman_TGL_KIRIM').datepicker({
                    format: 'dd/mm/yyyy',
                    language: 'id'
                }).change(function () {
                    if ($flagPerubahan == 0) {
