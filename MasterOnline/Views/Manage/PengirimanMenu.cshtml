        function getExpedisi() {
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetEkspedisi", "Manage")',
                contentType: 'application/json',
                cache: false,
                success: function(data) {
                    var listEkspedisi = [];

                    $.each(data,
                        function(i, item) {
                            listEkspedisi[i] = {
                                id: item.RecNum,
                                text: item.NamaEkspedisi
                            };
                        });

                    var expedisiSelect = $('#EXPEDISI').selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: listEkspedisi,
                        onChange: function (value) {
                            var $objSup = $.grep(listEkspedisi, function (data) { return data.id == value });

                            if (value == null) {
                                $("#Pengiriman_KURIR").val(null);
                            } else {
                                $("#Pengiriman_KURIR").val(value).trigger('change');
                            }
                            $('#btn-simpan-perubahan-row').show();
                        }
                    });

                    expedisiSelect[0].selectize.setValue($('#Pengiriman_KURIR').val());
                    $('#btn-simpan-perubahan-row').hide();
                },
                error: function(xhr) {
                    console.log(xhr);
                }
            });
        }

        function Select() {
            $('#PESANAN').style.width = "400px";
        }

        function cetakKirim(kirimId) {
            $('#cetakfaktur_label').text($('.cetak').attr("data-nobuk"));
            $('#modalPrintOption').modal('show');

            $('#btn-print-kirim').off('click').on('click', function() {

                var link = '@Url.Action("CetakPengiriman", "Manage", new { KirimId = "replaceNoBuk"})';
                link = link.replace('replaceNoBuk', kirimId);

                var win = encodeURIComponent(window.open(link , '_blank'));
                $('#modalPrintOption').modal('hide');
                if (win) {
                    win.focus();
                } else {
                    alert('Please allow popups for this website');
                }
            });
        }

