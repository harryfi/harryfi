        function GetDataBrgWithRecord(page, recordCount, statBL, totalData) {
            if ($('#promptHiddenField').val() == null || $('#promptHiddenField').val() == "" ) {
                alert('Anda belum memilih Toko');
            } else {
                @*var link = '@Url.Action("ImportDataMP", "Manage", new { cust = "replaceCust"})';
                link = link.replace("replaceCust", $('#promptHiddenField').val());*@
                 var link = '@Url.Action("ImportDataMP", "Manage")';
                link += '?cust=' + encodeURIComponent($('#promptHiddenField').val()) + '&page=' + encodeURIComponent(page) + '&recordCount=' + encodeURIComponent(recordCount) + '&statBL=' + encodeURIComponent(statBL) + '&totalData=' + encodeURIComponent(totalData);
                $.ajax({
                    type: "GET",
                    url: link,
                    timeout: 0,                    
                    beforeSend: function () {
                        //$('#datatable').hide();
                        $('#table-upload-barang-partial').hide();
                        $('#lblPleasewait').show();
                        $('#loading_barang_tab').show();
                        //$('#loadingGetBarang').modal({ backdrop: 'static', keyboard: false });
                        $('#info_record').show();
                    },
                    success: function (response) {
                        lastPage = page;
                        if (response.Errors == null || response.Errors.length == 0) {
                            if (response.RecordCount != null) {
                                //document.getElementById('txtInfo').innerText = 'Jumlah data berhasil tersimpan ke Master Online : ' + response.RecordCount + '\nJumlah data yang didapat dari marketplace : ' + response.totalData;
                                document.getElementById('txtInfo').innerText = 'Jumlah data berhasil tersimpan ke Master Online : ' + response.RecordCount;
                                document.getElementById('txtInfo2').innerText = 'Jumlah data yang didapat dari marketplace : ' + response.totalData;

                            }
                            if (response.exception == 1)
                                exGetData = 1;
                            if (response.Recursive) {
                                if (response.BLProductActive == 1) {
                                    GetDataBrgWithRecord(response.Page, response.RecordCount, 1, response.totalData);
                                } else {
                                    GetDataBrgWithRecord(response.Page, response.RecordCount, 0, response.totalData);
                                }
                            } else {
                                //document.getElementById('txtInfo').innerText = '';
                                //$('#loadingGetBarang').modal('hide');
                                //document.getElementById('lblModalDataSaved').innerText = '';
                                //document.getElementById('lblModalDataSaved').innerText = '';
                                var msg = "Jumlah data berhasil tersimpan :" + response.RecordCount + '\nJumlah data yang didapat dari marketplace : ' + response.totalData;
                                if (exGetData == 1) {
                                    msg += '\nAda data barang yang tidak masuk ke MO. Silahkan lakukan pengambilan data lagi. \nJika anda masih mendapat pesan error ini setelah mencoba lagi, silahkan hubungi tim support MO.';
                                }
                                alert(msg);
                                //$('#table-upload-barang-partial').html(response);
                                //var table = $('#datatable').DataTable();
                                //$('#search_barang').keyup(function () {
                                //    table.search($(this).val()).draw();
                                //});
                                refreshTableBarangUpload();
                                //$('#loading_barang_tab').hide();
                                $('#lblPleasewait').hide();
                                $('#info_record').hide();
                                document.getElementById('txtInfo').innerText = 'Jumlah data berhasil tersimpan ke Master Online : 0';
                                document.getElementById('txtInfo2').innerText = 'Jumlah data yang didapat dari marketplace : 0';
                                //document.getElementById('lblModalDataSaved').innerText = 'Jumlah data berhasil tersimpan ke Master Online : 0';
                                //document.getElementById('lblModalDataChecked').innerText = 'Jumlah data yang didapat dari marketplace : 0';
                                //$('#datatable').show();
                            }
                        } else {
                            alert(response.Errors);
                            $('#lblPleasewait').hide();
                            //$('#loading_barang_tab').hide();
                            //$('#datatable').show();
                            $('#loadingGetBarang').modal('hide');
                            $('#table-upload-barang-partial').show();
                            $('#info_record').hide();
                        }


                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                        //var err = JSON.parse(xhr.responseText);
                        //alert(err.Message);
                        //console.log(err.error);
                        if (xhr.status == 524) {
                            if (lastPage == page) {
                                loop = loop + 1;
                            } else {
                                loop = 1;
                            }
                            if (loop <= 3) {
                                GetDataBrgWithRecord(page, recordCount, statBL, totalData);
                            } else {
                                alert('Terdapat Error saat mengambil data barang marketplace ini.\nSilahkan hubungi tim support MO');
                                refreshTableBarangUpload();
                                $('#lblPleasewait').hide();
                                $('#info_record').hide();
                            }
                        }
                    }
                });
                $('#konfTarikData').modal('hide');
                $('#konfSyncAll').modal('hide');
            }

        }
