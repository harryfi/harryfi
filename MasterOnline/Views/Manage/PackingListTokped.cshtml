    function RequestPickupPackingTokped(cust) {
        $('#btnProsesTokped_' + cust).attr("disabled", true);
        $('#modalPrintTokped__label__' + cust).html('Proses Pickup Pesanan Tokopedia');
        $('#modalPrintTokped__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintTokped__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var $link = '@Url.Action("RequestPickupTokpedPerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_tokped').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });

        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };
        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();
                if (!response.mo_error) {
                    var textareaValue = "";

                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintTokped__textarea__' + cust).show();
                    $('#modalPrintTokped__textarea__' + cust).html(textareaValue);

                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintTokped__textarea__' + cust).show();
                    $('#modalPrintTokped__textarea__' + cust).html(response.mo_error);
                }
            },
            complete: function () {
                $('#btnProsesTokped_' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

