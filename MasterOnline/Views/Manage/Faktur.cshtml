        function refreshInputan() {
            //add by nurul 19/12/2018 ngatur width barang
            $(window).on('resize', function () {
                //var win = $(this); //this = window
                var style = $('.selectivity-dropdown').attr('style');
                if (style !== undefined && style !== null) {
                    $('.selectivity-dropdown').attr('style', style.replace('width: 100px', 'width: 400px; min-width: 400px !important'));
                }
            });

            $(window).scroll(function () {
                //var scrollTop = $(window).scrollTop();
                var style = $('.selectivity-dropdown').attr('style');
                if (style !== undefined && style !== null) {
                    $('.selectivity-dropdown').attr('style', style.replace('width: 100px', 'width: 400px; min-width: 400px !important'));
                }
            });

            $('#BRG').click(function () {
                var checkExist = setInterval(function () {
                    //if ($('.selectivity-dropdown').length) {
                    //    $('html, body').animate({ scrollTop: '-=10px' }, 500);
                    //    clearInterval(checkExist);
                    //}
                    if (window.pageYOffset == 0) {
                        $('html, body').animate({ scrollTop: '+=1px' }, 500);
                        clearInterval(checkExist);
                    } else {
                        $('html, body').animate({ scrollTop: '-=1px' }, 500);
                        clearInterval(checkExist);
                    }
                }, 100);
            });

            $('body').click(function () {
                $('#BRG').css('max-width', '100px').attr('style', 'width: 100px !important');
            });
            //end add

            if ($('#Faktur_TGL').val() != '') {
                $('#Faktur_TGL').val($('#Faktur_TGL').val().split(' ')[0]);
                $('#TGL').val($('#Faktur_TGL').val());
            }

            $('#Faktur_NILAI_DISC').focus(function () {
                $(this).val(convertToAngka($(this).val()));
            });

            $('#Faktur_NILAI_DISC').blur(function () {
                if ($(this).val() == '') {
                    $(this).val(0);
                }
                var $tot = convertToAngka($('#Faktur_BRUTO').val());
                if ($(this).val() > $tot) {
                    alert('Diskon tidak boleh melebihi total bruto sebesar ' + convertToRupiah($tot) + '!');
                } else {
                    if ($(this).val() < 0 || $(this).val() === '') { $(this).val(convertToRupiah(0)); }
                    refreshNetto();
                }
            });

            $('#Faktur_NILAI_DISC').change(function () {
                if ($(this).val() == '') {
                    $(this).val(0);
                }
                if ($(this).val() <= convertToAngka($('#Faktur_BRUTO').val())) {
