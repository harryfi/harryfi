                                    $('#table_tambah_faktur > tbody > tr[data-faktur-id="0"] > td.disc2-barang').click();
                                }

                                e.preventDefault();
                            }
                        } else if ($(this).hasClass('disc-input') && $(this).parent().hasClass('disc2-barang')) {
                            numOnlyAllowDecimalOnKeyDown(e);
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == 0) {
                                    $('#table_tambah_faktur > tbody > tr[data-faktur-id="0"] > td.ndisc2-barang').click().click();
                                } else {
                                    $(this).blur();
                                }

                                e.preventDefault();
                            }
                        } else if ($(this).hasClass('ndisc-input') && $(this).parent().hasClass('ndisc1-barang')) {
                            numOnlyOnKeyDown(e);
                            if (keyCode === 190) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                if ($(this).val() == '0,00') {
                                    $('#table_tambah_faktur > tbody > tr[data-faktur-id="0"] > td.disc2-barang').click();
                                } else {
                                    $('#table_tambah_faktur > tbody > tr[data-faktur-id="0"] > td.ndisc2-barang').click();
                                }

                                e.preventDefault();
                            }
                        } else {
                            numOnlyOnKeyDown(e);
                            if (keyCode === 190) e.preventDefault();
                            if (keyCode === 13 || keyCode === 9) {
                                $(this).blur();
                            }
                        }
                    },
                    change: function () {
                        if ($(this).hasClass('qty-input')) {
                            if ($(this).val() > 999) {
                                $(this).val(999);
                            }
                        }

                        if ($(this).hasClass('disc-input')) {
                            if ($(this).val() > 99) {
                                $(this).val(99);
                            }
                        }
                    }
                }).appendTo($this.empty()).focus();
            });
        }

