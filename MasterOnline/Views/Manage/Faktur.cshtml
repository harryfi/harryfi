                                "DiskonKartuKredit","OngkosKirimDibayarolehPembeli","TotalPembayarandibayarpembeli","PerkiraanOngkosKirim","CatatandariPembeli",
                                "Catatan","UsernamePembeli","NamaPenerima","NoTelepon","AlamatPengiriman",
                                "KotaKabupaten","Provinsi","WaktuPesananSelesai"
                                ],raw:true});

                            link = '@Url.Action("UploadFakturShopee", "Manage")';
                        }
                        //console.log(
                        //JSON.stringify({
                        //    data : jsondata,
                        //    cust: $('#CUSTOMER_UPLOAD_FAKTUR_CUST').val(),
                        //    perso: $('#CUSTOMER_UPLOAD_FAKTUR_PERSO').val()
                        //}));
                        $.ajax({
