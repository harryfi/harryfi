            else if ($('#file_upload_faktur')[0].files.length < 1) {
                $('#file_upload_faktur').click();
            }
            else {
                var files = $('#file_upload_faktur')[0].files;
                var i,f;
                for (i = 0, f = files[i]; i != files.length; ++i) {
                    var reader = new FileReader();
                    var name = f.name;
                    reader.onload = function(e) {
                        var data = e.target.result;
                        //var cust = $('#CUSTOMER_UPLOAD_FAKTUR_CUST').val();
                        var workbook = XLSX.read(data, {type: 'binary'});

                        var worksheet = workbook.Sheets[workbook.SheetNames[0]];
                        var jsondata = '';
                        var link = '';
                        if ($('#CUSTOMER_UPLOAD_FAKTUR_NAMA').val() == '15') {

                            var range = XLSX.utils.decode_range(worksheet['!ref']);
                            range.s.r = 4; // <-- zero-indexed, so setting to 4 will skip row 0,1,2,4
