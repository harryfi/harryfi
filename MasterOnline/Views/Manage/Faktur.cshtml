        function refreshTableFakturLunas() {
            var link = '@Url.Action("RefreshTableFakturLunas", "Manage")';

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#loading_spinner').show();
                },
                success: function (response) {
                    $('#table-faktur-partial').html(response);
                    var table = $('#datatable').DataTable({
                        "order": [[ 0,1, "desc" ]], //or asc
                        "columnDefs" : [{"targets":1, "type":"date-eu"}],
