        function refreshNettoBarang($trId) {
            var $trElement = 'tr[data-faktur-id="' + $trId + '"]';
            var $hargaBarang = convertToAngka($($trElement + ' .harga-barang').text());
            var $ndisc1 = convertToAngka($($trElement + ' .ndisc1-barang').text());
            var $ndisc2 = convertToAngka($($trElement + ' .ndisc2-barang').text());
            var $totDisc = $ndisc1 + $ndisc2;
            var $total = ($($trElement + ' .qty-barang').text() * $hargaBarang) - $totDisc;

            $($trElement + ' .netto-barang').html(convertToRupiah($total) + '<input data-val="true" data-val-number="The field HARGA must be a number." data-val-required="The HARGA field is required." id="FakturDetail_HARGA" name="FakturDetail.HARGA" type="hidden" value="">');
            $('#Faktur_BRUTO').val(convertToRupiah($total + $totalAwal + $nettoSebelumnya));

            refreshNetto();
        }

        function refreshNetto() {
            $bakalNetto = convertToAngka($('#Faktur_BRUTO').val());

            var $nPpn = Math.ceil($bakalNetto * $('#Faktur_PPN').val() / 100);
            $('#Faktur_NILAI_PPN').val(convertToRupiah($nPpn));

            $bakalNetto -= convertToAngka($('#Faktur_NILAI_DISC').val());
            $bakalNetto += $nPpn;
            $bakalNetto += convertToAngka($('#Faktur_MATERAI').val());

            $('#Faktur_NETTO').val(convertToRupiah($bakalNetto));
        }

