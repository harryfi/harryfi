    //add by nurul 24/7/2020
    function refreshTablePackingListPerAkunBukalapak(cust) {
        //$('#search_table_packing_pesanan_shopee__' + cust).val('');
        //$('#filter_packing_field_shopee__' + cust).val('Harap Pilih').change();
        var $link = '@Html.Raw(Url.Action("refreshTablePackingListPerAkun", "Manage", new { bukti = "replacebukti", cust = "replacecust", marketplace = "replacemarket", search = "replacesearch", filter = "replacefilter", filtervalue = "replacefiltervalue" }))';
        $link = $link.replace("replacebukti", encodeURIComponent($('#packingList_NO_BUKTI').val()));
        $link = $link.replace("replacecust", encodeURIComponent(cust));
        $link = $link.replace("replacemarket", encodeURIComponent("BUKALAPAK"));
        $link = $link.replace("replacesearch", encodeURIComponent($('#search_table_packing_pesanan_bukalapak__' + cust).val()));
        //$link = $link.replace("replacefilter", encodeURIComponent(null));
        //$link = $link.replace("replacefiltervalue", encodeURIComponent(null));
        var filterbybukalapak = get_bukalapak_filterby(cust);
        var filtervaluebukalapak = get_bukalapak_filtervalue(filterbybukalapak, cust);
        $link = $link.replace("replacefilter", encodeURIComponent(filterbybukalapak));
        $link = $link.replace("replacefiltervalue", encodeURIComponent(filtervaluebukalapak));

        $.ajax({
            type: "GET",
            url: $link,
            beforeSend: function () {
                $('#table_packing_bukalapak__' + cust).hide();
                //$('#loading_faktur_tab').show();
            },
            success: function (response) {
                $('#table_packing_bukalapak__' + cust).html(response);
                //$('#loading_faktur_tab').hide();
                $('#table_packing_bukalapak__' + cust).show();

                //add by nurul 23/7/2020
                $('#filter_packing_field_bukalapak__' + cust).change(function () {
                    var filterBy = $(this).val();
                    switch (filterBy) {
                        case "kirim":
                            $('#filter_packing_kirim_field_option_div_bukalapak__' + cust).show();
                            $('#filter_packing_print_field_option_div_bukalapak__' + cust).hide();
                            $('#filter_packing_kirim_bukalapak__' + cust).val('Harap Pilih');
                            break;
                        case "print":
                            $('#filter_packing_kirim_field_option_div_bukalapak__' + cust).hide();
                            $('#filter_packing_print_field_option_div_bukalapak__' + cust).show();
                            $('#filter_packing_print_bukalapak__' + cust).val('Harap Pilih');
                            break;
                        default:
                            $('#filter_packing_kirim_field_option_div_bukalapak__' + cust).hide();
                            $('#filter_packing_print_field_option_div_bukalapak__' + cust).hide();
                            break;
                    }

                });
                $('#filter_packing_kirim_bukalapak__' + cust).change(function () {
                    if ($('#filter_packing_kirim_bukalapak__' + cust).val() != "Harap Pilih" && $('#filter_packing_field_bukalapak__' + cust).val() != "Harap Pilih") {
                        SearchTablePackingListPerAkunBukalapak(cust);
                    }
                });
                $('#filter_packing_print_bukalapak__' + cust).change(function () {
                    if ($('#filter_packing_print_bukalapak__' + cust).val() != "Harap Pilih" && $('#filter_packing_field_bukalapak__' + cust).val() != "Harap Pilih") {
                        SearchTablePackingListPerAkunBukalapak(cust);
                    }
                });

                $('#search_table_packing_pesanan_bukalapak__' + cust).keypress(function (e) {
                    var key = e.which;
                    if (key == 13)// the enter key code
                    {
                        SearchTablePackingListPerAkunBukalapak(cust);
                        return false;
                    }
                });
                $('#search_table_packing_pesanan_bukalapak_button__' + cust).click(function () {
                    SearchTablePackingListPerAkunBukalapak(cust);
                });
                //end add by nurul 23/7/2020
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    function get_bukalapak_filterby(cust) {
        return $('#filter_packing_field_bukalapak__' + cust).val();
    }
    function get_bukalapak_filtervalue(filterbybukalapak, cust) {
        var filtervaluebukalapak = "";
        switch (filterbybukalapak) {
            case "kirim":
                filtervaluebukalapak = $('#filter_packing_kirim_bukalapak__' + cust).val();
                break;
            case "print":
                filtervalue = $('#filter_packing_print_bukalapak__' + cust).val();
                break;
            default:
                break;
        }
        return filtervaluebukalapak;
    }
