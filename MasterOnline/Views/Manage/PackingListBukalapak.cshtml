    function RequestPackingBukalapak(cust, tipe) {
        $('#btnProsesPackingBukalapak__' + cust).attr("disabled", true);

        $('#modalPrintBukalapak__label__' + cust).html('Proses Ambil Resi Pesanan Bukalapak');
        $('#modalPrintBukalapak__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintBukalapak__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var $link = '@Url.Action("RequestPackedBukalapakPerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_bukalapak').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        var serviceType = "drop off";
        if (tipe == "2") {
            serviceType = "pickup";
        }
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            DeliveryProvider : $('#txtShipment__' + cust).val(),
            rows_selected: rows_selected,
            serviceType: serviceType
        };

        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
                $('#loading_spinner').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();

                if (!response.mo_error) {
                    var textareaValue = "";

                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintBukalapak__textarea__' + cust).show();
                    $('#modalPrintBukalapak__textarea__' + cust).html(textareaValue);

                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintBukalapak__textarea__' + cust).show();
                    $('#modalPrintBukalapak__textarea__' + cust).html(response.mo_error);
                }
            },
            complete: function () {
                $('#loading_spinner').hide();
                $('#btnProsesPackingBukalapak__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
