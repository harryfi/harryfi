    function RefreshPackingBukalapak(cust) {
        $('#search_table_packing_pesanan_bukalapak__' + cust).val('');
        $('#filter_packing_field_bukalapak__' + cust).val('Harap Pilih').change();
        var $link = '@Html.Raw(Url.Action("refreshTablePackingListPerAkun", "Manage", new { bukti = "replacebukti", cust = "replacecust", marketplace = "replacemarket", search = "replacesearch", filter = "replacefilter", filtervalue = "replacefiltervalue" }))';
        $link = $link.replace("replacebukti", encodeURIComponent($('#packingList_NO_BUKTI').val()));
        $link = $link.replace("replacecust", encodeURIComponent(cust));
        $link = $link.replace("replacemarket", encodeURIComponent("BUKALAPAK"));
        $link = $link.replace("replacesearch", encodeURIComponent($('#search_table_packing_pesanan_bukalapak__' + cust).val()));
        $link = $link.replace("replacefilter", encodeURIComponent(null));
        $link = $link.replace("replacefiltervalue", encodeURIComponent(null));

        $.ajax({
            type: "GET",
            url: $link,
            beforeSend: function () {
                $('#table_packing_bukalapak__' + cust).hide();
                //$('#loading_faktur_tab').show();
            },
            success: function (response) {
                $('#table_packing_bukalapak__' + cust).html(response);
                //$('#loading_faktur_tab').hide();
                $('#table_packing_bukalapak__' + cust).show();

            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    //end add by nurul 24/7/2020
</script>
