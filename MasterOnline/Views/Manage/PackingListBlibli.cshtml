    function allcheckboxpackingbliblionclick(cust){
        $('.checkbox_packing_blibli').each( function() {
        	if($(this).attr("data-cust") == cust){
        	    $(this).prop('checked', $('#all_checkbox_packing_blibli__' + cust).prop('checked'));
        	}
