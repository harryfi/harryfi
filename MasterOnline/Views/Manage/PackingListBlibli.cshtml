@using System.Globalization
@*@using MasterOnline.ViewModels*@
@using PagedList.Mvc
@model PagedList.IPagedList<PackingPerMP>
@{
    string currentFilter = ViewBag.CurrentFilter;
    string currentSort = ViewBag.CurrentSort;

}
<script type="text/javascript">
    $(document).ready(function () {
        if ($('#PackingListBlibliFound')) {
            if (parseInt($('#PackingListBlibliFound').val()) > 0) {
                $('#li_pesanan_blibli_tab_focus').show().on('click', function () {
                    $('#blibli_packing_cust_tabs li:first').find('a').click();
                });
            }
            else{
                $('#li_pesanan_blibli_tab_focus').hide();
            }
        }
    });
    function allcheckboxpackingbliblionclick(cust){
        $('.checkbox_packing_blibli').each( function() {
        	if($(this).attr("data-cust") == cust){
        	    $(this).prop('checked', $('#all_checkbox_packing_blibli__' + cust).prop('checked'));
        	}
        });
    }
    function RequestResiPackingBlibli(cust) {
        $('#btnProsesGetResi__' + cust).attr("disabled", true);
        $('#modalPrintBlibli__label__' + cust).html('Proses Packing Pesanan Blibli');
        $('#modalPrintBlibli__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintBlibli__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var rows_selected = [];
        $('.checkbox_packing_blibli').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });

        var $link = '@Url.Action("BlibliCreatePackage", "Manage")';

        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };

        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            complete: function () {
                $('#btnProsesGetResi__' + cust).attr("disabled", false);
            },
            success: function (response) {
                if (!response.mo_error) {
                    var textareaValue = "";

                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintBlibli__textarea__' + cust).show();
                    $('#modalPrintBlibli__textarea__' + cust).html(textareaValue);
                } else {
                    $('#modalPrintBlibli__textarea__' + cust).show();
                    $('#modalPrintBlibli__textarea__' + cust).html(response.mo_error);
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    function RequestDropoffPackingBlibli(cust) {
        $('#btnProsesDropoffBlibli__' + cust).attr("disabled", true);
        $('#modalPrintBlibli__label__' + cust).html('Proses Kirim Pesanan Blibli');
        $('#modalPrintBlibli__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintBlibli__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var rows_selected = [];
        $('.checkbox_packing_blibli').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });

        var $link = '@Url.Action("BlibliBatchFulfill", "Manage")';

        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };

        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            complete: function () {
                $('#btnProsesDropoffBlibli__' + cust).attr("disabled", false);
            },
            success: function (response) {
                if (!response.mo_error) {
                    var textareaValue = "";

                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintBlibli__textarea__' + cust).show();
                    $('#modalPrintBlibli__textarea__' + cust).html(textareaValue);
                } else {
                    $('#modalPrintBlibli__textarea__' + cust).show();
                    $('#modalPrintBlibli__textarea__' + cust).html(response.mo_error);
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    function BlibliGetLabelPerPacking(cust) {
        $('#modalPrintBlibli__label__' + cust).html('Print Pesanan Blibli');
        $('#modalPrintBlibli__textarea__' + cust).html('Sedang diproses. Mohon menunggu..');
        $('#modalPrintBlibli__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var rows_selected = [];
        $('.checkbox_packing_blibli').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });

        var $link = '@Url.Action("BlibliLabelPerPacking", "Manage")';

        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };

        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            success: function (response) {

                if (response.mo_error != undefined) {
                    // alert(response.mo_error);
                    $('#modalPrintBlibli__textarea__' + cust).html(response.mo_error);
                }
                else {
                    var textareaValue = "";

                    var adaUrl = false;
                    var urlValue = "";
                    var countUrl = 0;
                    response.listSuccess.forEach(function (item) {
                        adaUrl = true;
                        countUrl++;
                        var buttonHtml = "<a class='btn btn-success' download='MO_Blibli_Shipping_"+ item.no_referensi + '_' + item.orderItemId +".pdf' target='_blank' href='data:application/octet-stream;base64,"+ item.pdf64 +"'>Download PDF File</a></br>";
                        urlValue += buttonHtml;
                    });
                    if (adaUrl) {
                        textareaValue += "Shipping Label PDF :</br>";
                        textareaValue += urlValue;
                    }

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.errorMessage + ". </br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Errors :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintBlibli__textarea__' + cust).html(textareaValue);
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    //add by nurul 24/7/2020
    function refreshTablePackingListPerAkunBlibli(cust) {
        //$('#search_table_packing_pesanan_blibli__' + cust).val('');
        //$('#filter_packing_field_blibli__' + cust).val('Harap Pilih').change();
        var $link = '@Html.Raw(Url.Action("refreshTablePackingListPerAkun", "Manage", new { bukti = "replacebukti", cust = "replacecust", marketplace = "replacemarket", search = "replacesearch", filter = "replacefilter", filtervalue = "replacefiltervalue" }))';
        $link = $link.replace("replacebukti", encodeURIComponent($('#packingList_NO_BUKTI').val()));
        $link = $link.replace("replacecust", encodeURIComponent(cust));
        $link = $link.replace("replacemarket", encodeURIComponent("BLIBLI"));
        $link = $link.replace("replacesearch", encodeURIComponent($('#search_table_packing_pesanan_blibli__' + cust).val()));
        //$link = $link.replace("replacefilter", encodeURIComponent(null));
        //$link = $link.replace("replacefiltervalue", encodeURIComponent(null));
        var filterbyBlibli = get_blibli_filterby(cust);
        var filtervalueBlibli = get_blibli_filtervalue(filterbyBlibli, cust);
        $link = $link.replace("replacefilter", encodeURIComponent(filterbyBlibli));
        $link = $link.replace("replacefiltervalue", encodeURIComponent(filtervalueBlibli));

        $.ajax({
            type: "GET",
            url: $link,
            beforeSend: function () {
                $('#table_packing_Blibli__' + cust).hide();
                //$('#loading_faktur_tab').show();
            },
            success: function (response) {
                $('#table_packing_Blibli__' + cust).html(response);
                //$('#loading_faktur_tab').hide();
                $('#table_packing_Blibli__' + cust).show();

                //add by nurul 23/7/2020
                $('#filter_packing_field_blibli__' + cust).change(function () {
                    var filterBy = $(this).val();
                    switch (filterBy) {
                        case "kirim":
                            $('#filter_packing_kirim_field_option_div_blibli__' + cust).show();
                            $('#filter_packing_print_field_option_div_blibli__' + cust).hide();
                            $('#filter_packing_kirim_blibli__' + cust).val('Harap Pilih');
                            break;
                        case "print":
                            $('#filter_packing_kirim_field_option_div_blibli__' + cust).hide();
                            $('#filter_packing_print_field_option_div_blibli__' + cust).show();
                            $('#filter_packing_print_blibli__' + cust).val('Harap Pilih');
                            break;
                        default:
                            $('#filter_packing_kirim_field_option_div_blibli__' + cust).hide();
                            $('#filter_packing_print_field_option_div_blibli__' + cust).hide();
                            break;
                    }

                });
                $('#filter_packing_kirim_blibli__' + cust).change(function () {
                    if ($('#filter_packing_kirim_blibli__' + cust).val() != "Harap Pilih" && $('#filter_packing_field_blibli').val() != "Harap Pilih") {
                        SearchTablePackingListPerAkunBlibli(cust);
                    }
                });
                $('#filter_packing_print_blibli__' + cust).change(function () {
                    if ($('#filter_packing_print_blibli__' + cust).val() != "Harap Pilih" && $('#filter_packing_field_blibli').val() != "Harap Pilih") {
                        SearchTablePackingListPerAkunBlibli(cust);
                    }
                });

                $('#search_table_packing_pesanan_blibli__' + cust).keypress(function (e) {
                    var key = e.which;
                    if (key == 13)// the enter key code
                    {
                        SearchTablePackingListPerAkunBlibli(cust);
                        return false;
                    }
                });
                $('#search_table_packing_pesanan_blibli_button__' + cust).click(function () {
                    SearchTablePackingListPerAkunBlibli(cust);
                });
                //end add by nurul 23/7/2020
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    function get_blibli_filterby(cust) {
        return $('#filter_packing_field_blibli__' + cust).val();
    }
    function get_blibli_filtervalue(filterbyBlibli, cust) {
        var filtervalueBlibli = "";
        switch (filterbyBlibli) {
            case "kirim":
                filtervalueBlibli = $('#filter_packing_kirim_blibli__' + cust).val();
                break;
            case "print":
                filtervalueBlibli = $('#filter_packing_print_blibli__' + cust).val();
                break;
            default:
                break;
        }
        return filtervalueBlibli;
    }
    function SearchTablePackingListPerAkunBlibli(cust) {
        $custTokped = cust;
        var $link = '@Html.Raw(Url.Action("refreshTablePackingListPerAkun", "Manage", new { bukti = "replacebukti", cust = "replacecust", marketplace = "replacemarket", search = "replacesearch", filter = "replacefilter", filtervalue = "replacefiltervalue" }))';
        $link = $link.replace("replacebukti", encodeURIComponent($('#packingList_NO_BUKTI').val()));
        $link = $link.replace("replacecust", encodeURIComponent(cust));
        $link = $link.replace("replacemarket", encodeURIComponent("BLIBLI"));
        $link = $link.replace("replacesearch", encodeURIComponent($('#search_table_packing_pesanan_blibli__' + cust).val()));
        var filterbyBlibli = get_blibli_filterby(cust);
        var filtervalueBlibli = get_blibli_filtervalue(filterbyBlibli, cust);
        $link = $link.replace("replacefilter", encodeURIComponent(filterbyBlibli));
        $link = $link.replace("replacefiltervalue", encodeURIComponent(filtervalueBlibli));

        $.ajax({
            type: "GET",
            url: $link,
            beforeSend: function () {
                $('#table_packing_Blibli__' + cust).hide();
                //$('#loading_faktur_tab').show();
            },
            success: function (response) {
                $('#table_packing_Blibli__' + cust).html(response);
                //$('#loading_faktur_tab').hide();
                $('#table_packing_Blibli__' + cust).show();

                
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    function RefreshPackingBlibli(cust) {
        $('#search_table_packing_pesanan_blibli__' + cust).val('');
        $('#filter_packing_field_blibli__' + cust).val('Harap Pilih').change();
        var $link = '@Html.Raw(Url.Action("refreshTablePackingListPerAkun", "Manage", new { bukti = "replacebukti", cust = "replacecust", marketplace = "replacemarket", search = "replacesearch", filter = "replacefilter", filtervalue = "replacefiltervalue" }))';
        $link = $link.replace("replacebukti", encodeURIComponent($('#packingList_NO_BUKTI').val()));
        $link = $link.replace("replacecust", encodeURIComponent(cust));
        $link = $link.replace("replacemarket", encodeURIComponent("BLIBLI"));
        $link = $link.replace("replacesearch", encodeURIComponent($('#search_table_packing_pesanan_blibli__' + cust).val()));
        $link = $link.replace("replacefilter", encodeURIComponent(null));
        $link = $link.replace("replacefiltervalue", encodeURIComponent(null));

        $.ajax({
            type: "GET",
            url: $link,
            beforeSend: function () {
                $('#table_packing_Blibli__' + cust).hide();
                //$('#loading_faktur_tab').show();
            },
            success: function (response) {
                $('#table_packing_Blibli__' + cust).html(response);
                //$('#loading_faktur_tab').hide();
                $('#table_packing_Blibli__' + cust).show();

            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    //end add by nurul 24/7/2020
</script>
@{
    var distinctCust = Model.Select(p => new { p.CUST, p.NAMA_CUST }).Distinct().ToList();
}
<div class="row">
    <div class="col-12">
        <ul id="blibli_packing_cust_tabs" class="nav nav-tabs">
            @foreach (var item in distinctCust)
            {
                var listPackingPerAkun = Model.Where(a => a.CUST == item.CUST).ToList();
                var countSOPerCust = Model.Where(a => a.CUST == item.CUST).Count();
                <li><a data-toggle="tab" href="#blibli_tab_content__@(item.CUST)" onclick="refreshTablePackingListPerAkunBlibli('@item.CUST')">@(item.NAMA_CUST)<span>(@countSOPerCust)</span></a></li>
            }
        </ul>
        <div class="tab-content">
            @{
                var PackingListBlibliFound = distinctCust.Count();
            }
            @Html.TextBox("PackingListBlibliFound", PackingListBlibliFound, new { @class = "form-control", disabled = "disabled", @style = "display:none;" })
