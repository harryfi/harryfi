        function startSyncAll() {
            var link = '@Url.Action("GetTotalData", "Manage", new { cust = "replaceId" })';
            link = link.replace("replaceId", encodeURIComponent($('#promptHiddenField').val()));
            link += "&newprocess=1";
            var maxPage = "";
            var dataPerPage = 0;
            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    //$('#loading_spinner').show();
                },
                success: function (response) {
                    if (response.Errors == null) {
                        if (response.Total != null && response.Total != "") {
                            namaFileLog = response.fileName;
                            if (parseInt(response.Total) > 0) {
                                var total = parseInt(response.Total);
                                maxPage = parseInt(total / 50);
                                dataPerPage = 50;
                                lastPage = 0;
                                loop = 1;
                                UploadAllBrgWithProgressBar(0, maxPage, dataPerPage, 0);
                            } else {
                                alert('Tidak ada data barang untuk di update dari Akun ini.\nSilahkan lakukan Ambil data dari Marketplace terlebih dahulu.');
                                    }
                        } else {
                            alert('Tidak ada data barang untuk di update dari Akun ini.\nSilahkan lakukan Ambil data dari Marketplace terlebih dahulu.');
                            }
                    } else {
                        alert(response.Errors);
                        }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }

        function UploadAllBrgWithProgressBar(currentPage, maxPage, dataPerPage, failedToSync) {

            var link = '@Url.Action("ProsesSinkroNewUI", "Manage")';
            link += "?cust=" + encodeURIComponent($('#promptHiddenField').val()) + "&dataPerPage=" + encodeURIComponent(dataPerPage) + "&skipDataError="
                + encodeURIComponent(failedToSync) + '&namaFileLog=' + encodeURIComponent(namaFileLog);

            $.ajax({
                type: "GET",
                url: link,
                success: function (response) {
                    lastPage = currentPage;
                    if (response.Errors != null) {
                        if (response.Errors.length > 0) {
                            response.Errors.forEach(ErrorMessageDialog);
                        }
                    }
                    $('#pBar_1').css('width', parseFloat(((currentPage + 1) / (maxPage + 1) * 100)).toFixed(2) + '%').attr('aria-valuenow', (currentPage + 1) / (maxPage + 1) * 100);
                    $('#pBar_1').html(parseFloat(((currentPage + 1) / (maxPage + 1) * 100)).toFixed(2) + '% Complete');
                    if (response.contRecursive == "1") {
                        if ((currentPage + 1) > maxPage) {
                            lastPage = 0;
                            loop = 1;

                            alert('Proses sinkronisasi data barang telah selesai.');
                            $('#btnCancelSinkroBrg').prop('disabled', false);
                            $('#btnProsesSinkroBrg').prop('disabled', false);
                            if (listErrorMessage != "") {
                                document.getElementById('lblListError').innerText = listErrorMessage;
                                $('#modalError').modal('show');
                            }

                        } else {
                            UploadAllBrgWithProgressBar((currentPage + 1), maxPage, dataPerPage, response.failedRecord);
                        }
                    } else {
                        alert('Proses sinkronisasi data barang telah selesai.');
                        $('#btnCancelSinkroBrg').prop('disabled', false);
                        $('#btnProsesSinkroBrg').prop('disabled', false);
                        if (listErrorMessage != "") {
                            document.getElementById('lblListError').innerText = listErrorMessage;
                            $('#modalError').modal('show');
                        }

                    }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                    if (xhr.status == 524) {
                        if (lastPage == currentPage) {
                            loop = loop + 1;
                        } else {
                            loop = 1;
                        }
                        if (loop <= 3) {
                            UploadAllBrgWithProgressBar(currentPage, maxPage, dataPerPage, failedToSync);
                        } else {
                            alert('Terdapat Error saat sinkroisasi data barang.\nSilahkan hubungi tim support MO');
                            $('#btnCancelSinkroBrg').prop('disabled', false);
                            $('#btnProsesSinkroBrg').prop('disabled', false);
                        }
                    } else {
                        alert('Proses mengambil data barang dari marketplace telah selesai.');
                        $('#btnCancelSinkroBrg').prop('disabled', false);
                        $('#btnProsesSinkroBrg').prop('disabled', false);
                        if (listErrorMessage != "") {
                            document.getElementById('lblListError').innerText = listErrorMessage;
                            $('#modalError').modal('show');
                        }
                    }
                }
            });

        }
