@section scripts{
    <script src="~/Content/selectize.js" type="text/javascript"></script>
    <script src="~/Content/build/js/selectivity-jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.ajax-progress.js"></script>
    <script type="text/javascript">
        function MenuLama() {
            //window.location.href = @Url.Action("master/uploadbarang", "Manage");
            window.location.href = "@Url.Action("master/uploadbarang", "Manage")";

        };

        function Initialize() {
            $('input[name="select_all"]').off('click').on('click',
                function () {
                    var attr = $(this).attr('checked');

                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('input[name="data-1"]').removeAttr('checked');
                        $('input[name="data-2"]').removeAttr('checked');
                    } else {
                        $('input[name="data-1"]').attr('checked', 'checked');
                        $('input[name="data-2"]').attr('checked', 'checked');
                    }
                });

            getKategori();
            getMerk();

            $("#Stf02_HJUAL").off('focus').focus(function () {
                $(this).val(convertToAngka($(this).val()));
            });

            $("#Stf02_HJUAL").off('keydown').on('keydown',
                function (e) {
                    if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                        // Allow: Ctrl+A, Command+A
                        (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                        // Allow: home, end, left, right, down, up
                        (e.keyCode >= 35 && e.keyCode <= 40)) {
                        // let it happen, don't do anything
                        return;
                    }
                    // Ensure that it is a number and stop the keypress
                    if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                        e.preventDefault();
                    }
                });

            $("#Stf02_HJUAL").off('change').on('change', function () {
                if ($(this).val() == '') {
                    $(this).val(0);
                }
                $(this).val(convertToAngka($(this).val()));

                var $input = $(this).val();
                var $rupiah = convertToRupiah($input);
                $(this).val($rupiah);

                //applyHargaPerMarket($rupiah);
            });

        }
        $(document).ready(function () {
            $('#search_barang').keypress(function (e) {
                var key = e.which;
                if (key == 13)// the enter key code
                {
                    RefreshTableBarangSinkroMaster('');
                    return false;
                }
            });
            $('#search_barang_click').click(function () {
                RefreshTableBarangSinkroMaster('');
            });

            $('#TakeRecord').change(function () {
                //searchTablePesananSudahDibayar();
                if ($('#promptHiddenField').val() != "") {
                    refreshTableBarangSinkro("1");
                }
            });
            

            $('#lblBackMenu').hover(function () {
                $(this).css('cursor', 'pointer');
            });
            $('#search_barang2').keypress(function (e) {
                var key = e.which;
                if (key == 13)// the enter key code
                {
                    refreshTableBarangSinkro();
                    return false;
                }
            });
            $('#search_barang_click2').click(function () {
                refreshTableBarangSinkro();
            });
            $('#close-editor-1').off('click').on('click', function () {
                $('#close-editor-1')[0].disabled = true;
                $('.barang_table_section').show();
                $('.barang_editor_section').hide();
                refreshTableBarangSinkro();
            });
        });
        function PromptCustomer() {
            showPromptCustomerTextBoxWindow();
        }

        function setModalMode(value) {
            //$('#btnDeleteAll').hide();
            //$('#btnSyncAll').hide();
            $('#btnGetDataBrg').hide();
            //if (value == 1) {
            //    $('#btnDeleteAll').show();
            //    document.getElementById('txtDeskripsi').innerText = 'Apakah Anda ingin menghapus semua data barang yang belum di sinkronisasi dari akun ini?';
            //}
            //else if (value == 3) {
            //    $('#btnSyncAll').show();
            //    document.getElementById('txtDeskripsi').innerText = 'Apakah Anda ingin mengupdate semua data barang dari Akun ini ke Master Online?';
            //}
            //else
                if (value == 2) {
                $('#btnGetDataBrg').show();
                document.getElementById('txtDeskripsi').innerText = 'Apakah Anda ingin mengambil semua data barang dari Akun ini?';
            }
        }
        function showPromptCustomerTextBoxWindow() {
            var $link = '@Url.Action("PromptCustomer", "Manage")';
            window.open($link, "popupWindow", "width=600, height=400, scrollbars=yes");
        }
        function afterPromptDelTempCust(val, val2) {
            $('#promptHiddenField').val(val);
            //$('#promptHiddenField2').val(val3);
            $('#txtCustomer').val(val2);
            $('#txtCustomer2').val(val2);

            refreshTableBarangSinkro();
        }
        var currentBrg = "";
        function pilihBrg(recnum, nama) {
            currentBrg = recnum;
            $('#search_barang').val(nama);
            RefreshTableBarangSinkroMaster(recnum);
            $('.btn-success').addClass('btn-primary').removeClass('btn-success');
            $('#btnPilih_' + recnum).addClass('btn-success');
        }
        function pilihBrgHapus(recnum, nama, brg) {
            if(confirm("Hapus Link barang : " + nama + " \ndengan kode barang : " + brg))
            {
                currentBrg = "";
                $('.btn-success').addClass('btn-primary').removeClass('btn-success');
                var $link = '@Html.Raw(Url.Action("HapusDraftSinkro", "Manage", new { cust = "replaceCust"}))';
                $link = $link.replace("replaceCust", encodeURIComponent($('#promptHiddenField').val()));
                $link += "&recnum=" + recnum;

                $.ajax({
                    type: "GET",
                    url: $link,
                    beforeSend: function () {
                    },
                    success: function (response) {
                        refreshTableBarangSinkro();
                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                    }
                });
            }
        }
        function RefreshTableBarangSinkroMaster(recnum) {

            var $link = '@Html.Raw(Url.Action("RefreshTableBarangSinkroMaster", "Manage", new { cust = "replaceCust", page = "replaceLastPage", search = "replaceLastSearch" }))';
            $link = $link.replace("replaceCust", encodeURIComponent($('#promptHiddenField').val()));
            $link = $link.replace("replaceLastPage", encodeURIComponent($('#txt_last_page_master').val()));
            $link = $link.replace("replaceLastSearch", encodeURIComponent($('#search_barang').val()));
            if (recnum != "") {
                $link += "&recnum=" + recnum;
            }
            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#form-rekomendasi').hide();
                    //$('#loading_barang_tab').show();
                    //$('#loading_uploadbarang_tab').show();
                },
                success: function (response) {
                    $('#divSearch').show();
                    $('#form-rekomendasi').html(response);
                    $('#form-rekomendasi').show();
                    //$('#loading_barang_tab').hide();
                    //$('#table-sinkro-barang').show();

                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }
        function refreshTableBarangSinkro($pageAwal) {
            currentBrg = "";
            $('#divSearch').hide();
            $('#form-rekomendasi').hide();
            var $setPage = "";
            if ($pageAwal != undefined) {
                $setPage = $pageAwal;
            } else {
                $setPage = $('#txt_last_page').val();
            }
            var $link = '@Html.Raw(Url.Action("RefreshTableBarangSinkro", "Manage", new { search = "replaceLastSearch", take = "replaceLastTake", cust = "replaceCust", page = "replaceLastPage" }))';
            $link = $link.replace("replaceCust", encodeURIComponent($('#promptHiddenField').val()));
            $link = $link.replace("replaceLastPage", encodeURIComponent($setPage));
            $link = $link.replace("replaceLastTake", encodeURIComponent($('#TakeRecord').val()));
            $link = $link.replace("replaceLastSearch", encodeURIComponent($('#search_barang2').val()));

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    $('#table-sinkro-barang').hide();
                    $('#loading_barang_tab').show();
                    //$('#loading_uploadbarang_tab').show();
                },
                success: function (response) {
                    $('#table-sinkro-barang').html(response);
                    $('#loading_barang_tab').hide();
                    $('#table-sinkro-barang').show();

                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }
        function pilihBrgMaster(value) {

            var $link = '@Html.Raw(Url.Action("SetMappingSinkro", "Manage"))';
            $link += "?recnumbrg=" + value + "&currenttemp=" + currentBrg + "&cust=" + encodeURIComponent($('#promptHiddenField').val());

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    //$('#table-sinkro-barang').hide();
                    //$('#loading_barang_tab').show();
                    //$('#loading_uploadbarang_tab').show();
                },
                success: function (response) {
                    //$('#table-sinkro-barang').html(response);
                    //$('#loading_barang_tab').hide();
                    //$('#table-sinkro-barang').show();
                    if (response.error != "") {
                        alert(response.error);
                    }
                    else {
                        if (response.linkrecnum != null) {
                            // remove other link
                            response.linkrecnum.forEach(hapusBrgLain);
                        }
                        $('#txtKodeBrgMO_' + currentBrg).text(response.currentBrg);
                        $('#txtGambarBrgMO_' + currentBrg).html('');
                        $('#txtGambarBrgMO_' + currentBrg).append('<img width="50" height="50" src="' + response.image + '" />');
                        $('#txtNamaBrgMO_' + currentBrg).text(response.namaBrg);

                        $('#btnPilih_' + currentBrg).hide();
                        $('#btnHapus_' + currentBrg).show();
                    }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
