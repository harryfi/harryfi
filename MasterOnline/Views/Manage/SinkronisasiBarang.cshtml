        function GetDataBrgWithRecordConfirm() {
            if ($('#promptHiddenField').val() == null || $('#promptHiddenField').val() == "") {
                alert('Anda belum memilih Toko');
            } else {
                if (confirm("Fungsi tarik data barang ini akan menghapus semua draft sinkronisasi pada akun ini. Anda yakin?")) {
