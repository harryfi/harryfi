<script type="text/javascript">
    $(document).ready(function () {
        if ($('#PackingListLazadaFound')) {
            if (parseInt($('#PackingListLazadaFound').val()) > 0) {
                $('#li_pesanan_lazada_tab_focus').show().on('click', function () {
                    $('#lazada_packing_cust_tabs li:first').find('a').click();
                });
            }
            else{
                $('#li_pesanan_lazada_tab_focus').hide();
            }
        }
    });
    function allcheckboxpackinglazadaonclick(cust){
        $('.checkbox_packing_lazada').each( function() {
        	if($(this).attr("data-cust") == cust){
        	    $(this).prop('checked', $('#all_checkbox_packing_lazada__' + cust).prop('checked'));
        	}
        });
    }
    function RequestPackingLazada(cust) {
        $('#btnProsesPackingLazada__' + cust).attr("disabled", true);

        $('#modalPrintLazada__label__' + cust).html('Proses Ambil Resi Pesanan Lazada');
        $('#modalPrintLazada__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintLazada__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });
        
        var $link = '@Url.Action("RequestPackedLazadaPerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_lazada').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            DeliveryProvider : $('#txtShipment__' + cust).val(),
            rows_selected : rows_selected
        };
        
        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
                $('#loading_spinner').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();

                if (!response.mo_error) {
                    var textareaValue = "";
    
                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";
    
                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintLazada__textarea__' + cust).show();
                    $('#modalPrintLazada__textarea__' + cust).html(textareaValue);
    
                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintLazada__textarea__' + cust).show();
                    $('#modalPrintLazada__textarea__' + cust).html(response.mo_error);
                }
            },
            complete: function () {
                $('#loading_spinner').hide();
                $('#btnProsesPackingLazada__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    function RequestPickupPackingLazada(cust) {
        $('#modalPrintLazada__label__' + cust).html('Proses Siap Kirim Pesanan Lazada');
        $('#btnProsesLazada__' + cust).attr("disabled", true);
        $('#modalPrintLazada__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintLazada__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });
        
        var $link = '@Url.Action("RequestRTSLazadaPerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_lazada').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            DeliveryProvider : $('#txtShipment__' + cust).val(),
            rows_selected : rows_selected
        };
        
        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
                $('#loading_spinner').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();
                if (!response.mo_error) {
                    var textareaValue = "";
    
                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";
    
                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintLazada__textarea__' + cust).show();
                    $('#modalPrintLazada__textarea__' + cust).html(textareaValue);
    
                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintLazada__textarea__' + cust).show();
                    $('#modalPrintLazada__textarea__' + cust).html(response.mo_error);
                }
            },
            complete: function () {
                $('#loading_spinner').hide();
                $('#btnProsesLazada__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
    function LazadaGetLabelPerPacking(cust, label) {
        
        var rows_selected = [];
        $('.checkbox_packing_lazada').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
                
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };

        if (label == "1") {
            var $link = '@Url.Action("LazadaLabelPerPacking", "Manage")';

            $.ajax({
                type: "POST",
                url: $link,
                contentType: "application/json; charset=UTF-8",
                cache: false,
                data: JSON.stringify($postdata),
                success: function (response) {
                    if (response != null) {
                        if (response.mo_error == null) {
                            //$('#labelContainer').html(response);

                            //window.open(response, "_self");
                            var w = window.open('about:blank');
                            w.document.open();
                            w.document.write(response);
                            w.document.close();
                        } else {
                            alert(response.mo_error);
                        }
                    }
                    //window.open(response, "_self");
                    //window.location.href(response);

                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        } else if (label == "2") {
            
            $('#modalPrintLabelOption').modal('show');
            $('#ctk_alamat').attr('checked', true);
            $('#ctk_tlp').attr('checked', true);
            $('#nama_toko').val("");
            $('#tlp_toko').val("");

            $('#btn-print-faktur').off('click').on('click', function () {
                var toko = $('#nama_toko').val();
                var $toko = '&toko=' + encodeURIComponent(toko);
                var tlpToko = $('#tlp_toko').val();
                var $tlpToko = '&tlpToko=' + encodeURIComponent(tlpToko);
                var alamat = 0;
                var tlp = 0;

                var link = '@Url.Action("CetakLabel", "Manage")';

                    if ($("#ctk_alamat").is(":checked") == true ) {
                        alamat = 1;
                    } else if ($("#ctk_alamat").is(":checked") == false ) {
                        alamat = 0;
                    }
                    if ($("#ctk_tlp").is(":checked") == true ) {
                        tlp = 1;
                    } else if ($("#ctk_tlp").is(":checked") == false ) {
                        tlp = 0;
                }

                var $alamat = '&alamat=' + alamat;
                var $tlp = '&tlp=' + tlp;
                //var win = encodeURIComponent(window.open(link += $alamat += $tlp += $toko += $tlpToko, '_blank'));

                $.ajax({
                    type: "POST",
                    url: link,
                    contentType: "application/json; charset=UTF-8",
                    cache: false,
                    data: JSON.stringify($postdata),
                    success: function (response) {
                        if (response != null) {
                            if (response.mo_error == null) {
                                $('#modalPrintLabelOption').modal('hide');
                                var w = window.open($alamat += $tlp += $toko += $tlpToko, 'about:blank');
                                w.document.open();
                                w.document.write(response);
                                w.document.close();

                            } else {
                                $('#modalPrintLabelOption').modal('hide');
                                alert(response.mo_error);
                            }
                        }

                    },
                    error: function (xhr, status, error) {
                        $('#modalPrintLabelOption').modal('hide');
                        console.log(error);
                    }
                });
                
                //if (win) {

                //    win.focus();
                //} else {
                //    alert('Please allow popups for this website');
                //}
            });
                        
        }
    }


    function PromptDeliveryProvLazada(r) {
        //PromptDeliveryProvLazadaTextBox = i;
        showPromptDeliveryProvLazadaTextBoxWindow(r);
    }
    var promptLazadaCust = "";
    function showPromptDeliveryProvLazadaTextBoxWindow(val) {
        promptLazadaCust = val;
        var $link = '@Url.Action("PromptDeliveryProviderLazada", "Manage", new { cust = "replaceCust" })';
        $link = $link.replace("replaceCust", val);
        window.open($link, "popupWindow", "width=600, height=400, scrollbars=yes");
    }

    function afterPromptDelTempLzd(val) {
        $('#txtShipment__' + promptLazadaCust).val(val);
    }

    function cekValidasiPackingLazada(cust) {
        if ($("#txtShipment__" + cust).val()) {
            RequestPackingLazada(cust);
        }
        else {
            alert("Delivery Provider harus dipilih terlebih dahulu.");
        }
    }
    
    function cekValidasiRTSLazada(cust) {
        if ($("#txtShipment__" + cust).val()) {
            RequestPickupPackingLazada(cust);
        }
        else {
            alert("Delivery Provider harus dipilih terlebih dahulu.");
        }
    }
</script>
@{
    var distinctCust = Model.Select(p => new { p.CUST, p.NAMA_CUST }).Distinct().ToList();
}
<div class="row">
    <div class="col-12">
        <ul id="lazada_packing_cust_tabs" class="nav nav-tabs">
            @foreach (var item in distinctCust)
            {
                <li><a data-toggle="tab" href="#lazada_tab_content__@(item.CUST)">@(item.NAMA_CUST)</a></li>
            }
        </ul>
        <div class="tab-content">
    @{ 
        var PackingListLazadaFound = distinctCust.Count();
    }
    @Html.TextBox("PackingListLazadaFound", PackingListLazadaFound, new { @class = "form-control", disabled = "disabled", @style = "display:none;" })
    @foreach (var item in distinctCust)
    {
        <div id="lazada_tab_content__@(item.CUST)" class="tab-pane fade in">
            <input id="txt_last_page__@(item.CUST)" type="hidden" class="form-control" value="@ViewData["LastPage"]">
            <div class="modal fade" id="modalPrintLazada__@(item.CUST)" tabindex="-1" role="dialog" aria-labelledby="modalPrintLazada__label__@(item.CUST)">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            @* <button type="button" class="close btn_modal_hide" aria-label="Close"><span aria-hidden="true">&times;</span></button> *@
                            <h4 class="modal-title" id="modalPrintLazada__label__@(item.CUST)">Print Pesanan Lazada</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-md-12" id="modalPrintLazada__textarea__@(item.CUST)">
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-danger" onclick="$('#modalPrintLazada__@(item.CUST)').modal('hide');">Tutup</button>
                            @*<button type="button" class="btn btn-success" onclick="printRekap();">Print</button>*@
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="input-group">
                        <input id="search_table_packing_pesanan_lazada__@(item.CUST)" type="text" class="form-control" placeholder="Pencarian">
                        <span class="input-group-btn">
                            <button type="button" id="search_table_packing_pesanan_lazada_button__@(item.CUST)" class="btn btn-primary">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </div>
                <div class="col-md-4" style="text-align:right">
                    <div class="input-group">
                        <input id="txtShipment__@(item.CUST)" disabled="disabled" placeholder="Delivery Provider" required="required" class="form-control" maxlength="30" data_val_length_max="30">
                        <span class="input-group-btn">
                            <button id="btnShipment__@(item.CUST)" class="btn btn-default" type="button" style="height: 34px;" onclick="PromptDeliveryProvLazada('@(item.CUST)')">
                                <span class="glyphicon glyphicon-option-horizontal"></span>
                            </button>
                        </span>
                    </div>
                </div>
                <div class="col-md-3" style="text-align:right">
                    <button id="btnProsesPackingLazada__@(item.CUST)" type="button" class="btn btn-primary" onclick="cekValidasiPackingLazada('@(item.CUST)')">Ambil Resi</button>
                    <button id="btnProsesLazada__@(item.CUST)" type="button" class="btn btn-primary" onclick="cekValidasiRTSLazada('@(item.CUST)')">Ready To Ship</button>
                </div>
                <div class="col-md-2" style="text-align:right">
                    @*<button id="btnPrintLazada__@(item.CUST)" type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalPrintLazada__@(item.CUST)">Print</button>*@

                    @*//change by nurul 11/12/2019*@
                    @*<button id="btnPrintLazada__@(item.CUST)" type="button" class="btn btn-primary" onclick="LazadaGetLabelPerPacking('@(item.CUST)')">Print</button>*@

                    <div class="btn-group" role="group">
                        <button id="pl_lazada_btnGroup" type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Print <span class="caret"></span>
                        </button>
                        <div class="dropdown-menu">
                            <button type="button" style="width:100%" id="btnPrintLazada__@(item.CUST)" class="btn btn-success pull-left" onclick="LazadaGetLabelPerPacking('@(item.CUST)','1')">
                                <span @*class="glyphicon glyphicon-eye-open"*@ aria-hidden="true"> Label Marketplace</span>
                            </button>
                            <button type="button" style="width:100%" id="btnPrintMOLazada__@(item.CUST)" class="btn btn-success pull-left" onclick="LazadaGetLabelPerPacking('@(item.CUST)','2')">
                                <span @*class="glyphicon glyphicon-eye-open"*@ aria-hidden="true"> Label MasterOnline</span>
                            </button>
                        </div>
                    </div>
                    @*//end change by nurul 11/12/2019*@
                </div>
            </div>
            <div class="hscroll table-responsive tbljurnal">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                    <tr role="row">
                        <th class="sorting text-center center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-label="Status: activate to sort column ascending" style="width: 40px; max-width: 40px; align-self:flex-start"><input type="checkbox" onclick="allcheckboxpackinglazadaonclick('@item.CUST');" class="dt-checkboxes-select-all center" id="all_checkbox_packing_lazada__@item.CUST" style="border-left:hidden; align-self:flex-start; width:30px; height:28px;" /></th>
                        <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 20%;">No. Pesanan</th>
                        <th class="sorting" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 20%;">No Referensi</th>
                        <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 15%;">No. Resi</th>
                        <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 15%;">Status Kirim</th>
                        <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 35%;">Nama Pemesan</th>
                        <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 15%;">Kurir</th>
                    </tr>
                    </thead>
                    <tbody>
                        @foreach (var order in Model.Where(p => p.CUST == item.CUST).ToList())
                        {
                            var status_kirim = "Dalam Proses";
                            switch (order.status_kirim)
                            {
                                case "0":
                                    status_kirim = "Dalam Proses";
                                    break;
                                case "1":
                                    status_kirim = "Gagal Proses";
                                    break;
                                case "2":
                                    status_kirim = "Siap Kirim";
                                    break;
                                default:
                                    status_kirim = "";
                                    break;
                            }
                            <tr>
                                <td style="text-align:center">
                                    <input type="checkbox" class="dt-checkboxes center checkbox_packing_lazada" data-cust="@order.CUST" data-recnum="@order.so_recnum" style="align-self:flex-start; width:30px; height:28px;" />
                                </td>
