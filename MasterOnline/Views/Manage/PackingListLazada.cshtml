@using System.Globalization
@*@using MasterOnline.ViewModels*@
@using PagedList.Mvc
@model PagedList.IPagedList<PackingPerMP>
@{
    string currentFilter = ViewBag.CurrentFilter;
    string currentSort = ViewBag.CurrentSort;

}
@Html.AntiForgeryToken()
<script type="text/javascript">
    $(document).ready(function () {
        if ($('#PackingListLazadaFound')) {
            if (parseInt($('#PackingListLazadaFound').val()) > 0) {
                $('#li_pesanan_lazada_tab_focus').show().on('click', function () {
                    $('#lazada_packing_cust_tabs li:first').find('a').click();
                });
            }
            else{
                $('#li_pesanan_lazada_tab_focus').hide();
            }
        }
    });
    function allcheckboxpackinglazadaonclick(cust){
        $('.checkbox_packing_lazada').each( function() {
        	if($(this).attr("data-cust") == cust){
        	    $(this).prop('checked', $('#all_checkbox_packing_lazada').prop('checked'));
        	}
        });
    }
    function RequestPackingLazada(cust) {
        $('#modalPrintLazada__label__' + cust).html('Proses Ambil Resi Pesanan Lazada');
        $('#btnProsesLazada__' + cust).attr("disabled", true);
        
        var $link = '@Url.Action("RequestPackedLazadaPerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_lazada').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            DeliveryProvider : $('#txtShipment__' + cust).val(),
            rows_selected : rows_selected
        };
        
        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
                $('#loading_spinner').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();

                if (response.mo_error != undefined) {
                    alert(response.mo_error);
                }
                else {
                    var textareaValue = "";

                    var adaSuccess = false;
                    var successValue = "";
                    response.listSuccess.forEach(function (item) {
                        adaSuccess = true;
                        if (successValue != "") {
                            successValue += ", ";
                        }
                        successValue += item.no_referensi;
                    });
                    if (adaSuccess) {
                        textareaValue += "Pesanan berhasil update status :</br>";
                        textareaValue += successValue + ". </br>";
                    }

                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + ". </br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Error Orders :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintLazada__textarea__' + cust).html(textareaValue);
                    
                    $('#modalPrintLazada__' + cust).modal({
                        show: true,
                        backdrop: 'static',
                        keyboard: false
                    });
                }
            },
            complete: function () {
                $('#loading_spinner').hide();
                $('#btnProsesLazada__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }
