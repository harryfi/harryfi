                                <tr role="row">
                                    <th class="sorting text-center center" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-label="Status: activate to sort column ascending" style="width: 40px; max-width: 40px; align-self:flex-start"><input type="checkbox" onclick="allcheckboxpackinglazadaonclick('@item.CUST');" class="dt-checkboxes-select-all center" id="all_checkbox_packing_lazada__@item.CUST" style="border-left:hidden; align-self:flex-start; width:30px; height:28px;" /></th>
