            }
        }

        function RefreshTableDashboardBaranglakuPartial() {
            var $Drtgl = "";
            if ($('#DrTanggal').val() != undefined) {
                $Drtgl = $('#DrTanggal').val();
            }
            var $Sdtgl = "";
            if ($('#SdTanggal').val() != undefined) {
                $Sdtgl = $('#SdTanggal').val();
            }
            var link = '@Html.Raw(Url.Action("RefreshDashboardBaranglakuPartial", "Manage", new { drTgl = "replaceDrtgl", sdTgl = "replaceSdtgl" }))';
            link = link.replace("replaceDrtgl", encodeURIComponent($Drtgl));
            link = link.replace("replaceSdtgl", encodeURIComponent($Sdtgl));

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#table-baranglaku-partial').hide();
                    $('#chart-baranglaku-section').hide();
                    $('#loading_dashboard_baranglaku').show();
                },
                success: function (response) {
                    $('#table-baranglaku-partial').html(response);
                    if (listPieBarang.length > 0) {

                        //var dataBrgLaku = response.ListBarangLaku.OrderByDescending(b => b.Qty).Take(10);
                        var dataBrgLaku = listPieBarang;
                        var kdBarang = [];
                        var nmBarang = [];
                        var qty = [];

                        for (var i = 0; i < dataBrgLaku.length; i++) {
                            kdBarang.push(dataBrgLaku[i].KodeBrg);
                            nmBarang.push(dataBrgLaku[i].NamaBrg);
                            qty.push(dataBrgLaku[i].Qty);
                        }

                        var ctxBarangLaku = document.getElementById("chart-baranglaku").getContext('2d');
                        var chart = new Chart(ctxBarangLaku, {
                            type: 'pie',
                            data: {

                                //labels: kdBarang,
                                labels: nmBarang,
                                datasets: [{
                                    data: qty,
                                    backgroundColor: [
                                        "#f990a7",
                                        "#9966FF",
                                        "#99e5e5",
                                        "#f7bd83",

                                        "#2ecc71",
                                        "#3498db",
                                        "#95a5a6",
                                        "#9b59b6",
                                        "#f1c40f",
                                        "#e74c3c",
                                    ],
                                    borderWidth: 0
                                }]
                            },
                            options: {
                                title: {
                                    display: true,
                                    text: '10 Barang Paling Laku',
                                    position: 'top',
                                    fontSize: 20,
                                },
                                legendCallback: function (chart) {


                                    var text = [];
                                    text.push('<ul class="' + chart.id + '-legendPie" style="list-style:none">');
                                    for (var i = 0; i < chart.data.datasets[0].data.length; i++) {
                                        text.push('<section>');
                                        text.push('<li><div style="width:10px;height:10px;display:inline-block;background:' + chart.data.datasets[0].backgroundColor[i] + '" />&nbsp;');
                                        if (chart.data.labels[i]) {
                                            var total = chart.data.datasets[0].data.reduce(function (previousValue, currentValue, currentIndex, array) {
                                                return previousValue + currentValue;
                                            });
