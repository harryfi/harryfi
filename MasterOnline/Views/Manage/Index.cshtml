        function RefreshTableDashboardBarangMinStok() {
            var $Drtgl = "";
            if ($('#DrTanggal2').val() != undefined) {
                $Drtgl = $('#DrTanggal2').val();
            }
            var $Sdtgl = "";
            if ($('#SdTanggal2').val() != undefined) {
                $Sdtgl = $('#SdTanggal2').val();
            }
            var link = '@Html.Raw(Url.Action("RefreshDashboardBarangMinStok", "Manage", new { drTgl = "replaceDrtgl", sdTgl = "replaceSdtgl" }))';
            link = link.replace("replaceDrtgl", encodeURIComponent($Drtgl));
            link = link.replace("replaceSdtgl", encodeURIComponent($Sdtgl));

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#table-barang-minstok-partial').hide();
                    $('#loading_barang_minstok').show();
                },
                success: function (response) {
                    if (response.Errors == null) {
                        $('#table-barang-minstok-partial').html(response);

                        $('#loading_barang_minstok').hide();
                        $('#table-barang-minstok-partial').show();
                    } else {
                        alert(response.Errors);
                        $('#loading_barang_minstok').hide();
                    }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }
