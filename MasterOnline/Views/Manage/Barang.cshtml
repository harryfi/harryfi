        function refreshTableBarang() {
            var link = '@Url.Action("RefreshTableBarang", "Manage")';

            $.ajax({
                type: "GET",
                url: link,
                beforeSend: function () {
                    $('#datatable').hide();
                    $('#loading_barang_tab').show();
                },
                success: function (response) {
                    $('#table-barang-partial').html(response);
                    var table = $('#datatable').DataTable();
                    $('#search_barang').keyup(function () {
                        table.search($(this).val()).draw();
                    });
                    $('#loading_barang_tab').hide();
                    $('#datatable').show();
                    //add by calvin 23 nov 2018, setelah di close, search lg
                    table.search($('#search_barang').val()).draw();
                    //end add by calvin 23 nov 2018, setelah di close, search lg
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        }

