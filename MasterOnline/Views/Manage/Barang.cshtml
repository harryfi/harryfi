        function ClearCategoryLazada(divIDpertama, divIDselanjutnya, divIDattribute, i, undoing) {
            var fieldCategoryCode = '#ListHargaJualPermarket_' + i + '__CATEGORY_CODE';
            var fieldCategoryName = '#ListHargaJualPermarket_' + i + '__CATEGORY_NAME';
            var msg = "Kosongkan Kategori & Attribute ?";
            if (undoing) {
                msg = "Kembalikan Seperti Semula ?";
            }
            if (confirm(msg)) {
                $('#' + divIDpertama).html('');
                $('#' + divIDselanjutnya).html('');
                $('#' + divIDattribute).html('');
                if (!undoing) {
                    $(fieldCategoryCode).val('');
