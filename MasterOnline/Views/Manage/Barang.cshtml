        function searchTableBarang(){
            var $link = '@Html.Raw(Url.Action("RefreshTableBarang", "Manage", new { search = "replaceSearch" }))';
            $link = $link.replace("replaceSearch", encodeURIComponent($('#search_barang').val()));

            $.ajax({
                type: "GET",
                url: $link,
                beforeSend: function () {
                    //$('#table-barang-1-partial').hide();
                    $('#search_barang_click')[0].disabled = true;
                    $('#loading_barang_tab').show();
                },
                success: function (response) {
                    $('#search_barang_click')[0].disabled = false;
                    $('#table-barang-1-partial').html(response);
                    $('#loading_barang_tab').hide();
                    //$('#table-barang-1-partial').show();
                },
                error: function (xhr, status, error) {
                    $('#search_barang_click')[0].disabled = false;
                    console.log(error);
                }
            });
        }
