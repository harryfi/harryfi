            link = link.replace("replaceCode", fieldCategoryCodeValue);

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listKategori = [];

                    //generate category
                    $.each(data,
                        function (i, item) {
                            if (i == 0) {
                                var textbox = '<input type="text" data-val="true" disabled="disabled" class="form-control" value="'+ item.NAME +'">';
                                $('#category_lazada_pertama_' + listMarketRecNum).append(textbox);
                            }
                            else {
                                var textbox = '<div class="col-md-3 col-sm-3 col-xs-12"><input type="text" data-val="true" disabled="disabled" class="form-control" value="'+ item.NAME +'"></div>';
                                $('#category_lazada_selanjutnya_' + listMarketRecNum).append(textbox);
