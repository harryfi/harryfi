                            $('#div_detail_' + data_ar_recnum + ' :input[data-valid-toggle-required="1"]').each(function () {
                                var $this = $(this);
                                var $value = $this.val();

                                if ($value == '' || isEmptyOrSpaces($value)) {
                                    IsValid = false;
                                }
                            });

                            if (!IsValid) {
                                switch (listMarket[i].NAMA) {
                                    case "17":
                                        errorShopee = 1;
                                        break;
                                    case "15":
                                        errorTokped = 1;
                                        break;
                                    case "7":
