                    var data_ar_recnum = $this.attr("data-ar-recnum");
                    $('#div_detail_' + data_ar_recnum + ' :input[data-valid-toggle-required="1"]').attr('required', 'required');

                    var listMarket = @Html.Raw(Json.Encode(Model.ListMarket));
                    for (var i = 0; i < listMarket.length; i++) {
