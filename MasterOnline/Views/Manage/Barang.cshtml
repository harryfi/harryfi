                                        break;
                                    case "9":
                                        errorElevenia = 1;
                                        break;
                                    case "19":
                                        errorJDID = 1;
                                        break;
                                    case "20":
                                        error82Cart = 1;
                                        break;
                                }
                            }
                        }
                    }
                }
            });
        }
