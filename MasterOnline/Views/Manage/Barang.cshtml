                }
                else {
                    $(fieldCategoryCode).val('undo');
                }
                getKategoriPertamaLazada(i);
            }
        }

        function getKategoriPertamaLazada(spec) {
            @*@{
                var context_ = new MoDbContext();
            }*@
            var idMarketLazada = @Html.Raw(Json.Encode(context_.Marketplaces.FirstOrDefault(m => m.NamaMarket.ToUpper().Equals("LAZADA")).IdMarket));
            var listMarket = @Html.Raw(Json.Encode(Model.ListMarket));
            for (var i = 0; i < listMarket.length; i++) {
                if (listMarket[i].NAMA == idMarketLazada.toString() && (spec < 0 || spec == i)) {
                    if (listMarket[i].TOKEN != null) {

                        var selectCategoryLazadaID = 'Category_Lazada_' + listMarket[i].RecNum;
                        var fieldCategoryCode = '#ListHargaJualPermarket_'+ i +'__CATEGORY_CODE';
                        var fieldCategoryName = '#ListHargaJualPermarket_'+ i +'__CATEGORY_NAME';
                        var nextselectID = "Category_Lazada_2_" + listMarket[i].RecNum;
                        var categoryLazadaSelanjutnya =  '#category_lazada_selanjutnya_' + listMarket[i].RecNum;
                        var listMarketRecNum = listMarket[i].RecNum;

                        if ($(fieldCategoryCode).val() != "" && $(fieldCategoryCode).val() != null) {
                            //edit
                            var ListHargaJualPermarketView = model_listhargajualpermarketview;
                            ListHargaJualPermarketView = $.grep( ListHargaJualPermarketView, function( n, i ) {
                                return n.BRG===$('#Stf02_BRG').val()  && n.IDMARKET === listMarketRecNum;
                            });

                            if (ListHargaJualPermarketView[0] != undefined) {

                                $(fieldCategoryCode).val(ListHargaJualPermarketView[0]['CATEGORY_CODE']);

                                var fieldCategoryCodeValue = $(fieldCategoryCode).val();
                                FuncGetKategoriLazadaByChildCode(fieldCategoryCodeValue,listMarketRecNum,fieldCategoryCode,ListHargaJualPermarketView);
                            }
                        }
                        else {
                            //new
                            FuncGetKategoriLazadaByCode(selectCategoryLazadaID,listMarket[i].RecNum,nextselectID,categoryLazadaSelanjutnya,fieldCategoryCode,fieldCategoryName,listMarketRecNum);
                        }
                    }
                }
            }
        }
        function FuncGetKategoriLazadaByChildCode(fieldCategoryCodeValue,listMarketRecNum,fieldCategoryCode,ListHargaJualPermarketView){
