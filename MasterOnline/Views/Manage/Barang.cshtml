        function Initialize() {

            $('#barangTab a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                var target = $(e.target).attr("href");
                if ((target == '#tab_content_variant')) {
                    $('#txtInfoKodeBarang').val($('#Stf02_BRG').val());
                    $('#txtInfoNamaBarang').val($('#Stf02_NAMA').val() + ' ' + $('#Stf02_NAMA2').val());
                }
            });

            $('.cbxtype').each(function () {
                var $this = $(this);

                $this.click(function () {
                    var $cbxTarget = $('.cbxtype[data-cbx="cbx-type"]');

                    if ($cbxTarget.val() == null || $cbxTarget.val() == '' || $cbxTarget.val() == '3') {
                        $cbxTarget.val('4');
                        $('#simpan_btn').hide();
                        $('#next_btn').show();
                        $('#div_info_harga_variant').show();
                    } else {
                        $cbxTarget.val('3');
                        $('#var-structure-partial').html('');
                        $('#var-detail-variant-partial').html('');
                        $('#simpan_btn').show();
                        $('#next_btn').hide();
                        $('#div_info_harga_variant').hide();
                    }

                    CekTabVariant();
                });
            });

            $('.cbx').each(function () {
                var $this = $(this);
                $this.click(function () {
                    var name = $this.attr('name');
                    var $cbxTarget = $('.cbxTarget[data-cbx="' + name + '"]');
                    if ($cbxTarget.val() == 'False' || $cbxTarget.val() == 'false') {
                        $cbxTarget.val(true);
                    } else {
                        $cbxTarget.val(false);
                    }
                });
            });

            $('.collapse-link').off('click').on('click', function () {
                var $BOX_PANEL = $(this).closest('.x_panel'),
                    $ICON = $(this).find('i'),
                    $BOX_CONTENT = $BOX_PANEL.find('.x_content');

                // fix for some div with hardcoded fix class
                if ($BOX_PANEL.attr('style')) {
                    $BOX_CONTENT.slideToggle(200, function () {
                        $BOX_PANEL.removeAttr('style');
                    });
                } else {
                    $BOX_CONTENT.slideToggle(200);
                    $BOX_PANEL.css('height', 'auto');
                }

                $ICON.toggleClass('fa-chevron-up fa-chevron-down');
            });

            $('input[name="select_all"]').off('click').on('click',
                function () {
                    var attr = $(this).attr('checked');

                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('input[name="data-1"]').removeAttr('checked');
                        $('input[name="data-2"]').removeAttr('checked');
                    } else {
                        $('input[name="data-1"]').attr('checked', 'checked');
                        $('input[name="data-2"]').attr('checked', 'checked');
                    }
                });

            $('#tambah_barang').off('click').on('click', function () {
                $('.barang_table_section').hide();
                $('.barang_editor_section').show();
                $('#close-editor-1')[0].disabled = false;
                $('#variant-induk-tab').removeAttr('data-toggle');
                $('#variant-induk-tab').addClass('disabled-tab');

                if ($('#Stf02_BRG').val() == '') {
                    $('#hjualmarket-section input[data-jenis="hjual"]').each(function () {
                        $(this).val(convertToRupiah(0));
                    });
                }

                //add by nruul 18/10/2018
                $("#Stf02_MINI").keydown(
                    function () {
                        validateInput($(this), 'ANGKA');
                    });
                $("#Stf02_MAXI").keydown(
                    function () {
                        validateInput($(this), 'ANGKA');
                    });
                $("#Stf02_BERAT").keydown(
                    function () {
                        validateInput($(this), 'ANGKA');
                    });
                $("#Stf02_PANJANG").keydown(
                    function () {
                        validateInput($(this), 'ANGKA');
