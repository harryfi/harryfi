                                    //getFaktur();
                                    $('#tambah-faktur').removeAttr('disabled');
                                    //end change by nurul 8/6/2020
                                }
                            }
                        }
                    });

                    custSelect[0].selectize.setValue($('#Faktur_CUST').val());
                },
