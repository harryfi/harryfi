<script type="text/javascript">
    $(document).ready(function () {
        if ($('#PackingListShopeeFound')) {
            if (parseInt($('#PackingListShopeeFound').val()) > 0) {
                $('#li_pesanan_shopee_tab_focus').show().on('click', function () {
                    $('#shopee_packing_cust_tabs li:first').find('a').click();
                });
            }
            else{
                $('#li_pesanan_shopee_tab_focus').hide();
            }
        }

    
        $('.selectShopeeResiPickupAddress').each(function (i, item) {

            var cust = $(item).attr("data-kodecust");

            var link = '@Html.Raw(Url.Action("GetShopeePickupAddressByCust", "Manage", new { cust = "replaceCode" }))';
            link = link.replace("replaceCode", encodeURIComponent(cust));

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listAddress = [];

                    $.each(data,
                        function (i, item) {
                            listAddress[i] = {
                                id: item.address_id,
                                text: item.address
                            };
                        });

                    var Select = $('#select-shopee-resi-pickup-address-' + cust).selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: listAddress,
                        onChange: function (value) {

                        }
                    });

                    //Select[0].selectize.setValue(valueTRACKING_SHIPMENT[1]);
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        });
    });

    function allcheckboxpackingshopeeonclick(cust){
        $('.checkbox_packing_shopee').each( function() {
            if($(this).attr("data-cust") == cust){
                $(this).prop('checked', $('#all_checkbox_packing_shopee__' + cust).prop('checked'));
            }
        });
    }
    function RequestPickupPackingShopee(cust) {
        $('#btnProsesPickupShopee__' + cust).attr("disabled", true);
        
        if ($('#select-shopee-resi-pickup-address-' + cust).val()) {
            $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
            $('#modalPrintShopee__label__' + cust).html('Proses Pickup Pesanan Shopee');
            $('#modalPrintShopee__' + cust).modal({
                show: true,
                backdrop: 'static',
                keyboard: false
            });
            
            var $link = '@Url.Action("RequestPickupShopeePerPacking", "Manage")';

            var rows_selected = [];
            $('.checkbox_packing_shopee').each( function() {
                if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                    rows_selected.push($(this).attr("data-recnum"));
                }
            });
            
            $postdata = {
                cust : cust,
                bukti : $('#packingList_NO_BUKTI').val(),
                alamat : $('#select-shopee-resi-pickup-address-' + cust).val(),
                rows_selected : rows_selected
            };
            
            $.ajax({
                type: "POST",
                url: $link,
                contentType: "application/json; charset=UTF-8",
                cache: false,
                data: JSON.stringify($postdata),
                beforeSend: function () {
                    // $('#loading_spinner').show();
                },
                success: function (response) {
                    // $('#loading_spinner').hide();
                    if (!response.mo_error) {
                        var textareaValue = "";

                        textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                        var adaError = false;
                        var errorValue = "";
                        response.listErrors.forEach(function (item) {
                            adaError = true;
                            errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                        });
                        if (adaError) {
                            textareaValue += "</br>";
                            textareaValue += "Pesanan gagal diproses :</br>";
                            textareaValue += errorValue;
                        }
                        $('#modalPrintShopee__textarea__' + cust).show();
                        $('#modalPrintShopee__textarea__' + cust).html(textareaValue);

                        // if(response.successCount > 0){
                        //     refreshTableSudahDibayar();
                        // }
                    } else {
                        $('#modalPrintShopee__textarea__' + cust).show();
                        $('#modalPrintShopee__textarea__' + cust).html(response.mo_error);
                    }
                },
                complete: function () {
                    $('#btnProsesPickupShopee__' + cust).attr("disabled", false);
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        } 
        else {
            alert('Silahkan pilih alamat Pickup terlebih dahulu.');
            $('#btnProsesPickupShopee__' + cust).attr("disabled", false);
        }
    }
    function RequestDropoffPackingShopee(cust) {
        $('#btnProsesDropoffShopee__' + cust).attr("disabled", true);
        $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu');
        $('#modalPrintShopee__label__' + cust).html('Proses Dropoff Pesanan Shopee');
        $('#modalPrintShopee__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });
        
        var $link = '@Url.Action("RequestDropoffShopeePerPacking", "Manage")';

        var rows_selected = [];
        $('.checkbox_packing_shopee').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        
        $postdata = {
            cust : cust,
            bukti : $('#packingList_NO_BUKTI').val(),
            rows_selected : rows_selected
        };
            
        $.ajax({
            type: "POST",
            url: $link,
            contentType: "application/json; charset=UTF-8",
            cache: false,
            data: JSON.stringify($postdata),
            beforeSend: function () {
                //$('#loading_barang_tab').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();
                if (!response.mo_error) {
                    var textareaValue = "";
    
                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";
    
                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintShopee__textarea__' + cust).show();
                    $('#modalPrintShopee__textarea__' + cust).html(textareaValue);
    
                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintShopee__textarea__' + cust).show();
                    $('#modalPrintShopee__textarea__' + cust).html(response.mo_error);
                }
            },
            complete: function () {
                $('#btnProsesDropoffShopee__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    function ShopeeGetLabelPerPacking(cust,label) {
        

        var rows_selected = [];
        $('.checkbox_packing_shopee').each( function() {
            if($(this).attr("data-cust") == cust && $(this).is(":checked")){
                rows_selected.push($(this).attr("data-recnum"));
            }
        });
        
        

        if (rows_selected.length <= 0) {
            alert("Mohon pilih pesanan yang mau diproses.");
        } else {
            $postdata = {
                cust: cust,
                bukti: $('#packingList_NO_BUKTI').val(),
                rows_selected: rows_selected
            };

            if (label == "1") {
                $('#modalPrintShopee__label__' + cust).html('Print Pesanan Shopee');
                $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
                $('#modalPrintShopee__' + cust).modal({
                    show: true,
                    backdrop: 'static',
                    keyboard: false
                });

                var $link = '@Url.Action("ShopeeLabelPerPacking", "Manage")';

                $.ajax({
                    type: "POST",
                    url: $link,
                    contentType: "application/json; charset=UTF-8",
                    cache: false,
                    data: JSON.stringify($postdata),
                    success: function (response) {
                        if (response.mo_error != undefined) {
                            $('#modalPrintShopee__textarea__' + cust).html(response.mo_error);
                        }
                        else {
                            var textareaValue = "";

                            var adaUrl = false;
                            var urlValue = "";
                            var countUrl = 0;
                            response.ret.batch_result.airway_bills.forEach(function (item) {
                                adaUrl = true;
                                //urlValue += item + " \n";
                                countUrl++;
                                //$("<iframe id='iframe_" + countUrl + "_" + cust + "'>")                             // create a new iframe element
                                //    .hide()                               // make it invisible
                                //    .attr("src", item) // point the iframe to the page you want to print
                                //    .appendTo("body");

                                var buttonHtml = "<a class='btn btn-success' target='_blank' href='" + item + "'>Print</a> </br>";
                                urlValue += buttonHtml;
                            });
                            if (adaUrl) {
                                textareaValue += "Url AWB :</br>";
                                textareaValue += urlValue;
                            }

                            var adaError = false;
                            var errorValue = "";
                            response.ret.batch_result.errors.forEach(function (item) {
                                adaError = true;
                                errorValue += item.ordersn + " : " + item.error_description + ". </br>";
                            });
                            if (adaError) {
                                textareaValue += "</br>";
                                textareaValue += "Error Orders :</br>";
                                textareaValue += errorValue;
                            }
                            $('#modalPrintShopee__textarea__' + cust).html(textareaValue);
                        }
                    },
                    error: function (xhr, status, error) {
                        console.log(error);
                    }
                });

                //function shopeePrint(url) {
                //    //var divToPrint = document.getElementById("iframe_" + countUrl + "_" + cust + "");
                //    var newWin = window.open(url, 'Print-Window');
                //    newWin.document.open();
                //    newWin.document.write('<html><body onload="window.print()">' + divToPrint.innerHTML + '</body></html>');
                //    newWin.document.close();
                //    setTimeout(function () {
                //        newWin.close();
                //    }, 10);
                //}
            } else if (label == "2") {

                $('#modalPrintLabelOption').modal('show');
                $('#ctk_alamat').attr('checked', true);
                $('#ctk_tlp').attr('checked', true);
                $('#nama_toko').val("");
                $('#tlp_toko').val("");

                $('#btn-print-faktur').off('click').on('click', function () {
                    var toko = $('#nama_toko').val();
                    var $toko = '&toko=' + encodeURIComponent(toko);
                    var tlpToko = $('#tlp_toko').val();
                    var $tlpToko = '&tlpToko=' + encodeURIComponent(tlpToko);
                    var alamat = 0;
                    var tlp = 0;

                    var link = '@Url.Action("CetakLabel", "Manage")';

                    if ($("#ctk_alamat").is(":checked") == true) {
                        alamat = 1;
                    } else if ($("#ctk_alamat").is(":checked") == false) {
                        alamat = 0;
                    }
                    if ($("#ctk_tlp").is(":checked") == true) {
                        tlp = 1;
                    } else if ($("#ctk_tlp").is(":checked") == false) {
                        tlp = 0;
                    }

                    var $alamat = '&alamat=' + alamat;
                    var $tlp = '&tlp=' + tlp;
                    //var win = encodeURIComponent(window.open(link += $alamat += $tlp += $toko += $tlpToko, '_blank'));

                    $.ajax({
                        type: "POST",
                        url: link,
                        contentType: "application/json; charset=UTF-8",
                        cache: false,
                        data: JSON.stringify($postdata),
                        success: function (response) {
                            if (response != null) {
                                if (response.mo_error == null) {
                                    $('#modalPrintLabelOption').modal('hide');
                                    var w = window.open($alamat += $tlp += $toko += $tlpToko, 'about:blank');
                                    w.document.open();
                                    w.document.write(response);
                                    w.document.close();

                                } else {
                                    $('#modalPrintLabelOption').modal('hide');
                                    alert(response.mo_error);
                                }
                            }

                        },
                        error: function (xhr, status, error) {
                            $('#modalPrintLabelOption').modal('hide');
                            console.log(error);
                        }
                    });
                });
            }
        }
    }
</script>
@{
