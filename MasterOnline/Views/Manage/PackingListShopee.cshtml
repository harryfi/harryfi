@using System.Globalization
@*@using MasterOnline.ViewModels*@
@using PagedList.Mvc
@model PagedList.IPagedList<PackingPerMP>
@{
    string currentFilter = ViewBag.CurrentFilter;
    string currentSort = ViewBag.CurrentSort;

}
<script type="text/javascript">
    $(document).ready(function () {
        if ($('#PackingListShopeeFound')) {
            if (parseInt($('#PackingListShopeeFound').val()) > 0) {
                $('#li_pesanan_shopee_tab_focus').show().on('click', function () {
                    $('#shopee_packing_cust_tabs li:first').find('a').click();
                });
            }
            else{
                $('#li_pesanan_shopee_tab_focus').hide();
            }
        }

        $('.selectShopeeResiPickupAddress').each(function (i, item) {

            var cust = $(item).attr("data-kodecust");

            var link = '@Html.Raw(Url.Action("GetShopeePickupAddressByCust", "Manage", new { cust = "replaceCode" }))';
            link = link.replace("replaceCode", encodeURIComponent(cust));

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listAddress = [];

                    $.each(data,
                        function (i, item) {
                            listAddress[i] = {
                                id: item.address_id,
                                text: item.address
                            };
                        });

                    var Select = $('#select-shopee-resi-pickup-address-' + cust).selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: listAddress,
                        onChange: function (value) {

                        }
                    });

                    //Select[0].selectize.setValue(valueTRACKING_SHIPMENT[1]);
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        });
    });

    function RequestPickupPackingShopee(cust) {
        $('#btnProsesPickupShopee__' + cust).attr("disabled", true);
        
        if ($('#select-shopee-resi-pickup-address-' + cust).val()) {
            var $link = '@Html.Raw(Url.Action("RequestPickupShopeePerPacking", "Manage", new { cust = "replaceCust",bukti = "replaceBuktiPacking", alamat = "replaceAlamatPickup" }))';
            $link = $link.replace("replaceCust", encodeURIComponent(cust));
            $link = $link.replace("replaceBuktiPacking", encodeURIComponent($('#packingList_NO_BUKTI').val()));
            $link = $link.replace("replaceAlamatPickup", encodeURIComponent($('#select-shopee-resi-pickup-address-' + cust).val()));

            $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
            $('#modalPrintShopee__label__' + cust).html('Proses Pickup Pesanan Shopee');
            $('#modalPrintShopee__' + cust).modal({
                show: true,
                backdrop: 'static',
                keyboard: false
            });
            $.ajax({
                url: $link,
                type: 'GET',
                beforeSend: function () {
                    // $('#loading_spinner').show();
                },
                success: function (response) {
                    // $('#loading_spinner').hide();
                    if (!response.mo_error) {
                        var textareaValue = "";

                        textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";

                        var adaError = false;
                        var errorValue = "";
                        response.listErrors.forEach(function (item) {
                            adaError = true;
                            errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                        });
                        if (adaError) {
                            textareaValue += "</br>";
                            textareaValue += "Pesanan gagal diproses :</br>";
                            textareaValue += errorValue;
                        }
                        $('#modalPrintShopee__textarea__' + cust).show();
                        $('#modalPrintShopee__textarea__' + cust).html(textareaValue);

                        // if(response.successCount > 0){
                        //     refreshTableSudahDibayar();
                        // }
                    } else {
                        $('#modalPrintShopee__textarea__' + cust).show();
                        $('#modalPrintShopee__textarea__' + cust).html('Gagal memproses pickup. Mohon hubungi support.');
                    }
                },
                complete: function () {
                    $('#btnProsesPickupShopee__' + cust).attr("disabled", false);
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            });
        } 
        else {
            alert('Silahkan pilih alamat Pickup terlebih dahulu.');
            $('#btnProsesPickupShopee__' + cust).attr("disabled", false);
        }
    }
    function RequestDropoffPackingShopee(cust) {
        $('#btnProsesDropoffShopee__' + cust).attr("disabled", true);
        $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu');
        $('#modalPrintShopee__label__' + cust).html('Proses Dropoff Pesanan Shopee');
        $('#modalPrintShopee__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });
        
        var $link = '@Html.Raw(Url.Action("RequestDropoffShopeePerPacking", "Manage", new { cust = "replaceCust",bukti = "replaceBuktiPacking" }))';
        $link = $link.replace("replaceCust", encodeURIComponent(cust));
        $link = $link.replace("replaceBuktiPacking", encodeURIComponent($('#packingList_NO_BUKTI').val()));

        $.ajax({
            url: $link,
            type: 'GET',
            beforeSend: function () {
                //$('#loading_barang_tab').show();
            },
            success: function (response) {
                //$('#loading_barang_tab').hide();
                if (!response.mo_error) {
                    var textareaValue = "";
    
                    textareaValue += "Jumlah pesanan berhasil diproses : "+ response.successCount +"</br>";
    
                    var adaError = false;
                    var errorValue = "";
                    response.listErrors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.keyname + " : " + item.errorMessage + "</br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Pesanan gagal diproses :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintShopee__textarea__' + cust).show();
                    $('#modalPrintShopee__textarea__' + cust).html(textareaValue);
    
                    // if(response.successCount > 0){
                    //     refreshTableSudahDibayar();
                    // }
                } else {
                    $('#modalPrintShopee__textarea__' + cust).show();
                    $('#modalPrintShopee__textarea__' + cust).html('Gagal memproses dropoff. Mohon hubungi support.');
                }
            },
            complete: function () {
                $('#btnProsesDropoffShopee__' + cust).attr("disabled", false);
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });
    }

    function ShopeeGetLabelPerPacking(cust) {
        $('#modalPrintShopee__label__' + cust).html('Print Pesanan Shopee');
        $('#modalPrintShopee__textarea__' + cust).html('Sedang diproses. mohon menunggu..');
        $('#modalPrintShopee__' + cust).modal({
            show: true,
            backdrop: 'static',
            keyboard: false
        });

        var $link = '@Html.Raw(Url.Action("ShopeeLabelPerPacking", "Manage", new { cust = "replaceCust",bukti = "replaceBuktiPacking" }))';
        $link = $link.replace("replaceCust", encodeURIComponent(cust));
        $link = $link.replace("replaceBuktiPacking", encodeURIComponent($('#packingList_NO_BUKTI').val()));

        $.ajax({
            type: "GET",
            url: $link,
            success: function (response) {

                if (response.mo_error != undefined) {
                    $('#modalPrintShopee__textarea__' + cust).html(response.mo_error);
                }
                else {
                    var textareaValue = "";

                    var adaUrl = false;
                    var urlValue = "";
                    var countUrl = 0;
                    response.ret.batch_result.airway_bills.forEach(function (item) {
                        adaUrl = true;
                        //urlValue += item + " \n";
                        countUrl++;
                        //$("<iframe id='iframe_" + countUrl + "_" + cust + "'>")                             // create a new iframe element
                        //    .hide()                               // make it invisible
                        //    .attr("src", item) // point the iframe to the page you want to print
                        //    .appendTo("body");

                        var buttonHtml = "<a class='btn btn-success' target='_blank' href='" + item + "'>Print</a> </br>";
                        urlValue += buttonHtml;
                    });
                    if (adaUrl) {
                        textareaValue += "Url AWB :</br>";
                        textareaValue += urlValue;
                    }

                    var adaError = false;
                    var errorValue = "";
                    response.ret.batch_result.errors.forEach(function (item) {
                        adaError = true;
                        errorValue += item.ordersn + " : " + item.error_description + ". </br>";
                    });
                    if (adaError) {
                        textareaValue += "</br>";
                        textareaValue += "Error Orders :</br>";
                        textareaValue += errorValue;
                    }
                    $('#modalPrintShopee__textarea__' + cust).html(textareaValue);
                }
            },
            error: function (xhr, status, error) {
                console.log(error);
            }
        });

        //function shopeePrint(url) {
        //    //var divToPrint = document.getElementById("iframe_" + countUrl + "_" + cust + "");
        //    var newWin = window.open(url, 'Print-Window');
        //    newWin.document.open();
        //    newWin.document.write('<html><body onload="window.print()">' + divToPrint.innerHTML + '</body></html>');
        //    newWin.document.close();
        //    setTimeout(function () {
        //        newWin.close();
        //    }, 10);
        //}
        
    }
</script>
@{
    var distinctCust = Model.Select(p => new { p.CUST, p.NAMA_CUST }).Distinct().ToList();
}
<div class="row">
    <div class="col-12">
        <ul id="shopee_packing_cust_tabs" class="nav nav-tabs">
            @foreach (var item in distinctCust)
            {
                <li><a data-toggle="tab" href="#shopee_tab_content__@(item.CUST)">@(item.NAMA_CUST)</a></li>
            }
        </ul>
        <div class="tab-content">
            @{
                var PackingListShopeeFound = distinctCust.Count();
            }
            @Html.TextBox("PackingListShopeeFound", PackingListShopeeFound, new { @class = "form-control", disabled = "disabled", @style = "display:none;" })
            @foreach (var item in distinctCust)
            {
                <div id="shopee_tab_content__@(item.CUST)" class="tab-pane fade in shopee_packing_tab_content">
                    <input id="txt_last_page__@(item.CUST)" type="hidden" class="form-control" value="@ViewData["LastPage"]">
                    <div class="modal fade" id="modalPrintShopee__@(item.CUST)" tabindex="-1" role="dialog" aria-labelledby="modalPrintShopee__label__@(item.CUST)">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    @* <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button> *@
                                    <h4 class="modal-title" id="modalPrintShopee__label__@(item.CUST)">Print Pesanan Shopee</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-md-12" id="modalPrintShopee__textarea__@(item.CUST)">
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-danger" onclick="$('#modalPrintShopee__@(item.CUST)').modal('hide');">Tutup</button>
                                    @*<button type="button" class="btn btn-success" onclick="printRekap();">Print</button>*@
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="input-group">
                                <input id="search_table_packing_pesanan_shopee__@(item.CUST)" type="text" class="form-control" placeholder="Pencarian">
                                <span class="input-group-btn">
                                    <button type="button" id="search_table_packing_pesanan_shopee_button__@(item.CUST)" class="btn btn-primary">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            @*<input id="txtShipment__@(item.CUST)" disabled="disabled" required="required" class="form-control" maxlength="30" data_val_length_max="30">
                        <button id="btnShipment__@(item.CUST)" class="btn btn-default" type="button" style="height: 34px;" onclick="PromptDeliveryProvShopee('@(item.CUST)')">
                            <span class="glyphicon glyphicon-option-horizontal"></span>
                        </button>*@
                            <div class="form-group">
                                <div id="shopee-resi-pickup-address-@(item.CUST)">
                                    <select id="select-shopee-resi-pickup-address-@(item.CUST)" class="selectShopeeResiPickupAddress" data-kodecust="@(item.CUST)" placeholder="Pickup Address" required="required"></select>
                                </div>
                            </div>
                            @*<button id="btnPrintShopee__@(item.CUST)" type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalPrintShopee">Print</button>*@
                        </div>
                        <div class="col-md-4" style="text-align:right">
                            <button id="btnProsesPickupShopee__@(item.CUST)" type="button" class="btn btn-primary" onclick="RequestPickupPackingShopee('@(item.CUST)')">Request Pickup</button>
                            <button id="btnProsesDropoffShopee__@(item.CUST)" type="button" class="btn btn-primary" onclick="RequestDropoffPackingShopee('@(item.CUST)')">Drop Off</button>
                            <button id="btnPrintShopee__@(item.CUST)" type="button" class="btn btn-primary" onclick="ShopeeGetLabelPerPacking('@(item.CUST)')">Print</button>
                        </div>
                    </div>
                    <div class="hscroll table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr role="row">
                                    <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 20%;">No. Pesanan</th>
                                    <th class="sorting" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 20%;">No Referensi</th>
                                    <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 35%;">Nama Pemesan</th>
                                    <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 15%;">Kurir</th>
                                    @*<th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Picture: activate to sort column descending" style="width: 10%;">Jumlah</th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var order in Model.Where(p => p.CUST == item.CUST).ToList())
                                {
                                    <tr>
                                        <td>@order.no_bukti</td>
                                        <td>@order.no_referensi</td>
                                        <td>@order.nama_pemesan</td>
                                        <td>@order.kurir</td>
                                        @*<td>@order.jumlah_item</td>*@
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
            
