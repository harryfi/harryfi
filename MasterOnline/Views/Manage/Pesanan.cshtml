        function getShopeePickupTime(address_id,time_value){
            var link = '@Html.Raw(Url.Action("GetShopeePickupTime", "Manage", new { recNum = "replaceCode", address_id = "replaceAddressId" }))';
            link = link.replace("replaceCode", window.$recResiShopee);
            link = link.replace("replaceAddressId", address_id);

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (data) {
                    var listTime = [];
                    var orderExpired = false;
                    $.each(data,
                        function (i, item) {
                            listTime[i] = {
                                id: item.pickup_time_id,
                                text: item.date_string
                            };
                            if (item.pickup_time_id == "-1"){ orderExpired = true;}
                        });

                    var Select = $('#select-shopee-resi-pickup-time').selectize({
                        valueField: 'id',
                        searchField: 'text',
                        options: listTime,
                        onChange: function (value) {

                        }
                    });

                    Select[0].selectize.clearOptions();
                    Select[0].selectize.renderCache = {};
                    Select[0].selectize.load(function (callback) {
                        callback(listTime);
                    });

                    if (orderExpired) {
                        $('#btn-resi-shopee-dropoff').hide();
                        $('#btn-resi-shopee-pickup').hide();
                        $('#btn-resi-shopee-non').hide();
                    }

                    if (time_value != null) {
                        Select[0].selectize.setValue(time_value);
                    }
                },
                error: function (xhr) {
                    console.log(xhr);
                }
            });
        }

