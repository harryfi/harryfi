        function hitungUlangBruto() {
            $total_bruto_barang = 0;
            if ($modeEditSelesai == 1) {
                $('tr[data-pesanan-selesai-id!="0"] .netto-barang').each(function () {
                    $total_bruto_barang += convertToAngka($(this).text());
                });
            } else if ($modeEditDibayar == 1) {
                $('tr[data-pesanan-dibayar-id!="0"] .netto-barang').each(function () {
                    $total_bruto_barang += convertToAngka($(this).text());
                });
            }
            else  {
                if ($modeEditDetail == 1) {
                    $('tr[data-pesanan-id!="0"] .netto-barang').each(function () {
                        if ($(this).attr("data-issaved") == "1") {
                            $total_bruto_barang += convertToAngka($(this).text());
                        }
                    });
                } else if ($modeEditDetail == 0) {
                    $('tr[data-barang-id!="0"] .netto-barang').each(function () {
                        if ($(this).attr("data-issaved") != undefined) {
                            $total_bruto_barang += convertToAngka($(this).text());
                        }
                    });
                }
