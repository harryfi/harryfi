        function getBarangForGudangQty(nobuk) {
            var link = '@Url.Action("RefreshGudangQtyPesanan", "Manage", new {noBuk = "replaceNoBuk"})';
            link = link.replace("replaceNoBuk", nobuk);

            $.ajax({
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function(response) {
                    $('#modal-gudang-qty-body').html(response);

                    $('#modal-gudang-qty-body div[data-jenis="gudang"]').each(function() {
                        var $this = $(this);
                        var $recNum = $this.attr('data-recnum');

                        //add by calvin 27 nov 2018, munculkan QOH di combobox gudang
                        var $brg_sot01b = $this.attr('data-brg');
                        var temp_listQOHPerGD = listQOHPerGD;

                        temp_listQOHPerGD = $.grep(temp_listQOHPerGD, function( n, i ) {
                            return n.BRG===$brg_sot01b;
                        });

                        $listGudang = [];
                        if (temp_listQOHPerGD[0] != undefined) {
                            $.each(temp_listQOHPerGD,
                                function(i, item) {
                                    $listGudang[i] = {
                                        id: item.GD,
                                        text: item.Nama_Gudang + " (Qty Stok : " + item.QOH + ")"
                                    };
                                });
                        }
                        //end add by calvin 27 nov 2018

                        $('#Gudang-Dropdown-' + $recNum).selectivity({
                            allowClear: true,
                            items: $listGudang,
                            placeholder: 'Harap pilih'
                        }).selectivity('value', $('#Gudang-' + $recNum).val()).change(function() {
                            var $val = $(this).selectivity('value');

                            //add by calvin 27 nov 2018, munculkan QOH, dan QSO
                            var getQOH_listQOHPerGD = listQOHPerGD;
                            getQOH_listQOHPerGD = $.grep(getQOH_listQOHPerGD, function( n, i ) {
                                return n.BRG===$brg_sot01b && n.GD === $val;
                            });

                            var getQSO_ListQOOPerBRG= ListQOOPerBRG;
                            getQSO_ListQOOPerBRG = $.grep(getQSO_ListQOOPerBRG, function( n, i ) {
                                return n.BRG===$brg_sot01b && n.GD === $val;
                            });

                            $('#Gudang-' + $recNum).val($val);
                            if (getQOH_listQOHPerGD[0] != undefined) {
                                $.each(getQOH_listQOHPerGD,
                                function(i, item) {
                                    $('#Gudang-stok-' + $recNum).html(item.QOH);
                                });
                            }else {
                                $('#Gudang-stok-' + $recNum).html("");
                            }

                            if (getQSO_ListQOOPerBRG[0] != undefined) {
                                $.each(getQSO_ListQOOPerBRG,
                                function(i, item) {
                                    $('#Gudang-pesanan-' + $recNum).html(item.QSO);
                                });
                            }else {
                                $('#Gudang-pesanan-' + $recNum).html("0");
                            }

                            var stok = convertToAngka($('#Gudang-stok-' + $recNum).html().trim())
                            var pesanan = convertToAngka($('#Gudang-pesanan-' + $recNum).html().trim())
                            $('#Gudang-sisa-' + $recNum).html(stok - pesanan);
                            //end add by calvin 27 nov 2018
                        }).click(function() {
                            var $offsetTop = $this.offset().top;
                            $this.find('.selectivity-dropdown').css('top', $offsetTop + 10);
                        }).change();

                        $('#modal-gudang-qty-body #Qty-' + $recNum).change(function() {
                            var $valMax = $('#modal-gudang-qty-body #Max-Qty-' + $recNum).val();
                            if (Number($(this).val()) > Number($valMax)) {
                                $(this).val($valMax);
                                alert('Qty kirim tidak boleh melebihi qty pesanan!');
                            }
                        });
                    });

                    $('input[data-jenis="gudang_siap_kirim"]').each(function() {
                        $readyToShip = false;
                        if ($(this).val() != null) {
                            if ($(this).val().trim() != '') {
                                $readyToShip = true;
                            }
                        }
                    });

                    console.log($readyToShip);
                },
                error: function(xhr) {
                    console.log(xhr);
                }
            });
        }

