        function modeEditTdBundling() {
            $('#table_tambah_bundling > tbody > tr[data-bundling-id="0"] > .qty-bundling').addClass('editable');
            $('#table_tambah_bundling > tbody > tr[data-bundling-id="0"] > td.editable').off('click').on('click',
                function () {
                    var $this = $(this);
                    var $class = $this.attr('class');
                    var $inputClass = '';
                    var $trId = $this.parent().attr('data-bundling-id');
                    if ($this.text() !== '') {
                        $harga = convertToAngka($this.text());
                    }
                    if ($class == 'qty-bundling editable') {
                        $inputClass = 'qty-input';
                    }

                    var $input = $('<input>', {
                        'class': $inputClass + 'num-only',
                        value: $harga,
                        blur: function () {
                            if (this.value == '') {
                                if ($class === 'qty-bundling editable') {
                                    $this.html(0 + '<input data-val="true" data-val-number="The field QTY must be a number." data-val-required="The QTY field is required." id="PesananBundling_QTY" name="PesananBundling.QTY" type="hidden" value="" maxlength="3">');
                                }
                                refreshNettoBarangBundling($trId);
                            } else {
                                if ($class === 'qty-bundling editable') {
                                    $this.html(this.value + '<input data-val="true" data-val-number="The field QTY must be a number." data-val-required="The QTY field is required." id="PesananBundling_QTY" name="PesananBundling.QTY" type="hidden" value="" maxlength="3">');
                                }
                                refreshNettoBarangBundling($trId);
                            }
                        },
                        focus: function () {
                            if ($harga == 0) {
                                this.value = '';
                            } else {
                                this.value = $harga;
                            }
                        },
                        keyup: function (e) {
                            if (e.which === 13) $input.blur();
                        },
                        keypress: function (e) {
                            if (e.which != 8 && e.which != 0 && e.which < 48 || e.which > 57) e.preventDefault();
                        }
                    }).appendTo($this.empty()).focus();
                });
        }

