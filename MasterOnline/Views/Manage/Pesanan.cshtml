        function saveResiShopee(metode) {
            var ret = true;
            if (metode == 0) { // DROPOFF
                if($("#div-shopee-resi-dropoff-branch").is(":visible")){
                    var value = $('#select-shopee-resi-dropoff-branch').val();
                    if (value == null || value == '') {
                        alert("Cabang harus dipilih.");
                        ret = false;
                    }
                }
                if($("#shopee-resi-dropoff-sender-real-name").is(":visible")){
                    var value = $('#shopee-resi-dropoff-sender-real-name').val();
                    if (value == null || value == '') {
                        alert("Nama Pengirim harus diisi.");
                        ret = false;
                    }
                }
                if($("#shopee-resi-dropoff-tracking-no").is(":visible")){
                    var value = $('#shopee-resi-dropoff-tracking-no').val();
                    if (value == null || value == '') {
                        alert("No. Resi harus diisi.");
                        ret = false;
                    }
