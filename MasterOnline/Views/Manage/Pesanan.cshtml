            $.ajax({
                type: "GET",
                url: '@Url.Action("GetEkspedisi", "Manage")',
                contentType: 'application/json',
                cache: false,
                success: function(data) {
                    var listEkspedisi = [];

                    $.each(data,
                        function(i, item) {
                            listEkspedisi[i] = {
