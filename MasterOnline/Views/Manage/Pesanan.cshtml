                if (replacedBranchValue == undefined) {
                    replacedBranchValue = "";
                }
                var replacepAddressValue = $('#select-shopee-resi-pickup-address').val();
                if (replacepAddressValue == undefined) {
                    replacepAddressValue = "";
                }
                var replacepTimeValue = $('#select-shopee-resi-pickup-time').val();
                if (replacepTimeValue == undefined) {
                    replacepTimeValue = "";
                }
                link = link.replace("replacedBranch", encodeURIComponent(replacedBranchValue));
                link = link.replace("replacedSender", encodeURIComponent($('#shopee-resi-dropoff-sender-real-name').val()));
                link = link.replace("replacedTrackNo", encodeURIComponent($('#shopee-resi-dropoff-tracking-no').val()));

                link = link.replace("replacepAddress", encodeURIComponent(replacepAddressValue));
                link = link.replace("replacepTime", encodeURIComponent(replacepTimeValue));

                link = link.replace("replacenTrackNo", encodeURIComponent($('#shopee-resi-non-integrated-tracking-no').val()));

