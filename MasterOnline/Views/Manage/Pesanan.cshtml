                    if (value == null || value == '') {
                        alert("Waktu Pickup harus dipilih.");
                        ret = false;
                    }
                }
            }
            if (metode == 2) { // NON INTEGRATED
                if($("#shopee-resi-non-integrated-tracking-no").is(":visible")){
                    var value = $('#shopee-resi-non-integrated-tracking-no').val();
                    if (value == null || value == '') {
                        alert("No. Resi harus diisi.");
                        ret = false;
                    }
                }
            }

            if (ret) {
                var link =
                    '@Html.Raw(Url.Action("SaveResiShopee", "Manage", new {recNum = "replaceRecNum", metode = "replaceMetode",
                    dBranch = "replacedBranch", dSender = "replacedSender", dTrackNo = "replacedTrackNo",
                    pAddress = "replacepAddress", pTime = "replacepTime",
                    nTrackNo = "replacenTrackNo"
                }))';

                link = link.replace("replaceRecNum", encodeURIComponent(window.$recResiShopee));
                link = link.replace("replaceMetode", encodeURIComponent(metode));

