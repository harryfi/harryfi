        function DownloadExcelPesanan() {
            $('#loading_spinner').show();
            var orId = "";
            var drTgl = $('#DrTanggal').val();
            var sdTgl = $('#SdTanggal').val();
            $('input[name="ckpesan"]:checked').each(function () {
                orId = this.value;
            });

            var link = '@Html.Raw(Url.Action("ListPesanantoExcel", "TransferExcel"))';
            link += "?orid=" + orId + "&drtgl=" + drTgl + "&sdtgl=" + sdTgl;

            $.ajax({
                xhr: function () {
                    var xhr = $.ajaxSettings.xhr();
                    xhr.onprogress = function (evt) {
                        if (evt.lengthComputable) {
                            $('#loading_spinner').hide();
                        }
                        $('#loading_spinner').hide();
                    };
                    return xhr;
                },
                type: "GET",
                url: link,
                contentType: 'application/json',
                cache: false,
                success: function (response) {
                    $('#loading_spinner').hide();
                    if (response.Errors.length == 0) {
                        var bytes = new Uint8Array(response.byteExcel);
                        saveByteArray(response.namaFile, bytes);
                    } else {
                        alert(response.Errors[0]);
