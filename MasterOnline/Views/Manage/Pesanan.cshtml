        function UbahStatusPesananPacking(varapproved) {
            $('#loading_ubahstatus_packing').show();
            var cekTrue = [];
            get_selected.length = 0;
            $(".checkbox_gntstatus_packing").each(function (z, chk) {
                if ($(chk).prop('checked') == true) {
                    cekTrue.push($(chk).attr("data-rec"));
                }
            });
            if (cekTrue.length == 0) {
                get_selected.push(this.$recPesanan);
            } else {
                $('.get_recnum').each(function (i, item) {
                    get_selected.push($(item).attr("data-recnum"));
                });
            }

            if(packingstatussinglerecnum != 0){
                get_selected.push(packingstatussinglerecnum);
            }

            var pList = $('#ckPackinglist').is(":checked");
            @*var link = '@Url.Action("UbahStatusPesananPackingTransaction", "Manage")';*@
            var link = '@Url.Action("UbahStatusPesananPackingTransactionVersi2", "Manage")';

            $.ajax({
                type: "POST",
                url: link,
                //data: JSON.stringify(get_selected),
                data: JSON.stringify({ get_selected: get_selected, packinglist: pList, approved : varapproved }),
                contentType: 'application/json; charset=UTF-8',
                cache: false,
                beforeSend: function () {
                    $('#loading_ubahstatus_packing').show();
                },
                complete: function (){
                    $('#loading_ubahstatus_packing').hide();
                },
                success: function(response) {
                    $('#modalUbahStatusMultiPacking_body').animate({ scrollTop: 0 }, 'slow');

                    if (response.error_packing_list == false) {
                        if(response.need_approval == 2){
                            var textareaValue = "Update status pesanan dibatalkan.";
                            $('#modalUbahStatusMultiPacking_error').show();
                            $('#modalUbahStatusMultiPacking_error').html(textareaValue);

                            $('#modalUbahStatusMultiPacking_header').hide();
                            $('#modalUbahStatusMultiPacking_btn_tidak').show();
                            $('#modalUbahStatusMultiPacking_btn_ya').hide();
                            $('#modalUbahStatusMultiPacking_btn_approve').hide();
                            $('#modalUbahStatusMultiPacking_btn_notapprove').hide();

                            $('#modalUbahStatusMultiPacking_btn_lanjut').hide();

                            $('#modalUbahStatusMultiPacking_btn_tidak').html('Close');
                        }
                        else{
                            var textareaValue = "";

                            if(response.need_approval == 1){
                                textareaValue += "Jumlah pesanan bisa dilakukan update status : "+ response.successCount +"</br>";
                                textareaValue += "Jumlah pesanan yang tidak bisa diproses : "+ response.listError.length +"</br>";
                                textareaValue += "Tetap lanjutkan pembuatan faktur dan packing list ?</br>";
                            }
                            else{
                                textareaValue += "Jumlah pesanan berhasil dilakukan update status : "+ response.successCount +"</br>";
                            }

                            var adaError = false;
                            var errorValue = "";
                            response.listError.forEach(function (item) {
                                adaError = true;
                                errorValue += item.no_bukti_so + " :</br>" + item.error_msg + "</br>";
                            });
                            if (adaError) {
                                textareaValue += "</br>";
                                if(response.need_approval == 1){
                                    textareaValue += "Pesanan yang tidak bisa diproses :</br>";
                                }
                                else{
                                    textareaValue += "Pesanan gagal update status :</br>";
                                }
                                textareaValue += errorValue;
                            }
                            $('#modalUbahStatusMultiPacking_error').show();
                            $('#modalUbahStatusMultiPacking_error').html(textareaValue);

                            if(response.need_approval == 1){
                                $('#modalUbahStatusMultiPacking_btn_tidak').hide();
                                $('#modalUbahStatusMultiPacking_btn_ya').hide();
                                $('#modalUbahStatusMultiPacking_btn_approve').show();
                                $('#modalUbahStatusMultiPacking_btn_notapprove').show();
                            }
                            else{
                                if(response.successCount > 0){
                                    refreshTableSiapKirim();
                                    refreshTablePesanan();
                                    refreshTableSudahKirim();
                                    refreshTableSudahDibayar();
                                    $('#packing_div').hide();
                                    $('#modalUbahStatusMultiPacking_header').hide();
                                    $('#modalUbahStatusMultiPacking_btn_tidak').hide();
                                    $('#modalUbahStatusMultiPacking_btn_ya').hide();
                                    $('#modalUbahStatusMultiPacking_btn_approve').hide();
                                    $('#modalUbahStatusMultiPacking_btn_notapprove').hide();

                                    $('#modalUbahStatusMultiPacking_btn_lanjut').show();
                                    $('#modalUbahStatusMultiPacking_packingNo').val(response.packingNo);
                                    $('#packingList_NO_BUKTI').val(response.packingNo);
                                    $('#modalPackingPengiriman_packingTitle').html('Nomor Bukti Packing List : ' + response.packingNo);
                                }
                            }
                        }

                    } else {
                        $('#modalUbahStatusMultiPacking_error').show();
                        $('#modalUbahStatusMultiPacking_error').html('Gagal memproses packing. Mohon hubungi support.');
                    }
                    $('#modal-status-multi-packing-body').modal('hide');
                    //$('#modalUbahStatusMultiPacking').modal('hide');
                    $('#konfUbahStatusSiapkirim').modal('hide');
                    get_selected.length = 0;
                },
                error: function(xhr, status, error) {
                    console.log(error);
                    get_selected.length = 0;
                }
            });
        }

